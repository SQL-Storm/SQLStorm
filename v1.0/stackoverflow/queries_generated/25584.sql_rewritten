WITH RankedPosts AS (
    SELECT 
        p.Id AS PostId,
        p.Title,
        p.Body,
        p.CreationDate,
        COUNT(c.Id) AS CommentCount,
        COUNT(DISTINCT ps.UserId) AS UniqueEditors,
        ROW_NUMBER() OVER (PARTITION BY p.Id ORDER BY p.LastEditDate DESC) AS EditRank
    FROM 
        Posts p
    LEFT JOIN 
        Comments c ON p.Id = c.PostId
    LEFT JOIN 
        PostHistory ph ON p.Id = ph.PostId AND ph.PostHistoryTypeId IN (4, 5, 6)
    LEFT JOIN 
        Users ps ON ph.UserId = ps.Id
    GROUP BY 
        p.Id, p.Title, p.Body, p.CreationDate
),
PostStats AS (
    SELECT 
        rp.PostId,
        rp.Title,
        rp.Body,
        rp.CreationDate,
        rp.CommentCount,
        rp.UniqueEditors,
        CASE 
            WHEN rp.CommentCount > 10 THEN 'Highly Discussed'
            WHEN rp.CommentCount BETWEEN 5 AND 10 THEN 'Moderately Discussed'
            ELSE 'Less Discussed'
        END AS DiscussionLevel,
        CASE 
            WHEN rp.UniqueEditors > 5 THEN 'Highly Collaborative'
            ELSE 'Less Collaborative'
        END AS CollaborationLevel
    FROM 
        RankedPosts rp
    WHERE 
        rp.EditRank = 1
),
FinalReport AS (
    SELECT 
        ps.Title,
        ps.CommentCount,
        ps.DiscussionLevel,
        ps.CollaborationLevel,
        EXTRACT(EPOCH FROM (cast('2024-10-01 12:34:56' as timestamp) - ps.CreationDate)) AS AgeInSeconds
    FROM 
        PostStats ps
    WHERE 
        ps.CommentCount > 0
    ORDER BY 
        ps.CommentCount DESC
)
SELECT 
    Title,
    CommentCount,
    DiscussionLevel,
    CollaborationLevel,
    AgeInSeconds
FROM 
    FinalReport
LIMIT 10;