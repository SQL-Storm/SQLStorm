
WITH RankedSales AS (
    SELECT
        ws_item_sk,
        SUM(ws_sales_price) AS total_sales,
        RANK() OVER (PARTITION BY ws_item_sk ORDER BY SUM(ws_sales_price) DESC) AS sales_rank
    FROM
        web_sales
    WHERE
        ws_net_profit IS NOT NULL
    GROUP BY
        ws_item_sk
),
CustomerDemographics AS (
    SELECT
        cd_demo_sk,
        cd_gender,
        cd_marital_status,
        cd_income_band_sk,
        COUNT(*) AS demo_count
    FROM
        customer_demographics
    GROUP BY
        cd_demo_sk, cd_gender, cd_marital_status, cd_income_band_sk
),
HighValueCustomers AS (
    SELECT
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        COALESCE(SUM(ws.ws_net_profit), 0) AS total_net_profit
    FROM
        customer c
    LEFT JOIN
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    GROUP BY
        c.c_customer_sk, c.c_first_name, c.c_last_name
    HAVING
        COALESCE(SUM(ws.ws_net_profit), 0) > (SELECT AVG(ws_net_profit) FROM web_sales WHERE ws_net_profit IS NOT NULL)
),
ReturnData AS (
    SELECT
        wr_returning_customer_sk,
        SUM(wr_return_amt) AS total_return_amt,
        COUNT(*) AS total_returns
    FROM
        web_returns
    GROUP BY
        wr_returning_customer_sk
),
FinalReport AS (
    SELECT
        hvc.c_customer_sk,
        hvc.c_first_name,
        hvc.c_last_name,
        hvc.total_net_profit,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.demo_count,
        COALESCE(rd.total_return_amt, 0) AS total_return_amt,
        COALESCE(rd.total_returns, 0) AS total_returns,
        CASE 
            WHEN hvc.total_net_profit > 1000 THEN 'Gold'
            WHEN hvc.total_net_profit BETWEEN 500 AND 999 THEN 'Silver'
            ELSE 'Bronze'
        END AS customer_tier
    FROM
        HighValueCustomers hvc
    LEFT JOIN
        CustomerDemographics cd ON hvc.c_customer_sk = cd.cd_demo_sk
    LEFT JOIN
        ReturnData rd ON hvc.c_customer_sk = rd.wr_returning_customer_sk
)
SELECT
    f.c_customer_sk,
    f.c_first_name,
    f.c_last_name,
    f.total_net_profit,
    f.cd_gender,
    f.cd_marital_status,
    f.demo_count,
    f.total_return_amt,
    f.total_returns,
    f.customer_tier,
    'Report generated on ' || CAST('2002-10-01' AS VARCHAR) || ' at ' || CAST('12:34:56' AS VARCHAR) AS report_timestamp
FROM
    FinalReport f
ORDER BY
    f.total_net_profit DESC, f.c_last_name ASC
LIMIT 100;
