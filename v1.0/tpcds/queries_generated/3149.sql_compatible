
WITH Sales_Details AS (
    SELECT 
        ws.web_site_id, 
        ws.ws_sold_date_sk,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_net_paid) AS total_sales,
        RANK() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws.ws_net_paid) DESC) AS rank_sales
    FROM web_sales ws
    JOIN customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    WHERE c.c_birth_year < 2000 
    GROUP BY ws.web_site_id, ws.ws_sold_date_sk
),
Returns_Details AS (
    SELECT 
        wr.wr_web_page_sk,
        COUNT(wr.wr_order_number) AS total_returns,
        SUM(wr.wr_return_amt) AS total_returned_amount
    FROM web_returns wr
    GROUP BY wr.wr_web_page_sk
),
Aggregated_Data AS (
    SELECT 
        sd.web_site_id,
        sd.total_quantity,
        sd.total_sales,
        COALESCE(rd.total_returns, 0) AS total_returns,
        COALESCE(rd.total_returned_amount, 0) AS total_returned_amount,
        (sd.total_sales - COALESCE(rd.total_returned_amount, 0)) AS net_sales
    FROM Sales_Details sd
    LEFT JOIN Returns_Details rd ON sd.web_site_id = rd.wr_web_page_sk
)
SELECT 
    ad.web_site_id,
    ad.total_quantity,
    ad.total_sales,
    ad.total_returns,
    ad.total_returned_amount,
    ad.net_sales,
    CASE 
        WHEN ad.net_sales > 0 THEN 'Profitable'
        ELSE 'Non-Profitable'
    END AS sales_status
FROM Aggregated_Data ad
WHERE ad.total_sales > 1000 
ORDER BY ad.net_sales DESC
FETCH FIRST 10 ROWS ONLY;
