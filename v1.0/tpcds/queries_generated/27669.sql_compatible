
WITH processed_addresses AS (
    SELECT 
        ca_address_sk,
        UPPER(CONCAT(ca_street_number, ' ', ca_street_name, ' ', ca_street_type)) AS full_address,
        ca_city,
        ca_state,
        ca_zip
    FROM 
        customer_address
),
customer_summary AS (
    SELECT 
        c.c_customer_sk,
        CONCAT(c.c_first_name, ' ', c.c_last_name) AS full_name,
        cd.cd_gender,
        cd.cd_marital_status,
        COUNT(DISTINCT c.c_current_addr_sk) AS address_count,
        COALESCE(cd.cd_purchase_estimate, 0) AS purchase_estimate
    FROM 
        customer AS c
    JOIN customer_demographics AS cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    GROUP BY 
        c.c_customer_sk, c.c_first_name, c.c_last_name, cd.cd_gender, cd.cd_marital_status
),
address_counts AS (
    SELECT 
        pa.ca_city,
        pa.ca_state,
        COUNT(pa.ca_address_sk) AS total_addresses
    FROM 
        processed_addresses AS pa
    GROUP BY 
        pa.ca_city, pa.ca_state
)
SELECT 
    cs.full_name,
    cs.cd_gender,
    cs.cd_marital_status,
    ac.total_addresses,
    cs.purchase_estimate
FROM 
    customer_summary AS cs
LEFT JOIN 
    address_counts AS ac ON cs.address_count = ac.total_addresses
WHERE 
    ac.total_addresses IS NOT NULL AND ac.total_addresses > 5
ORDER BY 
    cs.full_name;
