
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.web_site_id,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_ext_sales_price) DESC) AS rank_sales
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023
    GROUP BY 
        ws.web_site_sk, ws.web_site_id
),
MaxRank AS (
    SELECT 
        web_site_sk, 
        MAX(rank_sales) AS max_rank
    FROM 
        RankedSales
    GROUP BY 
        web_site_sk
),
CustomerReturns AS (
    SELECT 
        sr.sr_item_sk,
        COUNT(sr.sr_ticket_number) AS return_count,
        SUM(sr.sr_return_amt) AS total_return_amt
    FROM 
        store_returns sr
    GROUP BY 
        sr.sr_item_sk
),
SalesWithReturns AS (
    SELECT 
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS sold_quantity,
        COALESCE(cr.return_count, 0) AS return_count,
        COALESCE(cr.total_return_amt, 0) AS total_return_amt
    FROM 
        web_sales ws
    LEFT JOIN 
        CustomerReturns cr ON ws.ws_item_sk = cr.sr_item_sk
    GROUP BY 
        ws.ws_item_sk
)
SELECT 
    rs.web_site_id,
    COUNT(DISTINCT ss.ss_ticket_number) AS total_sales_count,
    SUM(ss.ss_ext_sales_price) AS total_sales,
    SUM(COALESCE(cr.return_count, 0)) AS total_returns,
    SUM(COALESCE(cr.total_return_amt, 0)) AS total_return_revenue,
    AVG(ss.ss_sales_price) AS avg_sales_price,
    MAX(ss.ss_sales_price) AS max_sales_price,
    MIN(ss.ss_sales_price) AS min_sales_price
FROM 
    store_sales ss
JOIN 
    RankedSales rs ON ss.ss_item_sk = rs.web_site_sk
LEFT JOIN 
    CustomerReturns cr ON ss.ss_item_sk = cr.sr_item_sk
WHERE 
    ss.ss_sold_date_sk = (
        SELECT 
            MAX(d_date_sk)
        FROM 
            date_dim
        WHERE 
            d_year = 2023
    )
AND 
    (ss.ss_net_paid > 100 OR cr.total_return_amt IS NULL)
GROUP BY 
    rs.web_site_id
HAVING 
    SUM(ss.ss_ext_sales_price) > 5000
ORDER BY 
    total_sales DESC;
