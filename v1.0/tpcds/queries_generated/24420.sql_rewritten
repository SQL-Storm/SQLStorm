WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        SUM(ws.ws_sales_price) AS total_sales,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_sales_price) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2001
    GROUP BY 
        ws.web_site_sk
),
CustomerDetails AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_purchase_estimate,
        COALESCE(cd.cd_dep_count, 0) AS dep_count,
        CASE 
            WHEN cd.cd_credit_rating = 'M' THEN 'Medium'
            WHEN cd.cd_credit_rating = 'H' THEN 'High'
            ELSE 'Low' 
        END AS credit_rating_category
    FROM 
        customer c
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
SaleReturns AS (
    SELECT 
        sr.returning_customer_sk,
        SUM(sr.return_amount) AS total_returned,
        COUNT(sr.return_quantity) AS return_count
    FROM 
        store_returns sr
    WHERE 
        sr.return_quantity > 0
    GROUP BY 
        sr.returning_customer_sk
),
FinalReport AS (
    SELECT 
        cd.c_customer_sk,
        cd.c_first_name,
        cd.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        COALESCE(SUM(ws.ws_sales_price), 0) AS total_spent,
        COALESCE(tc.total_returned, 0) AS total_returned,
        (COALESCE(SUM(ws.ws_sales_price), 0) - COALESCE(tc.total_returned, 0)) AS net_spent,
        CASE 
            WHEN COALESCE(SUM(ws.ws_sales_price), 0) - COALESCE(tc.total_returned, 0) < 0 THEN 'Net Loss'
            WHEN COALESCE(SUM(ws.ws_sales_price), 0) = 0 THEN 'No Purchases'
            ELSE 'Profitable' 
        END AS profitability_status
    FROM 
        CustomerDetails cd
    LEFT JOIN 
        web_sales ws ON cd.c_customer_sk = ws.ws_bill_customer_sk
    LEFT JOIN 
        SaleReturns tc ON cd.c_customer_sk = tc.returning_customer_sk
    GROUP BY 
        cd.c_customer_sk, cd.c_first_name, cd.c_last_name, cd.cd_gender, cd.cd_marital_status, tc.total_returned
)
SELECT 
    fr.*,
    rs.web_site_sk,
    rs.total_sales,
    rs.sales_rank
FROM 
    FinalReport fr
JOIN 
    RankedSales rs ON fr.c_customer_sk IN (
        SELECT 
            ws_ship_customer_sk 
        FROM 
            web_sales 
        WHERE 
            ws_sales_price > 100 
    )
ORDER BY 
    fr.net_spent DESC,
    rs.sales_rank
LIMIT 10;