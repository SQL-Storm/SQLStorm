
WITH SalesData AS (
    SELECT 
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_sales_price) AS total_sales_price,
        SUM(ws.ws_ext_discount_amt) AS total_discount,
        w.w_warehouse_name,
        c.c_country AS customer_country,
        d.d_year,
        d.d_month_seq
    FROM 
        web_sales ws
    JOIN 
        warehouse w ON ws.ws_warehouse_sk = w.w_warehouse_sk
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year BETWEEN 2021 AND 2022
    GROUP BY 
        ws.ws_item_sk, w.w_warehouse_name, c.c_country, d.d_year, d.d_month_seq
),
RankingData AS (
    SELECT 
        ws_item_sk,
        total_quantity,
        total_sales_price,
        total_discount,
        w_warehouse_name AS warehouse_name,
        customer_country,
        d_year,
        d_month_seq,
        RANK() OVER (PARTITION BY d_year, d_month_seq ORDER BY total_sales_price DESC) AS sales_rank
    FROM 
        SalesData
)
SELECT 
    rd.ws_item_sk,
    rd.warehouse_name,
    rd.customer_country,
    rd.total_quantity,
    rd.total_sales_price,
    rd.total_discount,
    rd.d_year,
    rd.d_month_seq
FROM 
    RankingData rd
WHERE 
    rd.sales_rank <= 5
ORDER BY 
    rd.d_year, rd.d_month_seq, rd.sales_rank;
