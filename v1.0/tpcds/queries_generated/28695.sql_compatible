
WITH AddressPatterns AS (
    SELECT 
        ca_address_id,
        CONCAT(TRIM(ca_street_number), ' ', TRIM(ca_street_name), ' ', TRIM(ca_street_type), 
               CASE WHEN ca_suite_number IS NOT NULL AND ca_suite_number <> '' THEN CONCAT(' Suite ', TRIM(ca_suite_number)) ELSE '' END) AS FullAddress,
        ca_city,
        ca_state,
        ca_zip,
        ca_country
    FROM 
        customer_address
),
CustomerAttributes AS (
    SELECT 
        c.c_customer_id,
        CONCAT(TRIM(c.c_first_name), ' ', TRIM(c.c_last_name)) AS FullName,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_education_status,
        cd.cd_purchase_estimate
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
DateDetails AS (
    SELECT 
        d.d_date_id,
        d.d_year,
        d.d_month_seq,
        d.d_week_seq,
        d.d_day_name
    FROM 
        date_dim d
)
SELECT 
    ca.FullAddress,
    ca.ca_address_id,
    addr.ca_city,
    addr.ca_state,
    addr.ca_zip,
    addr.ca_country,
    c.FullName,
    c.cd_gender,
    c.cd_marital_status,
    c.cd_education_status,
    c.cd_purchase_estimate,
    d.d_date_id,
    d.d_year,
    d.d_month_seq,
    d.d_week_seq,
    d.d_day_name
FROM 
    AddressPatterns ca
JOIN 
    customer_address addr ON ca.ca_address_id = addr.ca_address_id
JOIN 
    CustomerAttributes c ON addr.ca_address_sk = c.c_current_addr_sk
JOIN 
    DateDetails d ON d.d_date_id = '2001-10-01' 
WHERE 
    ca.FullAddress LIKE '%Main St%'
    AND c.cd_gender = 'F'
    AND d.d_year = 2001
ORDER BY 
    addr.ca_city, 
    c.FullName;
