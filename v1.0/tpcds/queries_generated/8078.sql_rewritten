WITH sales_summary AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws.ws_order_number) AS order_count
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    JOIN 
        web_site w ON ws.ws_web_site_sk = w.web_site_sk
    WHERE 
        dd.d_year = 2001 
        AND dd.d_moy IN (11, 12)  
    GROUP BY 
        ws.web_site_id
), 
top_sites AS (
    SELECT 
        web_site_id, 
        total_quantity,
        total_profit,
        order_count,
        RANK() OVER (ORDER BY total_profit DESC) AS profit_rank
    FROM 
        sales_summary
)
SELECT 
    ts.web_site_id,
    ts.total_quantity,
    ts.total_profit,
    ts.order_count,
    w.web_name,
    w.web_country
FROM 
    top_sites ts
JOIN 
    web_site w ON ts.web_site_id = w.web_site_id
WHERE 
    ts.profit_rank <= 10
ORDER BY 
    ts.total_profit DESC;