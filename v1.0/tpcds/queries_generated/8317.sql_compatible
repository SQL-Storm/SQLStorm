
WITH SalesData AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_sales_price) AS total_sales,
        SUM(ws.ws_net_profit) AS total_profit,
        d.d_year,
        d.d_quarter_seq,
        c.c_birth_year,
        cd.cd_marital_status,
        cd.cd_gender,
        ca.ca_state
    FROM 
        web_sales ws
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
    WHERE 
        d.d_year = 2023
    GROUP BY 
        ws.web_site_id, d.d_year, d.d_quarter_seq, c.c_birth_year, cd.cd_marital_status, cd.cd_gender, ca.ca_state
),
RankingData AS (
    SELECT 
        web_site_id,
        total_quantity,
        total_sales,
        total_profit,
        ROW_NUMBER() OVER (PARTITION BY d_year, d_quarter_seq ORDER BY total_profit DESC) AS rank,
        d_year,
        d_quarter_seq
    FROM 
        SalesData
)
SELECT 
    web_site_id,
    total_quantity,
    total_sales,
    total_profit,
    rank
FROM 
    RankingData
WHERE 
    rank <= 10
ORDER BY 
    d_year, d_quarter_seq, total_profit DESC;
