WITH CustomerSales AS (
    SELECT 
        c.c_customer_sk,
        SUM(ws.ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders
    FROM 
        customer c
    JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_ship_customer_sk
    WHERE 
        ws.ws_ship_date_sk BETWEEN 2458335 AND 2458595 
    GROUP BY 
        c.c_customer_sk
),
HighValueCustomers AS (
    SELECT 
        cs.c_customer_sk,
        cs.total_profit,
        cs.total_orders
    FROM 
        CustomerSales cs
    WHERE 
        cs.total_profit > 1000 AND cs.total_orders > 5
),
Demographics AS (
    SELECT 
        cd.cd_demo_sk,
        cd.cd_gender,
        cd.cd_marital_status,
        COUNT(DISTINCT c.c_customer_sk) AS customer_count
    FROM 
        customer_demographics cd
    JOIN 
        customer c ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        HighValueCustomers hvc ON hvc.c_customer_sk = c.c_customer_sk
    GROUP BY 
        cd.cd_demo_sk, cd.cd_gender, cd.cd_marital_status
)
SELECT 
    d.cd_gender,
    d.cd_marital_status,
    d.customer_count,
    (SELECT COUNT(DISTINCT c.c_customer_sk) FROM customer c) AS total_customers,
    ROUND((d.customer_count::decimal / (SELECT COUNT(DISTINCT c.c_customer_sk) FROM customer c)) * 100, 2) AS customer_percentage
FROM 
    Demographics d
ORDER BY 
    customer_count DESC;