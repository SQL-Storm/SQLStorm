
WITH CustomerAddressData AS (
    SELECT
        ca_address_sk,
        CONCAT(ca_street_number, ' ', ca_street_name, ' ', ca_street_type) AS full_address,
        ca_city,
        ca_state,
        ca_zip
    FROM customer_address
),
CustomerDemographicsData AS (
    SELECT
        cd_demo_sk,
        cd_gender,
        cd_marital_status,
        cd_education_status,
        cd_purchase_estimate,
        cd_credit_rating
    FROM customer_demographics
),
SalesData AS (
    SELECT
        ws.bill_customer_sk,
        SUM(ws.ws_sales_price * ws.ws_quantity) AS total_sales,
        COUNT(ws.ws_order_number) AS order_count
    FROM web_sales ws
    GROUP BY ws.bill_customer_sk
),
RankedSales AS (
    SELECT
        sd.bill_customer_sk,
        sd.total_sales,
        sd.order_count,
        RANK() OVER (ORDER BY sd.total_sales DESC) AS sales_rank
    FROM SalesData sd
)
SELECT
    ca.ca_address_sk,
    ca.full_address,
    cd.cd_gender,
    cd.cd_marital_status,
    rd.total_sales,
    rd.order_count,
    rd.sales_rank
FROM CustomerAddressData ca
JOIN CustomerDemographicsData cd ON cd.cd_demo_sk = (
    SELECT c.c_current_cdemo_sk
    FROM customer c
    WHERE c.c_customer_sk = ca.ca_address_sk
)
JOIN RankedSales rd ON rd.bill_customer_sk = ca.ca_address_sk
WHERE ca.ca_state = 'NY' AND rd.sales_rank <= 10
ORDER BY rd.total_sales DESC;
