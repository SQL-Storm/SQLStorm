
WITH SalesData AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_sold_date_sk,
        SUM(ws.ws_sales_price) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        SUM(ws.ws_net_profit) AS total_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_sales_price) DESC) AS sales_rank
    FROM web_sales AS ws
    JOIN date_dim AS d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE d.d_year = 2023
    GROUP BY ws.web_site_sk, ws.ws_sold_date_sk
),
ReturnData AS (
    SELECT 
        wr.wr_web_page_sk,
        SUM(wr.wr_return_amt) AS total_returns,
        COUNT(DISTINCT wr.wr_order_number) AS return_count
    FROM web_returns AS wr
    GROUP BY wr.wr_web_page_sk
),
CombinedSales AS (
    SELECT 
        s.web_site_sk,
        sd.total_sales,
        rd.total_returns,
        sd.order_count,
        sd.total_profit,
        COALESCE(rd.total_returns, 0) AS adjusted_returns,
        sd.total_sales - COALESCE(rd.total_returns, 0) AS net_sales
    FROM (
        SELECT web_site_sk, SUM(total_sales) AS total_sales 
        FROM SalesData 
        GROUP BY web_site_sk
    ) AS s
    LEFT JOIN SalesData AS sd ON s.web_site_sk = sd.web_site_sk
    LEFT JOIN ReturnData AS rd ON sd.web_site_sk = rd.wr_web_page_sk
)
SELECT 
    c.c_customer_id,
    ca.ca_city,
    cs.total_sales,
    cs.total_profit,
    cs.order_count,
    RANK() OVER (ORDER BY cs.net_sales DESC) AS sales_rank
FROM customer AS c
JOIN customer_address AS ca ON c.c_current_addr_sk = ca.ca_address_sk
JOIN CombinedSales AS cs ON cs.web_site_sk = c.c_customer_sk
WHERE c.c_birth_year > 1980 AND cs.order_count > 5
ORDER BY sales_rank
LIMIT 10;
