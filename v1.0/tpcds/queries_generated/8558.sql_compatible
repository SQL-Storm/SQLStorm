
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws_sold_date_sk,
        SUM(ws_sales_price) AS total_sales,
        RANK() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws_sales_price) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        web_site w ON ws.ws_web_site_sk = w.web_site_sk
    WHERE 
        ws.ws_sold_date_sk BETWEEN 20230101 AND 20231231
    GROUP BY 
        ws.web_site_sk, ws_sold_date_sk
),
TopSites AS (
    SELECT 
        web_site_sk,
        RANK() OVER (ORDER BY total_sales DESC) AS site_rank
    FROM 
        RankedSales
    WHERE 
        sales_rank = 1
)
SELECT 
    w.web_name,
    SUM(ss.ss_quantity) AS total_items_sold,
    SUM(ss.ss_sales_price) AS total_revenue,
    AVG(ss.ss_sales_price) AS avg_item_price,
    COUNT(DISTINCT ss.ss_customer_sk) AS unique_customers
FROM 
    store_sales ss
JOIN 
    store s ON ss.ss_store_sk = s.s_store_sk
JOIN 
    warehouse w ON s.s_company_id = w.w_warehouse_sk
WHERE 
    s.s_state = 'CA'
    AND s.s_store_id IN (SELECT web_site_sk FROM TopSites WHERE site_rank <= 5)
GROUP BY 
    w.web_name
ORDER BY 
    total_revenue DESC;
