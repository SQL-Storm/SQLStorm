
WITH SalesSummary AS (
    SELECT
        ws.ws_sold_date_sk,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        SUM(ws.ws_quantity) AS total_quantity,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders
    FROM
        web_sales ws
    JOIN
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    JOIN
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE
        d.d_year = 2023 AND
        cd.cd_gender = 'M' AND
        cd.cd_marital_status = 'S'
    GROUP BY
        ws.ws_sold_date_sk
),
WarehouseSales AS (
    SELECT
        w.w_warehouse_id,
        SUM(ss.ss_ext_sales_price) AS warehouse_sales
    FROM
        warehouse w
    JOIN
        store s ON w.w_warehouse_sk = s.s_store_sk
    JOIN
        store_sales ss ON s.s_store_sk = ss.ss_store_sk
    WHERE
        ss.ss_sold_date_sk IN (SELECT ws_sold_date_sk FROM SalesSummary)
    GROUP BY
        w.w_warehouse_id
)
SELECT
    ss.ws_sold_date_sk,
    ss.total_sales,
    ss.total_quantity,
    ws.warehouse_sales,
    (CASE WHEN ws.warehouse_sales > 0 THEN (ss.total_sales / NULLIF(ws.warehouse_sales, 0)) ELSE NULL END) AS sales_to_warehouse_ratio
FROM
    SalesSummary ss
LEFT JOIN
    WarehouseSales ws ON ss.ws_sold_date_sk = ws.w_warehouse_id
ORDER BY
    ss.ws_sold_date_sk;
