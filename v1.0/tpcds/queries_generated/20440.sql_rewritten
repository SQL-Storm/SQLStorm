WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.web_sales_price,
        RANK() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.web_sales_price DESC) AS price_rank
    FROM web_sales ws
    WHERE ws.ws_sold_date_sk BETWEEN 2458435 AND 2458535 
),
FilteredCustomers AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        RANK() OVER (PARTITION BY cd.cd_gender ORDER BY cd.cd_purchase_estimate DESC) AS purchase_rank
    FROM customer c
    JOIN customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE cd.cd_purchase_estimate IS NOT NULL
      AND cd.cd_marital_status = 'M'
),
TopRankedSales AS (
    SELECT 
        rs.web_site_sk,
        rs.web_sales_price
    FROM RankedSales rs
    WHERE rs.price_rank <= 5
),
SalesSummary AS (
    SELECT
        f.c_customer_sk,
        f.c_first_name,
        f.c_last_name,
        t.web_site_sk,
        COUNT(t.web_sales_price) AS total_sales,
        SUM(t.web_sales_price) AS total_revenue
    FROM FilteredCustomers f
    LEFT JOIN TopRankedSales t ON f.purchase_rank = (SELECT MAX(rank) FROM FilteredCustomers WHERE c_customer_sk = f.c_customer_sk)
    GROUP BY f.c_customer_sk, f.c_first_name, f.c_last_name, t.web_site_sk
)
SELECT 
    s.c_customer_sk,
    s.c_first_name,
    s.c_last_name,
    s.total_sales,
    s.total_revenue,
    CASE 
        WHEN s.total_revenue IS NULL THEN 'No Sales' 
        WHEN s.total_revenue = 0 THEN 'Zero Sales' 
        ELSE 'Sales Present' 
    END AS sales_status,
    CASE 
        WHEN EXISTS (SELECT 1 FROM store s WHERE s.s_store_sk = s.c_customer_sk) THEN 'Store Affiliated'
        ELSE 'No Store Affiliation'
    END AS store_affiliation
FROM SalesSummary s
WHERE s.total_sales > 0
ORDER BY s.total_revenue DESC
LIMIT 10;