
WITH CustomerInfo AS (
    SELECT 
        CONCAT(c.c_first_name, ' ', c.c_last_name) AS full_name,
        ca.ca_city,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_income_band_sk,
        DENSE_RANK() OVER (PARTITION BY ca.ca_city ORDER BY c.c_birth_year DESC) AS city_rank
    FROM 
        customer c
    JOIN 
        customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_marital_status = 'M'
        AND cd.cd_gender = 'F'
),
IncomeBands AS (
    SELECT 
        ib.ib_income_band_sk,
        CONCAT(CAST(ib.ib_lower_bound AS VARCHAR(255)), ' - ', CAST(ib.ib_upper_bound AS VARCHAR(255))) AS income_range
    FROM 
        income_band ib
)
SELECT 
    ci.full_name,
    ci.ca_city,
    ib.income_range,
    ci.city_rank
FROM 
    CustomerInfo ci
JOIN 
    IncomeBands ib ON ci.cd_income_band_sk = ib.ib_income_band_sk
WHERE 
    ci.city_rank <= 10
GROUP BY 
    ci.full_name, ci.ca_city, ib.income_range, ci.city_rank
ORDER BY 
    ci.ca_city, ci.city_rank;
