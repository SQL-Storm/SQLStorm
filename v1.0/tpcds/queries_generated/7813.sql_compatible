
WITH sales_data AS (
    SELECT 
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_net_profit) AS total_net_profit,
        MAX(ws.ws_sold_date_sk) AS last_sale_date,
        MIN(ws.ws_sold_date_sk) AS first_sale_date
    FROM 
        web_sales ws
    JOIN 
        item i ON ws.ws_item_sk = i.i_item_sk
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        store s ON ws.ws_ship_addr_sk = s.s_store_sk
    WHERE 
        cd.cd_marital_status = 'M' 
        AND cd.cd_gender = 'F'
        AND s.s_state = 'CA'
    GROUP BY 
        ws.ws_item_sk
),
sales_summary AS (
    SELECT 
        ws_item_sk,
        total_quantity,
        total_net_profit,
        last_sale_date,
        first_sale_date,
        DATEDIFF(DAY, first_sale_date, last_sale_date) AS sale_duration
    FROM 
        sales_data
)
SELECT 
    ss.ws_item_sk,
    i.i_item_desc,
    ss.total_quantity,
    ss.total_net_profit,
    ss.sale_duration
FROM 
    sales_summary ss
JOIN 
    item i ON ss.ws_item_sk = i.i_item_sk
WHERE 
    ss.sale_duration > 30
ORDER BY 
    ss.total_net_profit DESC
FETCH FIRST 10 ROWS ONLY;
