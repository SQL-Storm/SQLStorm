
WITH RankedCustomers AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        ROW_NUMBER() OVER (PARTITION BY cd.cd_gender ORDER BY c.c_birth_year DESC) AS GenderRank
    FROM 
        customer AS c
    JOIN 
        customer_demographics AS cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        c.c_birth_year BETWEEN 1970 AND 2000
),
CustomerAddresses AS (
    SELECT 
        c.c_customer_sk,
        CONCAT(ca.ca_street_number, ' ', ca.ca_street_name, ' ', ca.ca_street_type, ', ', ca.ca_city, ', ', ca.ca_state, ' ', ca.ca_zip) AS FullAddress
    FROM 
        customer AS c
    JOIN 
        customer_address AS ca ON c.c_current_addr_sk = ca.ca_address_sk
),
GenderDistribution AS (
    SELECT 
        cd.cd_gender,
        COUNT(*) AS GenderCount,
        AVG(cd.cd_dep_count) AS AvgDependents
    FROM 
        customer_demographics AS cd
    GROUP BY 
        cd.cd_gender
)
SELECT 
    rc.c_first_name,
    rc.c_last_name,
    ca.FullAddress,
    rg.GenderCount,
    rg.AvgDependents
FROM 
    RankedCustomers AS rc
JOIN 
    CustomerAddresses AS ca ON rc.c_customer_sk = ca.c_customer_sk
JOIN 
    GenderDistribution AS rg ON rg.cd_gender = rc.cd_gender
WHERE 
    rc.GenderRank <= 5
ORDER BY 
    rc.cd_gender, rc.c_birth_year DESC;
