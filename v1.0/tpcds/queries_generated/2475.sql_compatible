
WITH ranked_sales AS (
    SELECT 
        ws_bill_customer_sk,
        SUM(ws_ext_sales_price) AS total_sales,
        DENSE_RANK() OVER (PARTITION BY ws_bill_customer_sk ORDER BY SUM(ws_ext_sales_price) DESC) AS sales_rank
    FROM 
        web_sales
    GROUP BY 
        ws_bill_customer_sk
),
customer_summary AS (
    SELECT 
        c.c_customer_sk,
        ca.ca_city,
        ca.ca_state,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_purchase_estimate,
        COALESCE(SUM(ss.ext_sales_price), 0) AS total_store_sales,
        COALESCE(SUM(ws.ws_ext_sales_price), 0) AS total_web_sales
    FROM 
        customer c
    LEFT JOIN 
        customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    LEFT JOIN 
        store_sales ss ON c.c_customer_sk = ss.ss_customer_sk
    LEFT JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    GROUP BY 
        c.c_customer_sk, ca.ca_city, ca.ca_state, cd.cd_gender, cd.cd_marital_status, cd.cd_purchase_estimate
),
customer_income AS (
    SELECT 
        h.hd_demo_sk,
        ib.ib_lower_bound,
        ib.ib_upper_bound,
        COUNT(CASE WHEN cs.cs_order_number IS NOT NULL THEN 1 END) AS total_orders
    FROM 
        household_demographics h
    LEFT JOIN 
        income_band ib ON h.hd_income_band_sk = ib.ib_income_band_sk
    LEFT JOIN 
        catalog_sales cs ON h.hd_demo_sk = cs.cs_bill_cdemo_sk
    GROUP BY 
        h.hd_demo_sk, ib.ib_lower_bound, ib.ib_upper_bound
)

SELECT 
    cs.c_customer_sk,
    cs.ca_city,
    cs.ca_state,
    cs.cd_gender,
    cs.cd_marital_status,
    cs.cd_purchase_estimate,
    cs.total_store_sales,
    cs.total_web_sales,
    ci.ib_lower_bound,
    ci.ib_upper_bound,
    ci.total_orders,
    RANK() OVER (ORDER BY cs.total_web_sales + cs.total_store_sales DESC) AS overall_rank,
    COUNT(ws.ws_item_sk) AS number_of_items_sold,
    MAX(ws.ws_net_paid) AS max_net_paid
FROM 
    customer_summary cs
LEFT JOIN 
    customer_income ci ON cs.c_customer_sk = ci.hd_demo_sk
LEFT JOIN 
    web_sales ws ON cs.c_customer_sk = ws.ws_bill_customer_sk
WHERE 
    cs.total_web_sales > 1000
AND 
    cs.cd_gender = 'M'
OR 
    (ci.ib_lower_bound > 50000 AND ci.ib_upper_bound < 100000)
GROUP BY 
    cs.c_customer_sk, cs.ca_city, cs.ca_state, cs.cd_gender, cs.cd_marital_status, cs.cd_purchase_estimate, ci.ib_lower_bound, ci.ib_upper_bound
ORDER BY 
    overall_rank
LIMIT 100;
