
WITH ranked_sales AS (
    SELECT 
        ws.web_site_sk,
        SUM(ws.ws_net_paid) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_net_paid) DESC) AS rank_sales
    FROM 
        web_sales ws
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_gender = 'F' 
        AND cd.cd_marital_status = 'M' 
        AND ws.ws_sold_date_sk BETWEEN 2459963 AND 2459999
    GROUP BY 
        ws.web_site_sk
), top_sales AS (
    SELECT 
        rs.web_site_sk, 
        rs.total_sales, 
        rs.total_orders
    FROM 
        ranked_sales rs
    WHERE 
        rs.rank_sales <= 10
), active_websites AS (
    SELECT 
        ws.web_site_id, 
        ws.web_name, 
        ws.web_manager 
    FROM 
        web_site ws
    WHERE 
        ws.web_rec_start_date <= DATE '2002-10-01' 
        AND (ws.web_rec_end_date IS NULL OR ws.web_rec_end_date > DATE '2002-10-01')
)
SELECT 
    aw.web_site_id,
    aw.web_name,
    aw.web_manager,
    ts.total_sales,
    ts.total_orders
FROM 
    top_sales ts
JOIN 
    active_websites aw ON ts.web_site_sk = aw.web_site_sk
ORDER BY 
    ts.total_sales DESC;
