WITH sales_data AS (
    SELECT 
        ws.ws_item_sk,
        ws.ws_order_number,
        ws.ws_quantity,
        ws.ws_sales_price,
        ws.ws_ext_sales_price,
        ws.ws_ext_tax,
        ws.ws_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.ws_item_sk ORDER BY ws.ws_order_number DESC) AS rn
    FROM 
        web_sales ws
    WHERE
        ws.ws_sales_price IS NOT NULL
        AND ws.ws_ship_date_sk IS NOT NULL
        AND ws.ws_quantity > 0
),
customer_info AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_credit_rating,
        ca.ca_city,
        ca.ca_state
    FROM customer c
    LEFT JOIN customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    LEFT JOIN customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
),
monthly_returns AS (
    SELECT 
        DATE_TRUNC('month', d.d_date) AS return_month,
        COUNT(cr.cr_return_quantity) AS total_returns,
        SUM(cr.cr_return_amt) AS total_return_amt
    FROM 
        catalog_returns cr
    JOIN date_dim d ON cr.cr_returned_date_sk = d.d_date_sk
    GROUP BY 
        DATE_TRUNC('month', d.d_date)
),
top_customers AS (
    SELECT 
        ci.c_customer_sk,
        ci.c_first_name,
        ci.c_last_name,
        SUM(sd.ws_sales_price * sd.ws_quantity) AS total_spent,
        RANK() OVER (ORDER BY SUM(sd.ws_sales_price * sd.ws_quantity) DESC) AS rank
    FROM 
        sales_data sd
    JOIN customer_info ci ON sd.ws_item_sk = ci.c_customer_sk
    GROUP BY 
        ci.c_customer_sk, ci.c_first_name, ci.c_last_name
),
final_report AS (
    SELECT 
        tc.c_customer_sk,
        tc.c_first_name,
        tc.c_last_name,
        tc.total_spent,
        COALESCE(mr.total_returns, 0) AS total_returns,
        COALESCE(mr.total_return_amt, 0) AS total_return_amt,
        CASE 
            WHEN tc.total_spent > 1000 THEN 'VIP'
            WHEN tc.total_spent BETWEEN 500 AND 1000 THEN 'Loyal'
            ELSE 'Regular'
        END AS customer_status
    FROM 
        top_customers tc
    LEFT JOIN monthly_returns mr ON DATE_TRUNC('month', cast('2002-10-01 12:34:56' as timestamp)) = mr.return_month
)
SELECT 
    fr.c_customer_sk,
    fr.c_first_name,
    fr.c_last_name,
    fr.total_spent,
    fr.total_returns,
    fr.total_return_amt,
    fr.customer_status
FROM 
    final_report fr
WHERE 
    fr.customer_status = 'VIP' 
    OR (fr.total_returns > 0 AND fr.total_return_amt > 100)
ORDER BY 
    fr.total_spent DESC
LIMIT 100;