
WITH RankedSales AS (
    SELECT 
        c.c_customer_id,
        SUM(ws.ws_net_profit) AS total_net_profit,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        COUNT(DISTINCT ws.ws_item_sk) AS unique_items,
        DENSE_RANK() OVER (PARTITION BY c.c_birth_year ORDER BY SUM(ws.ws_net_profit) DESC) AS profit_rank
    FROM 
        customer c
    JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year = 2023
    GROUP BY 
        c.c_customer_id, c.c_birth_year
),
TopCustomers AS (
    SELECT 
        r.c_customer_id,
        r.total_net_profit,
        r.total_orders,
        r.unique_items
    FROM 
        RankedSales r
    WHERE 
        r.profit_rank <= 10
)
SELECT 
    tc.c_customer_id,
    tc.total_net_profit,
    tc.total_orders,
    tc.unique_items,
    STRING_AGG(DISTINCT p.p_promo_name, ', ') AS applied_promotions
FROM 
    TopCustomers tc
LEFT JOIN 
    web_sales ws ON tc.c_customer_id = ws.ws_bill_customer_sk
LEFT JOIN 
    promotion p ON ws.ws_promo_sk = p.p_promo_sk
GROUP BY 
    tc.c_customer_id, 
    tc.total_net_profit, 
    tc.total_orders, 
    tc.unique_items
ORDER BY 
    tc.total_net_profit DESC;
