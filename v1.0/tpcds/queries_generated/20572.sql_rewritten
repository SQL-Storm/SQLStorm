WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_order_number,
        ws.ws_quantity,
        ws.ws_ext_sales_price,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.ws_ext_sales_price DESC) AS rank_by_sales
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sold_date_sk BETWEEN 2450000 AND 2451000 
),
CustomerReturns AS (
    SELECT 
        cr.returning_customer_sk,
        SUM(cr.return_quantity) AS total_return_quantity,
        COUNT(cr.return_order_number) AS total_return_count,
        SUM(cr.return_amount) AS total_return_amount
    FROM 
        catalog_returns cr
    GROUP BY 
        cr.returning_customer_sk
),
CustomerDemographics AS (
    SELECT 
        cd.cd_demo_sk,
        cd.cd_gender,
        hd.hd_income_band_sk,
        hd.hd_buy_potential
    FROM 
        customer_demographics cd
    JOIN 
        household_demographics hd ON cd.cd_demo_sk = hd.hd_demo_sk
),
SalesAggregated AS (
    SELECT 
        s_store_sk,
        SUM(ss_net_profit) AS total_net_profit,
        COUNT(ss_ticket_number) AS total_sales_count,
        AVG(ss_sales_price) AS avg_sales_price
    FROM 
        store_sales ss
    GROUP BY 
        s_store_sk
),
QualifiedReturns AS (
    SELECT 
        cr.returning_customer_sk,
        cr.return_order_number
    FROM 
        CustomerReturns cr
    WHERE 
        cr.total_return_quantity > 1 AND cr.total_return_amount > 100
)
SELECT 
    c.c_customer_id,
    c.c_first_name,
    c.c_last_name,
    cd.cd_gender,
    SUM(COALESCE(st.total_sales_count, 0)) AS total_store_sales_count,
    SUM(COALESCE(cr.total_return_count, 0)) AS total_return_count,
    RANK() OVER (ORDER BY SUM(COALESCE(st.total_net_profit, 0)) DESC) AS customer_rank,
    STRING_AGG(CAST(d.d_day_name AS VARCHAR), ', ') AS sales_days
FROM 
    customer c
LEFT JOIN 
    CustomerDemographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
LEFT JOIN 
    SalesAggregated st ON c.c_customer_sk = st.s_store_sk
LEFT JOIN 
    QualifiedReturns cr ON c.c_customer_sk = cr.returning_customer_sk
LEFT JOIN 
    date_dim d ON d.d_date_sk = c.c_first_sales_date_sk
GROUP BY 
    c.c_customer_id, c.c_first_name, c.c_last_name, cd.cd_gender
HAVING 
    SUM(COALESCE(st.total_sales_count, 0)) > 5 
    AND COUNT(DISTINCT cr.return_order_number) = 0
ORDER BY 
    customer_rank, c.c_last_name ASC;