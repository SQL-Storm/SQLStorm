
WITH CustomerReturns AS (
    SELECT 
        wr_returning_customer_sk,
        wr_return_quantity,
        SUM(wr_return_amt) AS total_return_amt,
        COUNT(*) AS total_returns
    FROM 
        web_returns
    GROUP BY 
        wr_returning_customer_sk
),
CustomerDemographics AS (
    SELECT 
        cd_demo_sk,
        cd_gender,
        cd_marital_status,
        cd_income_band_sk
    FROM 
        customer_demographics
),
IncomeBracket AS (
    SELECT 
        ib_income_band_sk,
        ib_lower_bound,
        ib_upper_bound
    FROM 
        income_band
),
DemographicsWithReturns AS (
    SELECT 
        cd.cd_gender AS gender,
        cd.cd_marital_status AS marital_status,
        ib.ib_lower_bound AS lower_bound,
        ib.ib_upper_bound AS upper_bound,
        cr.total_return_amt,
        cr.total_returns
    FROM 
        CustomerReturns cr
    JOIN 
        customer c ON cr.wr_returning_customer_sk = c.c_customer_sk
    JOIN 
        CustomerDemographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        IncomeBracket ib ON cd.cd_income_band_sk = ib.ib_income_band_sk
)
SELECT 
    gender,
    marital_status,
    COUNT(*) AS count_customers,
    AVG(total_return_amt) AS avg_return_amt,
    AVG(total_returns) AS avg_returns
FROM 
    DemographicsWithReturns
GROUP BY 
    gender,
    marital_status
ORDER BY 
    count_customers DESC
FETCH FIRST 10 ROWS ONLY;
