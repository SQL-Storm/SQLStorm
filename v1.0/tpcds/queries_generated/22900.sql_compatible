
WITH RankedSales AS (
    SELECT 
        ws.ws_order_number,
        ws.ws_item_sk,
        ws.ws_quantity,
        ws.ws_sales_price,
        ws.ws_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.ws_item_sk ORDER BY ws.ws_net_profit DESC) AS ProfitRank
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sold_date_sk BETWEEN 2459900 AND 2459913 
),
CustomerInfo AS (
    SELECT 
        c.c_customer_sk,
        CONCAT(c.c_first_name, ' ', c.c_last_name) AS FullName,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_birth_month AS BirthMonth,
        COALESCE(hd.hd_income_band_sk, 0) AS IncomeBand
    FROM 
        customer c
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    LEFT JOIN 
        household_demographics hd ON c.c_customer_sk = hd.hd_demo_sk
),
MonthlyReturns AS (
    SELECT 
        sr_returned_date_sk,
        SUM(sr_return_quantity) AS TotalReturns,
        SUM(sr_return_amt) AS TotalReturnAmount
    FROM 
        store_returns
    GROUP BY 
        sr_returned_date_sk
)
SELECT 
    ci.FullName,
    ci.cd_gender,
    ci.BirthMonth,
    RS.ws_order_number,
    RS.ws_item_sk,
    RS.ws_quantity,
    RS.ws_sales_price,
    RS.ws_net_profit,
    COALESCE(MR.TotalReturns, 0) AS TotalReturns,
    MR.TotalReturnAmount,
    CASE 
        WHEN ci.cd_marital_status = 'M' THEN 'Married'
        WHEN ci.cd_marital_status = 'S' THEN 'Single'
        ELSE 'Unknown'
    END AS MaritalStatus,
    CASE 
        WHEN RS.ProfitRank <= 5 THEN 'Top 5 Profitable Items'
        ELSE 'Other Items'
    END AS ProfitCategory
FROM 
    RankedSales RS
JOIN 
    CustomerInfo ci ON RS.ws_order_number = ci.c_customer_sk
LEFT JOIN 
    MonthlyReturns MR ON RS.ws_item_sk = MR.sr_returned_date_sk
WHERE 
    ci.IncomeBand IS NOT NULL AND 
    ci.BirthMonth IN (SELECT DISTINCT d_moy FROM date_dim WHERE d_year = 2001)
ORDER BY 
    RS.ws_net_profit DESC,
    ci.FullName;
