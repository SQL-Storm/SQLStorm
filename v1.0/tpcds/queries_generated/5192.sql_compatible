
WITH sales_data AS (
    SELECT 
        ws_item_sk,
        SUM(ws_quantity) AS total_sales_quantity,
        SUM(ws_ext_sales_price) AS total_sales_amount,
        AVG(ws_net_profit) AS average_net_profit,
        COUNT(DISTINCT ws_order_number) AS total_orders
    FROM web_sales
    WHERE ws_sold_date_sk BETWEEN 2451557 AND 2452200
    GROUP BY ws_item_sk
),
demographics_data AS (
    SELECT 
        cd_demo_sk,
        cd_gender,
        cd_marital_status,
        cd_education_status,
        COUNT(DISTINCT c_customer_sk) AS customer_count
    FROM customer_demographics
    JOIN customer ON cd_demo_sk = c_current_cdemo_sk
    GROUP BY cd_demo_sk, cd_gender, cd_marital_status, cd_education_status
),
combined_data AS (
    SELECT 
        s.ws_item_sk,
        d.cd_gender,
        d.cd_marital_status,
        d.cd_education_status,
        sd.total_sales_quantity,
        sd.total_sales_amount,
        sd.average_net_profit,
        sd.total_orders
    FROM sales_data sd
    JOIN item i ON sd.ws_item_sk = i.i_item_sk
    JOIN demographics_data d ON 1 = 1  
)
SELECT 
    cd_gender,
    cd_marital_status,
    cd_education_status,
    SUM(total_sales_quantity) AS gender_marital_education_sales_quantity,
    SUM(total_sales_amount) AS gender_marital_education_sales_amount,
    AVG(average_net_profit) AS average_net_profit,
    SUM(total_orders) AS total_orders
FROM combined_data
GROUP BY cd_gender, cd_marital_status, cd_education_status
ORDER BY gender_marital_education_sales_amount DESC
LIMIT 10;
