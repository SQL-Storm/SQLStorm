
WITH sales_summary AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        SUM(ws.ws_quantity) AS total_quantity,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        AVG(ws.net_profit) AS avg_net_profit,
        ds.d_year AS sales_year
    FROM 
        web_sales ws
    JOIN 
        date_dim ds ON ws.ws_sold_date_sk = ds.d_date_sk
    JOIN 
        customer c ON c.c_customer_sk = ws.ws_bill_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        (ds.d_year = 2022 OR ds.d_year = 2023)
        AND cd.cd_gender = 'F'
        AND cd.cd_marital_status = 'M'
    GROUP BY 
        ws.web_site_id, ds.d_year
),
warehouse_summary AS (
    SELECT 
        w.w_warehouse_id,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders_warehouse
    FROM 
        warehouse w
    JOIN 
        web_sales ws ON w.w_warehouse_sk = ws.ws_warehouse_sk
    GROUP BY 
        w.w_warehouse_id
)
SELECT 
    ss.web_site_id,
    ss.total_sales,
    ss.total_quantity,
    ss.total_orders,
    ss.avg_net_profit,
    ws.total_orders_warehouse,
    ss.sales_year
FROM 
    sales_summary ss
JOIN 
    warehouse_summary ws ON ss.web_site_id = ws.w_warehouse_id
ORDER BY 
    ss.total_sales DESC
FETCH FIRST 100 ROWS ONLY;
