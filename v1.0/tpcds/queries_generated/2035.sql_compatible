
WITH CustomerSales AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        SUM(COALESCE(ss.ss_net_paid, 0) + COALESCE(cs.cs_net_paid, 0) + COALESCE(ws.ws_net_paid, 0)) AS total_spent,
        COUNT(DISTINCT CASE 
            WHEN ss.ss_ticket_number IS NOT NULL THEN ss.ss_ticket_number 
            ELSE NULL 
        END) AS store_transactions,
        COUNT(DISTINCT CASE 
            WHEN cs.cs_order_number IS NOT NULL THEN cs.cs_order_number 
            ELSE NULL 
        END) AS catalog_transactions,
        COUNT(DISTINCT CASE 
            WHEN ws.ws_order_number IS NOT NULL THEN ws.ws_order_number 
            ELSE NULL 
        END) AS web_transactions
    FROM 
        customer c
    LEFT JOIN 
        store_sales ss ON c.c_customer_sk = ss.ss_customer_sk 
    LEFT JOIN 
        catalog_sales cs ON c.c_customer_sk = cs.cs_ship_customer_sk 
    LEFT JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_ship_customer_sk 
    WHERE 
        (c.c_birth_year BETWEEN 1980 AND 1990 OR c.c_country = 'USA') 
        AND c.c_current_addr_sk IS NOT NULL 
    GROUP BY 
        c.c_customer_sk, c.c_first_name, c.c_last_name
),
IncomeBracket AS (
    SELECT 
        hd.hd_demo_sk,
        ib.ib_lower_bound,
        ib.ib_upper_bound,
        COUNT(*) AS num_customers
    FROM 
        household_demographics hd
    JOIN 
        income_band ib ON hd.hd_income_band_sk = ib.ib_income_band_sk 
    GROUP BY 
        hd.hd_demo_sk, ib.ib_lower_bound, ib.ib_upper_bound
)
SELECT 
    cs.c_first_name,
    cs.c_last_name,
    cs.total_spent,
    ib.ib_lower_bound,
    ib.ib_upper_bound,
    COUNT(cs.c_customer_sk) OVER (PARTITION BY ib.ib_lower_bound, ib.ib_upper_bound) AS customers_in_bracket
FROM 
    CustomerSales cs
LEFT JOIN 
    IncomeBracket ib ON cs.c_customer_sk = ib.hd_demo_sk 
WHERE 
    cs.total_spent IS NOT NULL 
ORDER BY 
    cs.total_spent DESC
LIMIT 10;
