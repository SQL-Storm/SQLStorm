
SELECT 
    ca.city AS City,
    COUNT(DISTINCT c.c_customer_id) AS Total_Customers,
    AVG(i.i_current_price) AS Average_Item_Price,
    SUM(ws.ws_net_profit) AS Total_Net_Profit,
    COUNT(DISTINCT ws.ws_order_number) AS Total_Orders,
    STRING_AGG(DISTINCT p.p_promo_name, ', ') AS Promos_Used,
    EXTRACT(YEAR FROM d.d_date) AS Year
FROM 
    customer_address ca
JOIN 
    customer c ON ca.ca_address_sk = c.c_current_addr_sk
JOIN 
    web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
JOIN 
    item i ON ws.ws_item_sk = i.i_item_sk
JOIN 
    promotion p ON ws.ws_promo_sk = p.p_promo_sk
JOIN 
    date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
WHERE 
    d.d_date >= '2023-01-01' AND d.d_date <= '2023-12-31'
GROUP BY 
    ca.city, EXTRACT(YEAR FROM d.d_date)
ORDER BY 
    Total_Net_Profit DESC, Total_Customers DESC
LIMIT 10;
