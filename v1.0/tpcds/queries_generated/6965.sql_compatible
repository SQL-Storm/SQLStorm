
WITH ranked_sales AS (
    SELECT 
        s.s_store_name, 
        i.i_item_id, 
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_net_paid) AS total_revenue,
        DENSE_RANK() OVER (PARTITION BY s.s_store_name ORDER BY SUM(ws.ws_net_paid) DESC) AS revenue_rank
    FROM 
        store s
    JOIN 
        web_sales ws ON s.s_store_sk = ws.ws_ship_addr_sk
    JOIN 
        item i ON ws.ws_item_sk = i.i_item_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year = 2001
        AND d.d_moy IN (11, 12)  
    GROUP BY 
        s.s_store_name, i.i_item_id
),
top_items AS (
    SELECT 
        s.s_store_name AS store_name, 
        i.i_item_id, 
        total_quantity, 
        total_revenue
    FROM 
        ranked_sales
    WHERE 
        revenue_rank <= 5
)
SELECT 
    ti.store_name,
    ti.i_item_id,
    ti.total_quantity,
    ti.total_revenue,
    cd.cd_gender,
    cd.cd_marital_status
FROM 
    top_items ti
JOIN 
    customer c ON c.c_current_addr_sk = (SELECT ca_address_sk FROM customer_address WHERE ca_country = 'USA' LIMIT 1)
JOIN 
    customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
ORDER BY 
    ti.store_name, ti.total_revenue DESC;
