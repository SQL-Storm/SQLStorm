
WITH CustomerSales AS (
    SELECT 
        c.c_customer_id,
        SUM(ss.ss_sales_price) AS total_sales,
        AVG(ss.ss_sales_price) AS avg_sales_price,
        COUNT(DISTINCT ss.ss_ticket_number) AS num_transactions
    FROM 
        customer c
    JOIN 
        store_sales ss ON c.c_customer_sk = ss.ss_customer_sk
    WHERE 
        c.c_birth_year BETWEEN 1970 AND 1990
    GROUP BY 
        c.c_customer_id
),
SalesByWarehouse AS (
    SELECT 
        w.w_warehouse_id,
        SUM(cs.total_sales) AS warehouse_sales,
        COUNT(cs.c_customer_id) AS num_customers
    FROM 
        CustomerSales cs
    JOIN 
        store s ON cs.c_customer_id = s.s_store_id
    JOIN 
        warehouse w ON s.s_store_sk = w.w_warehouse_sk
    GROUP BY 
        w.w_warehouse_id
),
MostProfitableWarehouse AS (
    SELECT 
        w.w_warehouse_id,
        w.w_warehouse_name,
        ROUND((warehouse_sales / NULLIF(num_customers, 0)), 2) AS avg_sales_per_customer
    FROM 
        SalesByWarehouse w
    ORDER BY 
        warehouse_sales DESC
    LIMIT 1
)
SELECT 
    mw.w_warehouse_id,
    mw.w_warehouse_name,
    mw.avg_sales_per_customer,
    COALESCE(SUM(ws.ws_net_profit), 0) AS total_net_profit
FROM 
    MostProfitableWarehouse mw
LEFT JOIN 
    web_sales ws ON mw.w_warehouse_id = ws.ws_web_site_sk
GROUP BY 
    mw.w_warehouse_id, mw.w_warehouse_name, mw.avg_sales_per_customer;
