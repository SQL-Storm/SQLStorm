
WITH SalesData AS (
    SELECT 
        ws.web_site_sk,
        SUM(ws.ws_net_profit) AS total_net_profit,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        ROW_NUMBER() OVER(PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_net_profit) DESC) AS rnk
    FROM 
        web_sales ws
    INNER JOIN 
        customer c ON ws.ws_ship_customer_sk = c.c_customer_sk
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_marital_status = 'M' AND 
        cd.cd_gender = 'F' AND 
        c.c_birth_year BETWEEN 1970 AND 1990
    GROUP BY 
        ws.web_site_sk
),
TopSales AS (
    SELECT 
        web_site_sk,
        total_net_profit,
        total_orders
    FROM 
        SalesData
    WHERE 
        rnk <= 5
)
SELECT 
    w.warehouse_id,
    COALESCE(ts.total_net_profit, 0) AS total_profit,
    (SELECT COUNT(*) 
     FROM web_page wp 
     WHERE wp.wp_customer_sk IS NOT NULL
     AND wp.wp_creation_date_sk > 
         (SELECT MAX(d_date_sk) 
          FROM date_dim dd 
          WHERE dd.d_year = 2023)
    ) AS total_web_pages,
    (SELECT SUM(sr.sr_return_amt) 
     FROM store_returns sr 
     WHERE sr.sr_returned_date_sk = (SELECT MAX(d_date_sk) 
                                       FROM date_dim 
                                       WHERE d_year = 2023)
    ) AS total_returns,
    w.warehouse_name,
    w.warehouse_sq_ft
FROM 
    warehouse w
LEFT JOIN 
    TopSales ts ON ts.web_site_sk = w.warehouse_sk
ORDER BY 
    total_profit DESC, 
    total_orders DESC
LIMIT 10;
