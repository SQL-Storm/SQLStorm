
WITH RankedSales AS (
    SELECT
        ws.web_site_id,
        ws.ws_order_number,
        ws.ws_sales_price,
        ws.ws_quantity,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_id ORDER BY ws.ws_sales_price DESC) AS sale_rank
    FROM web_sales ws
    JOIN customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE cd.cd_gender = 'F' AND cd.cd_marital_status = 'M'
    AND ws.ws_sold_date_sk BETWEEN (SELECT MAX(d_date_sk) - 30 FROM date_dim) AND (SELECT MAX(d_date_sk) FROM date_dim)
),
TopSales AS (
    SELECT web_site_id, SUM(ws_sales_price) AS total_sales
    FROM RankedSales
    WHERE sale_rank <= 10
    GROUP BY web_site_id
)
SELECT
    w.warehouse_name,
    ts.web_site_id,
    ts.total_sales
FROM TopSales ts
JOIN warehouse w ON ts.web_site_id = w.w_warehouse_id
ORDER BY ts.total_sales DESC
FETCH FIRST 5 ROWS ONLY;
