WITH SalesData AS (
    SELECT 
        ws_item_sk,
        SUM(ws_quantity) AS total_quantity,
        SUM(ws_sales_price) AS total_sales,
        SUM(ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws_order_number) AS total_orders
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk BETWEEN 2459193 AND 2459247  
    GROUP BY 
        ws_item_sk
),
CustomerData AS (
    SELECT 
        c_customer_sk,
        cd_gender,
        cd_marital_status,
        cd_education_status,
        cd_purchase_estimate,
        cd_credit_rating,
        cd_dep_count,
        cd_dep_employed_count,
        cd_dep_college_count
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
SalesRanking AS (
    SELECT 
        sd.ws_item_sk,
        sd.total_quantity,
        sd.total_sales,
        sd.total_profit,
        sd.total_orders,
        ROW_NUMBER() OVER (ORDER BY sd.total_profit DESC) AS sales_rank
    FROM 
        SalesData sd
)
SELECT 
    sr.sales_rank,
    sr.ws_item_sk,
    sr.total_quantity,
    sr.total_sales,
    sr.total_profit,
    cd.gender,
    cd.education_status,
    cd.marital_status,
    COUNT(DISTINCT cd.c_customer_sk) AS customer_count
FROM 
    SalesRanking sr
JOIN 
    CustomerData cd ON sr.ws_item_sk IN (
        SELECT 
            ws_item_sk 
        FROM 
            web_sales 
        WHERE 
            ws_order_number IN (
                SELECT 
                    DISTINCT ws_order_number 
                FROM 
                    web_sales 
                WHERE 
                    ws_item_sk = sr.ws_item_sk
            )
    )
GROUP BY 
    sr.sales_rank, sr.ws_item_sk, sr.total_quantity, sr.total_sales, sr.total_profit, cd.gender, cd.education_status, cd.marital_status
ORDER BY 
    sr.sales_rank
LIMIT 10;