
WITH SalesData AS (
    SELECT 
        ws.web_site_id,
        cs.cs_order_number,
        cs.cs_quantity,
        cs.cs_sales_price,
        cs.cs_ext_sales_price,
        cs.cs_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_id ORDER BY cs.cs_net_profit DESC) AS rank
    FROM web_sales ws
    JOIN catalog_sales cs ON ws.ws_order_number = cs.cs_order_number
    WHERE cs.cs_sold_date_sk = (SELECT MAX(cs2.cs_sold_date_sk) FROM catalog_sales cs2)
),
HighestProfit AS (
    SELECT 
        web_site_id,
        SUM(cs_net_profit) AS total_profit
    FROM SalesData
    WHERE rank <= 10
    GROUP BY web_site_id
)
SELECT 
    w.warehouse_id,
    w.warehouse_name,
    COALESCE(hp.total_profit, 0) AS total_profit,
    (SELECT AVG(cs_net_profit) FROM SalesData) AS average_profit,
    (SELECT COUNT(DISTINCT w.site_id) FROM web_site w) AS total_websites
FROM warehouse w
LEFT JOIN HighestProfit hp ON w.warehouse_id = hp.web_site_id
WHERE w.warehouse_sq_ft > (SELECT AVG(w2.warehouse_sq_ft) FROM warehouse w2)
ORDER BY total_profit DESC;
