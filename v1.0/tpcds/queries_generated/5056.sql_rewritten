WITH RankedSales AS (
    SELECT 
        ws.web_site_id, 
        SUM(ws.ws_sales_price * ws.ws_quantity) AS total_revenue,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        DENSE_RANK() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws.ws_sales_price * ws.ws_quantity) DESC) AS revenue_rank
    FROM 
        web_sales ws
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_gender = 'F' 
        AND cd.cd_marital_status = 'M' 
        AND cd.cd_education_status IN ('Bachelors', 'Masters')
    GROUP BY 
        ws.web_site_id
),
TopWebSites AS (
    SELECT 
        web_site_id,
        total_revenue,
        total_orders
    FROM 
        RankedSales
    WHERE 
        revenue_rank <= 5 
)
SELECT 
    w.web_site_id,
    w.web_name,
    t.total_revenue,
    t.total_orders,
    (SELECT AVG(total_revenue) FROM TopWebSites) AS avg_revenue,
    (SELECT AVG(total_orders) FROM TopWebSites) AS avg_orders
FROM 
    web_site w
JOIN 
    TopWebSites t ON w.web_site_id = t.web_site_id
ORDER BY 
    t.total_revenue DESC;