
WITH ranked_sales AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_sales_price) AS total_sales,
        DENSE_RANK() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws.ws_sales_price) DESC) AS sales_rank
    FROM
        web_sales ws
    JOIN
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE
        dd.d_year = 2022
    GROUP BY 
        ws.web_site_id
),
top_sales AS (
    SELECT 
        web_site_id, 
        total_quantity, 
        total_sales
    FROM 
        ranked_sales
    WHERE 
        sales_rank <= 10
)
SELECT
    wa.w_warehouse_name,
    SUM(ws.ws_quantity) AS total_quantity_sold,
    SUM(ws.ws_net_profit) AS total_profit
FROM 
    top_sales ts
JOIN 
    web_sales ws ON ts.web_site_id = ws.ws_web_site_sk
JOIN 
    warehouse wa ON ws.ws_warehouse_sk = wa.w_warehouse_sk
GROUP BY 
    wa.w_warehouse_name
ORDER BY 
    total_profit DESC;
