WITH ranked_customers AS (
    SELECT 
        c.c_customer_id,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_education_status,
        SUM(ss.ss_net_profit) AS total_net_profit,
        DENSE_RANK() OVER (PARTITION BY cd.cd_gender, cd.cd_marital_status ORDER BY SUM(ss.ss_net_profit) DESC) AS gender_marital_rank
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        store_sales ss ON c.c_customer_sk = ss.ss_customer_sk
    WHERE 
        ss.ss_sold_date_sk BETWEEN 2450000 AND 2450500 
    GROUP BY 
        c.c_customer_id, cd.cd_gender, cd.cd_marital_status, cd.cd_education_status
),
top_customers AS (
    SELECT 
        rc.c_customer_id,
        rc.total_net_profit,
        rc.cd_gender,
        rc.cd_marital_status,
        rc.cd_education_status
    FROM 
        ranked_customers rc
    WHERE 
        rc.gender_marital_rank <= 10 
)
SELECT 
    tc.c_customer_id,
    tc.total_net_profit,
    ca.ca_city,
    ca.ca_state,
    ca.ca_country,
    COUNT(ws.ws_order_number) AS total_orders,
    AVG(ws.ws_net_profit) AS avg_order_profit
FROM 
    top_customers tc
JOIN 
    customer_address ca ON tc.c_customer_id = ca.ca_address_id
JOIN 
    web_sales ws ON tc.c_customer_id = ws.ws_bill_customer_sk
GROUP BY 
    tc.c_customer_id, tc.total_net_profit, ca.ca_city, ca.ca_state, ca.ca_country
ORDER BY 
    tc.total_net_profit DESC
LIMIT 
    20;