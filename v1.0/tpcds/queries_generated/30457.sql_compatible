
WITH RECURSIVE sales_hierarchy AS (
    SELECT 
        ws_sold_date_sk,
        ws_item_sk,
        ws_order_number,
        ws_quantity,
        ws_sales_price,
        ws_net_profit,
        1 AS level
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk >= (SELECT MIN(d_date_sk) FROM date_dim WHERE d_year = 2023)
    UNION ALL
    SELECT 
        sh.ws_sold_date_sk,
        sh.ws_item_sk,
        sh.ws_order_number,
        sh.ws_quantity,
        sh.ws_sales_price,
        sh.ws_net_profit,
        sh.level + 1
    FROM 
        web_sales sh
    INNER JOIN sales_hierarchy shier ON sh.ws_item_sk = shier.ws_item_sk AND sh.ws_order_number = shier.ws_order_number
    WHERE 
        sh.ws_sold_date_sk < shier.ws_sold_date_sk
)

SELECT 
    ca.ca_address_id,
    cd.cd_gender,
    COUNT(DISTINCT ws.ws_order_number) AS total_orders,
    SUM(ws.ws_quantity) AS total_quantity,
    SUM(ws.ws_net_profit) AS total_net_profit,
    MAX(ws.ws_sales_price) AS max_sales_price,
    MIN(ws.ws_sales_price) AS min_sales_price,
    ROW_NUMBER() OVER (PARTITION BY ca.ca_state ORDER BY SUM(ws.ws_net_profit) DESC) AS state_rank
FROM 
    web_sales ws
JOIN 
    customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
JOIN 
    customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
JOIN 
    customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
LEFT JOIN 
    sales_hierarchy sh ON ws.ws_order_number = sh.ws_order_number
WHERE 
    cd.cd_marital_status = 'M'
    AND cd.cd_gender = 'F'
    AND ca.ca_country = 'USA'
    AND (ws.ws_net_profit > 0 OR ws.ws_net_profit IS NULL)
GROUP BY 
    ca.ca_address_id, cd.cd_gender, ca.ca_state
HAVING 
    COUNT(DISTINCT ws.ws_order_number) > 5
ORDER BY 
    total_net_profit DESC
LIMIT 10;
