
WITH SalesData AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.net_profit) AS total_net_profit,
        COUNT(DISTINCT ws.order_number) AS total_orders,
        AVG(ws.net_paid) AS avg_order_value
    FROM 
        web_sales AS ws
    JOIN 
        customer AS c ON ws.bill_customer_sk = c.c_customer_sk
    JOIN 
        customer_demographics AS cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        date_dim AS dd ON ws.sold_date_sk = dd.d_date_sk
    JOIN 
        item AS i ON ws.item_sk = i.i_item_sk
    WHERE 
        dd.d_year = 2022
        AND cd.cd_gender = 'F'
        AND i.i_current_price > 50.00
    GROUP BY 
        ws.web_site_id 
),
WarehouseMetrics AS (
    SELECT 
        w.warehouse_id,
        COUNT(DISTINCT ws.order_number) AS total_orders_from_warehouse,
        SUM(ws.net_profit) AS warehouse_net_profit
    FROM 
        warehouse AS w
    JOIN 
        web_sales AS ws ON w.warehouse_sk = ws.warehouse_sk
    GROUP BY 
        w.warehouse_id
)
SELECT 
    sd.web_site_id,
    sd.total_net_profit,
    sd.total_orders,
    sd.avg_order_value,
    wm.total_orders_from_warehouse,
    wm.warehouse_net_profit
FROM 
    SalesData sd
LEFT JOIN 
    WarehouseMetrics wm ON sd.web_site_id = wm.warehouse_id
ORDER BY 
    sd.total_net_profit DESC
FETCH FIRST 10 ROWS ONLY;
