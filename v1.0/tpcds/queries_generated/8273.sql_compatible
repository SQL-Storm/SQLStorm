
WITH RankedSales AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        COUNT(DISTINCT ws.ws_ship_customer_sk) AS unique_customers,
        DENSE_RANK() OVER (ORDER BY SUM(ws.ws_net_profit) DESC) AS profit_rank
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    JOIN 
        customer c ON ws.ws_ship_customer_sk = c.c_customer_sk
    WHERE 
        dd.d_year = 2022
        AND c.c_birth_country = 'USA'
    GROUP BY 
        ws.web_site_id
)
SELECT 
    w.warehouse_id,
    w.warehouse_name,
    rs.total_profit,
    rs.total_orders,
    rs.unique_customers,
    rs.profit_rank
FROM 
    RankedSales rs
JOIN 
    warehouse w ON rs.web_site_id = w.warehouse_id
WHERE 
    rs.profit_rank <= 10
ORDER BY 
    rs.total_profit DESC;
