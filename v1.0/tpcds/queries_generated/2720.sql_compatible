
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_order_number,
        SUM(ws.ws_net_paid) AS total_sales,
        DENSE_RANK() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_net_paid) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_marital_status = 'M'
        AND cd.cd_gender = 'F'
        AND ws.ws_sold_date_sk >= (SELECT MAX(d_date_sk) 
                                    FROM date_dim 
                                    WHERE d_year = 2023)
    GROUP BY 
        ws.web_site_sk, 
        ws.ws_order_number
),
StoreReturns AS (
    SELECT 
        sr_store_sk,
        COUNT(sr_return_quantity) AS total_returns,
        SUM(sr_return_amt_inc_tax) AS total_return_value
    FROM 
        store_returns
    GROUP BY 
        sr_store_sk
),
TopWebsites AS (
    SELECT 
        web_site_sk,
        ROW_NUMBER() OVER (ORDER BY total_sales DESC) AS row_num
    FROM 
        RankedSales
    WHERE 
        sales_rank = 1
)
SELECT 
    s.s_store_sk,
    s.s_store_name,
    COALESCE(ts.total_sales, 0) AS total_web_sales,
    COALESCE(sr.total_returns, 0) AS total_store_returns,
    COALESCE(sr.total_return_value, 0) AS total_return_value
FROM 
    store s
LEFT JOIN 
    TopWebsites ts ON s.s_store_sk = ts.web_site_sk
LEFT JOIN 
    StoreReturns sr ON s.s_store_sk = sr.sr_store_sk
WHERE 
    s.s_state = 'CA'
ORDER BY 
    total_web_sales DESC, total_store_returns DESC;
