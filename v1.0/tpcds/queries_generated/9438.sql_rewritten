WITH sales_data AS (
    SELECT 
        w.w_warehouse_id,
        c.c_customer_id,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        AVG(ws.ws_net_profit) AS avg_net_profit
    FROM 
        warehouse w
    JOIN 
        web_sales ws ON w.w_warehouse_sk = ws.ws_warehouse_sk
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year = 2001 AND d.d_moy IN (6, 7) 
    GROUP BY 
        w.w_warehouse_id, c.c_customer_id
), 
customer_demographics AS (
    SELECT 
        cd_demo_sk,
        cd_gender,
        cd_marital_status,
        cd_purchase_estimate
    FROM 
        customer_demographics
    WHERE 
        cd_purchase_estimate > 50000
),
combined_data AS (
    SELECT 
        sd.w_warehouse_id,
        sd.c_customer_id,
        sd.total_sales,
        sd.order_count,
        sd.avg_net_profit,
        cd.cd_gender,
        cd.cd_marital_status
    FROM 
        sales_data sd
    LEFT JOIN 
        customer_demographics cd ON sd.c_customer_id = cd.cd_demo_sk
)
SELECT 
    w.w_warehouse_id,
    cd_gender,
    cd_marital_status,
    COUNT(*) AS customer_count,
    SUM(total_sales) AS total_sales_sum,
    AVG(avg_net_profit) AS average_profit
FROM 
    combined_data
JOIN 
    warehouse w ON combined_data.w_warehouse_id = w.w_warehouse_id
GROUP BY 
    w.w_warehouse_id, cd_gender, cd_marital_status
ORDER BY 
    total_sales_sum DESC, customer_count DESC
LIMIT 10;