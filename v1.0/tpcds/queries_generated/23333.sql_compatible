
WITH RankedSales AS (
    SELECT 
        ws.ws_item_sk,
        ws.ws_order_number,
        ws.ws_quantity,
        ws.net_paid_inc_tax,
        ROW_NUMBER() OVER (PARTITION BY ws.ws_item_sk ORDER BY ws.ws_sales_price DESC) AS SalesRank
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sales_price IS NOT NULL AND 
        ws.ws_ship_date_sk BETWEEN 1000 AND 2000
),
AggregatedSales AS (
    SELECT
        rs.ws_item_sk,
        COUNT(rs.ws_order_number) AS total_orders,
        SUM(rs.ws_quantity) AS total_quantity,
        AVG(rs.net_paid_inc_tax) AS avg_paid
    FROM 
        RankedSales rs
    WHERE 
        rs.SalesRank <= 5
    GROUP BY 
        rs.ws_item_sk
)
SELECT 
    ca.ca_city,
    ca.ca_state,
    CASE 
        WHEN ABS(income.ib_upper_bound - income.ib_lower_bound) > 10000 THEN 'High Variance'
        ELSE 'Low Variance'
    END AS variance_category,
    COALESCE(SUM(asales.total_orders), 0) AS total_sales_orders,
    COALESCE(SUM(asales.total_quantity), 0) AS total_sales_quantity,
    COALESCE(AVG(asales.avg_paid), 0) AS average_payment
FROM 
    customer_address ca
LEFT JOIN 
    customer c ON ca.ca_address_sk = c.c_current_addr_sk
LEFT JOIN 
    household_demographics hd ON c.c_current_hdemo_sk = hd.hd_demo_sk
LEFT JOIN 
    income_band income ON hd.hd_income_band_sk = income.ib_income_band_sk
LEFT JOIN 
    AggregatedSales asales ON asales.ws_item_sk = c.c_customer_sk
WHERE 
    ca.ca_state IN ('CA', 'NY') AND 
    ca.ca_city IS NOT NULL
GROUP BY 
    ca.ca_city, ca.ca_state, income.ib_lower_bound, income.ib_upper_bound
ORDER BY 
    variance_category DESC, total_sales_orders DESC;
