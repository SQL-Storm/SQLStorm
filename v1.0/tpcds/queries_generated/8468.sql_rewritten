WITH sales_data AS (
    SELECT 
        w.w_warehouse_name,
        c.c_first_name,
        c.c_last_name,
        SUM(ss.ss_sales_price) AS total_sales,
        COUNT(ss.ss_ticket_number) AS total_transactions,
        AVG(ss.ss_sales_price) AS average_sale_price
    FROM 
        store_sales ss 
    JOIN 
        store s ON ss.ss_store_sk = s.s_store_sk
    JOIN 
        customer c ON ss.ss_customer_sk = c.c_customer_sk
    JOIN 
        warehouse w ON s.s_store_sk = w.w_warehouse_sk
    WHERE 
        ss.ss_sold_date_sk BETWEEN 2450000 AND 2450500 
    GROUP BY 
        w.w_warehouse_name, c.c_first_name, c.c_last_name
),
demographic_data AS (
    SELECT 
        cd.cd_gender,
        cd.cd_marital_status,
        COUNT(cd.cd_demo_sk) AS num_customers
    FROM 
        customer_demographics cd
    GROUP BY 
        cd.cd_gender, cd.cd_marital_status
),
final_report AS (
    SELECT 
        sd.w_warehouse_name,
        sd.c_first_name,
        sd.c_last_name,
        sd.total_sales,
        sd.total_transactions,
        sd.average_sale_price,
        dd.cd_gender,
        dd.cd_marital_status,
        dd.num_customers
    FROM 
        sales_data sd
    LEFT JOIN 
        demographic_data dd ON sd.total_transactions > 0
)
SELECT 
    w.w_warehouse_name,
    COUNT(DISTINCT c.c_customer_sk) AS customer_count,
    SUM(sd.total_sales) AS total_sales_sum,
    AVG(sd.average_sale_price) AS avg_sales_per_customer,
    MAX(sd.total_sales) AS max_sales_per_customer,
    dd.cd_gender,
    dd.cd_marital_status
FROM 
    final_report sr
JOIN 
    warehouse w ON sr.w_warehouse_name = w.w_warehouse_name
LEFT JOIN 
    demographic_data dd ON sr.cd_gender = dd.cd_gender AND sr.cd_marital_status = dd.cd_marital_status
GROUP BY 
    w.w_warehouse_name, dd.cd_gender, dd.cd_marital_status
ORDER BY 
    total_sales_sum DESC, customer_count DESC
LIMIT 100;