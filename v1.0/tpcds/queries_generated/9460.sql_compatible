
WITH sales_summary AS (
    SELECT 
        ws.web_site_id,
        wd.d_year,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_sales_price) AS total_sales,
        AVG(ws.ws_net_profit) AS average_profit
    FROM 
        web_sales ws
    JOIN 
        date_dim wd ON ws.ws_sold_date_sk = wd.d_date_sk
    WHERE 
        wd.d_year BETWEEN 2020 AND 2023
    GROUP BY 
        ws.web_site_id, wd.d_year
),
customer_summary AS (
    SELECT 
        cd.cd_gender,
        cd.cd_marital_status,
        SUM(ss.total_sales) AS total_sales_by_gender,
        SUM(ss.total_quantity) AS total_quantity_by_gender,
        AVG(ss.average_profit) AS average_profit_by_gender
    FROM 
        sales_summary ss
    JOIN 
        customer c ON ss.web_site_id = c.c_customer_id
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    GROUP BY 
        cd.cd_gender, cd.cd_marital_status
),
warehouse_summary AS (
    SELECT 
        w.w_warehouse_id,
        SUM(ss.total_sales) AS warehouse_total_sales,
        COUNT(DISTINCT ss.web_site_id) AS unique_web_sites
    FROM 
        sales_summary ss
    JOIN 
        warehouse w ON ss.web_site_id = w.w_warehouse_id
    GROUP BY 
        w.w_warehouse_id
)
SELECT 
    cs.cd_gender,
    cs.cd_marital_status,
    cs.total_sales_by_gender,
    cs.total_quantity_by_gender,
    cs.average_profit_by_gender,
    ws.w_warehouse_id,
    ws.warehouse_total_sales,
    ws.unique_web_sites
FROM 
    customer_summary cs
JOIN 
    warehouse_summary ws ON cs.total_sales_by_gender > 10000
ORDER BY 
    cs.total_sales_by_gender DESC, ws.warehouse_total_sales ASC
LIMIT 50;
