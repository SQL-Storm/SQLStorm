
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_sold_date_sk,
        ws.ws_item_sk,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.ws_net_profit DESC) AS rank_profit,
        SUM(ws.ws_net_profit) OVER (PARTITION BY ws.web_site_sk) AS total_profit
    FROM 
        web_sales ws
    WHERE 
        ws.ws_net_profit IS NOT NULL
        AND ws.ws_shipped_date_sk BETWEEN 10000 AND 20000
),
CustomerReturns AS (
    SELECT 
        sr_customer_sk,
        COUNT(*) AS return_count,
        SUM(sr_return_amt) AS total_returns
    FROM 
        store_returns 
    GROUP BY 
        sr_customer_sk
    HAVING 
        SUM(sr_return_amt) > 100
),
SalesAsReturned AS (
    SELECT 
        cs.cs_order_number, 
        SUM(cs.cs_sales_price) AS total_sales_price,
        MAX(CASE WHEN wr.wr_return_qty IS NOT NULL THEN 'Returned' ELSE 'Not Returned' END) AS return_status
    FROM 
        catalog_sales cs
    LEFT JOIN 
        web_returns wr ON cs.cs_order_number = wr.wr_order_number
    GROUP BY 
        cs.cs_order_number
),
FilteredCustomers AS (
    SELECT 
        c.c_customer_id, 
        cd.cd_gender, 
        cd.cd_marital_status,
        RANK() OVER (PARTITION BY cd.cd_gender ORDER BY cd.cd_purchase_estimate DESC) AS customer_rank,
        cd.cd_purchase_estimate
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_purchase_estimate BETWEEN 100 AND 1000
        AND (cd.cd_marital_status = 'M' OR cd.cd_marital_status IS NULL)
)
SELECT 
    f.c_customer_id,
    f.cd_gender,
    f.cd_marital_status,
    s.total_profit AS web_sales_profit,
    COALESCE(rr.return_count, 0) AS customer_return_count,
    COUNT(DISTINCT s.web_site_sk) AS total_orders,
    ROW_NUMBER() OVER (ORDER BY f.cd_purchase_estimate DESC) AS order_rank
FROM 
    FilteredCustomers f
LEFT JOIN 
    RankedSales s ON f.c_customer_id = s.web_site_sk
LEFT JOIN 
    CustomerReturns rr ON f.c_customer_id = rr.sr_customer_sk
WHERE 
    f.customer_rank <= 10
    AND (f.cd_gender = 'F' OR f.cd_marital_status IS NULL)
GROUP BY 
    f.c_customer_id, f.cd_gender, f.cd_marital_status, s.total_profit, rr.return_count
HAVING 
    COUNT(DISTINCT s.web_site_sk) > 5 OR COUNT(DISTINCT s.web_site_sk) IS NULL
ORDER BY 
    f.c_customer_id;
