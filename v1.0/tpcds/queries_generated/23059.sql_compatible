
WITH RankedSales AS (
    SELECT 
        ws.bill_customer_sk,
        ws.item_sk,
        ws.web_site_sk,
        ws.net_profit,
        RANK() OVER (PARTITION BY ws.bill_customer_sk ORDER BY ws.net_profit DESC) AS profit_rank
    FROM 
        web_sales ws
    WHERE 
        ws.sold_date_sk BETWEEN (SELECT MIN(d_date_sk) FROM date_dim WHERE d_year = 2023) 
                            AND (SELECT MAX(d_date_sk) FROM date_dim WHERE d_year = 2023)
),
CustomerProfits AS (
    SELECT 
        c.c_customer_sk,
        COALESCE(SUM(rs.net_profit), 0) AS total_profit,
        COUNT(DISTINCT rs.item_sk) AS item_count,
        COUNT(DISTINCT rs.web_site_sk) AS website_count
    FROM 
        customer c
    LEFT JOIN 
        RankedSales rs ON c.c_customer_sk = rs.bill_customer_sk AND rs.profit_rank = 1
    GROUP BY 
        c.c_customer_sk
),
TopCustomers AS (
    SELECT 
        cp.c_customer_sk,
        cp.total_profit,
        cp.item_count,
        cp.website_count,
        CASE 
            WHEN cp.total_profit = 0 THEN 'No Sales'
            WHEN cp.total_profit > 1000 THEN 'High Value'
            WHEN cp.total_profit BETWEEN 500 AND 1000 THEN 'Medium Value'
            ELSE 'Low Value' 
        END AS customer_value_category
    FROM 
        CustomerProfits cp
)
SELECT 
    cc.c_customer_id,
    cca.ca_city,
    cca.ca_state,
    tc.total_profit,
    tc.item_count,
    tc.website_count,
    tc.customer_value_category
FROM 
    TopCustomers tc
JOIN 
    customer cc ON tc.c_customer_sk = cc.c_customer_sk
JOIN 
    customer_address cca ON cc.c_current_addr_sk = cca.ca_address_sk
WHERE 
    tc.customer_value_category != 'No Sales'
ORDER BY 
    tc.total_profit DESC, cca.ca_city ASC
LIMIT 10;
