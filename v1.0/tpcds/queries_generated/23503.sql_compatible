
WITH customer_info AS (
    SELECT 
        c.c_customer_sk,
        c.c_customer_id,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_purchase_estimate, 
        cd.cd_credit_rating,
        ROW_NUMBER() OVER (PARTITION BY cd.cd_gender ORDER BY cd.cd_purchase_estimate DESC) AS purchase_rank
    FROM customer AS c
    JOIN customer_demographics AS cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
address_info AS (
    SELECT 
        ca.ca_address_sk,
        ca.ca_city,
        ca.ca_state,
        ca.ca_country,
        COUNT(DISTINCT c.c_customer_sk) AS customer_count
    FROM customer_address AS ca
    LEFT JOIN customer AS c ON c.c_current_addr_sk = ca.ca_address_sk
    GROUP BY ca.ca_address_sk, ca.ca_city, ca.ca_state, ca.ca_country
),
return_summary AS (
    SELECT 
        CASE 
            WHEN sr.return_quantity IS NULL THEN 'No Returns'
            ELSE 'Has Returns'
        END AS return_status,
        COUNT(sr.sr_ticket_number) AS return_count,
        SUM(sr.sr_return_amt_inc_tax) AS total_return_value
    FROM store_returns AS sr
    GROUP BY CASE 
                 WHEN sr.return_quantity IS NULL THEN 'No Returns'
                 ELSE 'Has Returns'
              END
),
sales_info AS (
    SELECT 
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity_sold,
        SUM(ws.ws_net_profit) AS total_net_profit,
        DENSE_RANK() OVER (ORDER BY SUM(ws.ws_net_profit) DESC) AS profit_rank
    FROM web_sales AS ws
    GROUP BY ws.ws_item_sk
)
SELECT 
    ci.c_first_name,
    ci.c_last_name,
    ci.cd_gender,
    ai.ca_city,
    ai.ca_state,
    ai.ca_country,
    ai.customer_count,
    rs.return_status,
    rs.return_count,
    rs.total_return_value,
    si.total_quantity_sold,
    si.total_net_profit
FROM customer_info AS ci
JOIN address_info AS ai ON ai.customer_count > 5
LEFT JOIN return_summary AS rs ON rs.return_status = 'Has Returns'
FULL OUTER JOIN sales_info AS si ON si.total_net_profit > 1000
WHERE ci.purchase_rank < 10 
AND (ci.cd_marital_status IS NULL OR ci.cd_marital_status <> 'S')
ORDER BY 
    ai.ca_state,
    ci.c_last_name,
    si.total_net_profit DESC;
