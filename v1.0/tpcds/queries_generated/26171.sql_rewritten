WITH AddressDetails AS (
    SELECT 
        ca_address_sk,
        ca_city,
        ca_state,
        CONCAT(ca_street_number, ' ', ca_street_name, ' ', ca_street_type) AS full_address,
        TRIM(ca_street_number) AS trimmed_street_number,
        UPPER(ca_city) AS upper_city,
        LENGTH(ca_zip) AS zip_length
    FROM customer_address
    WHERE ca_country = 'USA'
),
DemoDetails AS (
    SELECT 
        cd_demo_sk,
        cd_gender,
        cd_marital_status,
        SUBSTRING(cd_education_status, 1, 10) AS short_education_status
    FROM customer_demographics
    WHERE cd_purchase_estimate > 1000
),
DateDetails AS (
    SELECT 
        d_date_sk,
        d_date,
        d_day_name,
        CASE 
            WHEN d_holiday = 'Y' THEN 'Holiday'
            ELSE 'Regular Day' 
        END AS day_type
    FROM date_dim
    WHERE d_year = 2001
),
SalesDetails AS (
    SELECT 
        ws_bill_customer_sk,
        SUM(ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws_order_number) AS order_count
    FROM web_sales
    GROUP BY ws_bill_customer_sk
)
SELECT 
    a.ca_address_sk,
    a.full_address,
    d.short_education_status,
    s.total_sales,
    s.order_count,
    CASE 
        WHEN g.cd_gender = 'M' THEN 'Male'
        WHEN g.cd_gender = 'F' THEN 'Female'
        ELSE 'Other'
    END AS gender_description,
    DATE_PART('YEAR', cast('2002-10-01' as date)) - EXTRACT(YEAR FROM c.c_birth_year) AS age
FROM AddressDetails a
JOIN DemoDetails g ON g.cd_demo_sk = c.c_current_cdemo_sk
JOIN DateDetails dt ON dt.d_date_sk = s.ws_bill_customer_sk
JOIN SalesDetails s ON s.ws_bill_customer_sk = c.c_customer_sk
WHERE a.trimmed_street_number IS NOT NULL
  AND a.zip_length = 5
  AND s.total_sales > 5000
ORDER BY total_sales DESC, a.ca_city;