
WITH SalesData AS (
    SELECT 
        ws.web_site_id,
        ws.ws_sold_date_sk,
        ws.ws_sales_price,
        ws.ws_quantity,
        cd.cd_gender,
        ca.ca_state,
        d.d_year,
        d.d_month_seq
    FROM web_sales ws
    JOIN customer c ON ws.ws_ship_customer_sk = c.c_customer_sk
    JOIN customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
    JOIN date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE d.d_year BETWEEN 2020 AND 2023
),
AggregatedSales AS (
    SELECT 
        web_site_id,
        d_year,
        d_month_seq,
        cd_gender,
        ca_state,
        SUM(ws_sales_price) AS total_sales,
        SUM(ws_quantity) AS total_quantity
    FROM SalesData
    GROUP BY web_site_id, d_year, d_month_seq, cd_gender, ca_state
),
RankedSales AS (
    SELECT 
        web_site_id,
        d_year,
        d_month_seq,
        cd_gender,
        ca_state,
        total_sales,
        total_quantity,
        RANK() OVER (PARTITION BY d_year, d_month_seq ORDER BY total_sales DESC) AS sales_rank
    FROM AggregatedSales
)
SELECT 
    web_site_id,
    d_year,
    d_month_seq,
    cd_gender,
    ca_state,
    total_sales,
    total_quantity,
    sales_rank
FROM RankedSales
WHERE sales_rank <= 10
ORDER BY d_year, d_month_seq, sales_rank;
