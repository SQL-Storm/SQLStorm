SELECT 
    CONCAT(c.c_first_name, ' ', c.c_last_name) AS full_name,
    c.c_email_address,
    ca.ca_street_number || ' ' || ca.ca_street_name || ' ' || ca.ca_street_type AS address,
    ca.ca_city,
    ca.ca_state,
    ca.ca_zip,
    cd.cd_gender,
    cd.cd_marital_status,
    DATE_FORMAT(cast('2002-10-01 12:34:56' as timestamp), '%Y-%m-%d') AS query_date,
    COUNT(DISTINCT ws.ws_order_number) AS total_orders,
    SUM(ws.ws_sales_price) AS total_spent,
    ROUND(AVG(ws.ws_sales_price), 2) AS average_order_value
FROM 
    customer c
JOIN 
    customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
JOIN 
    customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
LEFT JOIN 
    web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
WHERE 
    ca.ca_city LIKE '%Springfield%'
    AND cd.cd_gender = 'F'
GROUP BY 
    full_name, c.c_email_address, address, ca.ca_city, ca.ca_state, ca.ca_zip, cd.cd_gender, cd.cd_marital_status
HAVING 
    total_orders > 5
ORDER BY 
    total_spent DESC
LIMIT 100;