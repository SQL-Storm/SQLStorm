WITH ranked_sales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_order_number,
        ws.ws_quantity,
        ws.ws_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.ws_net_profit DESC) AS rank
    FROM 
        web_sales ws
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    WHERE 
        c.c_birth_year > 1980 AND 
        ws.ws_sold_date_sk BETWEEN 2459580 AND 2459587 
),
aggregate_data AS (
    SELECT 
        ra.web_site_sk,
        SUM(ra.ws_quantity) AS total_quantity,
        SUM(ra.ws_net_profit) AS total_net_profit
    FROM 
        ranked_sales ra
    WHERE 
        ra.rank <= 10
    GROUP BY 
        ra.web_site_sk
)
SELECT 
    wd.web_site_id,
    ad.total_quantity,
    ad.total_net_profit
FROM 
    aggregate_data ad
JOIN 
    web_site wd ON ad.web_site_sk = wd.web_site_sk
WHERE 
    ad.total_net_profit > 10000
ORDER BY 
    ad.total_net_profit DESC;