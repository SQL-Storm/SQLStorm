WITH RankedSales AS (
    SELECT 
        ws_item_sk,
        ws_order_number,
        ws_sales_price,
        ws_quantity,
        ws_net_profit,
        RANK() OVER (PARTITION BY ws_item_sk ORDER BY ws_net_profit DESC) AS profit_rank
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk BETWEEN 2451013 AND 2451346 
),
TopSellingItems AS (
    SELECT 
        is.i_item_id,
        ISNULL(reduction, 0) AS total_discount,
        SUM(rs.ws_sales_price * rs.ws_quantity) AS total_sales,
        SUM(rs.ws_net_profit) AS total_net_profit
    FROM 
        RankedSales rs
    JOIN 
        item is ON rs.ws_item_sk = is.i_item_sk
    LEFT JOIN (
        SELECT 
            p.p_item_sk,
            SUM(p.p_cost) AS reduction
        FROM 
            promotion p
        WHERE 
            p.p_discount_active = 'Y'
        GROUP BY 
            p.p_item_sk
    ) AS promotions ON is.i_item_sk = promotions.p_item_sk
    WHERE 
        rs.profit_rank <= 5
    GROUP BY 
        is.i_item_id, reduction
)
SELECT 
    tsi.i_item_id,
    tsi.total_sales,
    tsi.total_net_profit,
    CASE 
        WHEN tsi.total_discount IS NULL THEN 'No Discount'
        WHEN tsi.total_discount > 0 THEN 'Discount Applied'
        ELSE 'Non-Discounted Item'
    END AS discount_status
FROM 
    TopSellingItems tsi
JOIN 
    customer_address ca ON ca.ca_address_id = (SELECT c.c_current_addr_sk FROM customer c WHERE c.c_customer_sk = (SELECT nested_customer.sk FROM (SELECT DISTINCT ws_bill_customer_sk FROM web_sales) AS nested_customer))
WHERE 
    ca.ca_state = 'CA'
ORDER BY 
    tsi.total_net_profit DESC;