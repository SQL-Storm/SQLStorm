
WITH CustomerSales AS (
    SELECT 
        c.c_customer_id, 
        SUM(ws.ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        AVG(ws.ws_net_profit) AS average_net_profit
    FROM 
        customer c
    JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year = 2023
    AND 
        c.c_preferred_cust_flag = 'Y'
    GROUP BY 
        c.c_customer_id
),
CustomerDemographics AS (
    SELECT 
        cd.cd_demo_sk, 
        cd.cd_gender, 
        cd.cd_marital_status, 
        cd.cd_education_status, 
        cd.cd_income_band_sk,
        cs.total_sales,
        cs.order_count,
        cs.average_net_profit
    FROM 
        customer_demographics cd
    JOIN 
        CustomerSales cs ON cd.cd_demo_sk = (
            SELECT c.c_current_cdemo_sk FROM customer c WHERE c.c_customer_id = cs.c_customer_id
        )
)
SELECT 
    cd.cd_gender,
    cd.cd_marital_status,
    cd.cd_education_status,
    ib.ib_lower_bound,
    ib.ib_upper_bound,
    SUM(cd.total_sales) AS total_sales_by_demo,
    AVG(cd.average_net_profit) AS avg_net_profit_by_demo,
    COUNT(DISTINCT cs.order_count) AS total_orders_by_demo
FROM 
    CustomerDemographics cd
JOIN 
    income_band ib ON cd.cd_income_band_sk = ib.ib_income_band_sk
GROUP BY 
    cd.cd_gender, 
    cd.cd_marital_status, 
    cd.cd_education_status, 
    ib.ib_lower_bound, 
    ib.ib_upper_bound
ORDER BY 
    total_sales_by_demo DESC
FETCH FIRST 10 ROWS ONLY;
