
WITH SalesData AS (
    SELECT 
        ws.web_site_id,
        ws.ws_net_profit,
        c.c_gender,
        d.d_year,
        SUM(ws.ws_quantity) AS total_quantity,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders
    FROM 
        web_sales ws
    JOIN
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year BETWEEN 2020 AND 2023
    GROUP BY 
        ws.web_site_id, ws.ws_net_profit, c.c_gender, d.d_year
),
RankedSales AS (
    SELECT 
        web_site_id,
        c_gender,
        d_year,
        total_quantity,
        total_orders,
        ROW_NUMBER() OVER (PARTITION BY d_year ORDER BY total_profit DESC) AS rank
    FROM (
        SELECT 
            SalesData.*, 
            SUM(ws.ws_net_profit) OVER (PARTITION BY web_site_id, d_year) AS total_profit
        FROM 
            SalesData
    ) AS subquery
)
SELECT 
    web_site_id,
    c_gender,
    d_year,
    total_quantity,
    total_orders,
    rank
FROM 
    RankedSales
WHERE 
    rank <= 10
ORDER BY 
    d_year, rank;
