WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_item_sk,
        SUM(ws.ws_sales_price) AS total_sales,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_sales_price) DESC) AS sales_rank
    FROM web_sales ws
    JOIN item i ON ws.ws_item_sk = i.i_item_sk
    WHERE i.i_rec_start_date <= cast('2002-10-01' as date) AND (i.i_rec_end_date IS NULL OR i.i_rec_end_date > cast('2002-10-01' as date))
    GROUP BY ws.web_site_sk, ws.ws_item_sk
), 
BestSellingItems AS (
    SELECT web_site_sk, ws_item_sk, total_sales
    FROM RankedSales
    WHERE sales_rank <= 5
), 
CustomerInfo AS (
    SELECT 
        c.c_customer_sk,
        COUNT(DISTINCT cs.cs_order_number) AS total_orders,
        SUM(cs.cs_net_profit) AS total_net_profit,
        MAX(ws.ws_net_paid) AS max_web_sales
    FROM customer c
    LEFT JOIN web_sales ws ON c.c_customer_sk = ws.ws_ship_customer_sk
    LEFT JOIN catalog_sales cs ON c.c_customer_sk = cs.cs_ship_customer_sk
    GROUP BY c.c_customer_sk
), 
IncomeDistribution AS (
    SELECT 
        hd.hd_income_band_sk,
        SUM(CASE WHEN ci.total_orders > 10 THEN 1 ELSE 0 END) AS high_order_customers,
        COUNT(ci.c_customer_sk) AS total_customers
    FROM household_demographics hd
    LEFT JOIN CustomerInfo ci ON hd.hd_demo_sk = ci.c_customer_sk
    GROUP BY hd.hd_income_band_sk
)
SELECT 
    ib.ib_lower_bound,
    ib.ib_upper_bound,
    COALESCE(hd.high_order_customers, 0) AS high_order_customers,
    hd.total_customers,
    (COALESCE(hd.high_order_customers, 0) * 100.0 / NULLIF(hd.total_customers, 0)) AS high_order_percentage
FROM income_band ib
LEFT JOIN IncomeDistribution hd ON ib.ib_income_band_sk = hd.hd_income_band_sk
WHERE (ib.ib_lower_bound IS NOT NULL OR ib.ib_upper_bound IS NOT NULL)
ORDER BY ib.ib_lower_bound;