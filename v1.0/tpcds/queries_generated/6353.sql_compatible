
WITH sales_summary AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        SUM(ws.ws_sales_price) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        COUNT(DISTINCT wr.wr_order_number) AS web_returns,
        d.d_date AS sales_date
    FROM 
        customer AS c
    JOIN 
        web_sales AS ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    JOIN 
        date_dim AS d ON ws.ws_sold_date_sk = d.d_date_sk
    LEFT JOIN 
        web_returns AS wr ON ws.ws_order_number = wr.wr_order_number AND ws.ws_bill_customer_sk = wr.w_returning_customer_sk
    WHERE 
        d.d_year = 2023 AND 
        d.d_month = 10
    GROUP BY 
        c.c_customer_sk, c.c_first_name, c.c_last_name, d.d_date
),
top_customers AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        ss.total_sales,
        ss.order_count,
        ss.web_returns,
        RANK() OVER (ORDER BY ss.total_sales DESC) AS sales_rank
    FROM 
        sales_summary AS ss
    JOIN 
        customer AS c ON ss.c_customer_sk = c.c_customer_sk
)
SELECT 
    tc.c_customer_sk,
    tc.c_first_name,
    tc.c_last_name,
    tc.total_sales,
    tc.order_count,
    tc.web_returns,
    CASE 
        WHEN tc.sales_rank <= 10 THEN 'Top 10 Customer'
        ELSE 'Regular Customer'
    END AS customer_type
FROM 
    top_customers AS tc
WHERE 
    tc.sales_rank <= 100
ORDER BY 
    tc.total_sales DESC;
