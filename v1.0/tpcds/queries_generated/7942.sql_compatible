
WITH sales_summary AS (
    SELECT
        ws.web_site_id,
        SUM(ws.ws_net_profit) AS total_net_profit,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        AVG(ws.ws_net_paid) AS avg_order_value
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        income_band ib ON cd.cd_purchase_estimate BETWEEN ib.ib_lower_bound AND ib.ib_upper_bound
    WHERE 
        dd.d_year = 2023
        AND cd.cd_gender = 'F'
        AND ws.ws_ship_mode_sk IN (SELECT sm_ship_mode_sk FROM ship_mode WHERE sm_type LIKE '%Air%')
    GROUP BY 
        ws.web_site_id
),
warehouse_summary AS (
    SELECT 
        w.w_warehouse_id,
        SUM(inv.inv_quantity_on_hand) AS total_inventory,
        AVG(inv.inv_quantity_on_hand) AS avg_inventory
    FROM 
        inventory inv
    JOIN 
        warehouse w ON inv.inv_warehouse_sk = w.w_warehouse_sk
    GROUP BY 
        w.w_warehouse_id
)
SELECT 
    ss.web_site_id,
    ss.total_net_profit,
    ss.total_orders,
    ss.avg_order_value,
    ws.w_warehouse_id,
    ws.total_inventory,
    ws.avg_inventory
FROM 
    sales_summary ss
JOIN 
    warehouse_summary ws ON ss.total_net_profit > 10000 AND ws.total_inventory > 500
ORDER BY 
    ss.total_net_profit DESC, 
    ws.total_inventory ASC
FETCH FIRST 10 ROWS ONLY;
