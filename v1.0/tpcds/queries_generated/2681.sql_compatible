
WITH customer_stats AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        COUNT(DISTINCT wr.wr_return_number) AS total_web_returns,
        SUM(wr.wr_return_amt) AS total_web_return_amt,
        COUNT(DISTINCT sr.sr_ticket_number) AS total_store_returns,
        SUM(sr.sr_return_amt) AS total_store_return_amt,
        COUNT(DISTINCT ws.ws_order_number) AS total_web_sales,
        SUM(ws.ws_net_profit) AS total_web_net_profit
    FROM 
        customer c
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    LEFT JOIN 
        web_returns wr ON c.c_customer_sk = wr.wr_returning_customer_sk
    LEFT JOIN 
        store_returns sr ON c.c_customer_sk = sr.sr_customer_sk
    LEFT JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    GROUP BY 
        c.c_customer_sk, c.c_first_name, c.c_last_name, cd.cd_gender, cd.cd_marital_status
),
ranked_customers AS (
    SELECT 
        cs.*,
        ROW_NUMBER() OVER (PARTITION BY cs.cd_gender ORDER BY cs.total_web_net_profit DESC) AS profit_rank,
        DENSE_RANK() OVER (PARTITION BY cs.cd_marital_status ORDER BY cs.total_web_returns DESC) AS returns_rank
    FROM 
        customer_stats cs
)
SELECT 
    r.c_customer_sk,
    r.c_first_name,
    r.c_last_name,
    r.cd_gender,
    r.cd_marital_status,
    r.total_web_returns,
    r.total_web_return_amt,
    r.total_store_returns,
    (r.total_store_return_amt / NULLIF(r.total_web_sales, 0)) AS total_store_return_pct,
    r.profit_rank,
    r.returns_rank
FROM 
    ranked_customers r
WHERE 
    (r.cd_gender = 'F' AND r.profit_rank <= 10) 
    OR (r.cd_marital_status = 'M' AND r.returns_rank <= 5)
ORDER BY 
    r.total_web_net_profit DESC, r.total_web_returns DESC
LIMIT 50;
