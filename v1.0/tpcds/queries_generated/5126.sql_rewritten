WITH SalesData AS (
    SELECT 
        ws_bill_customer_sk,
        ws_ship_customer_sk,
        ws_item_sk,
        SUM(ws_quantity) AS total_quantity,
        SUM(ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws_order_number) AS order_count
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk BETWEEN 2458849 AND 2458918 
    GROUP BY 
        ws_bill_customer_sk, 
        ws_ship_customer_sk, 
        ws_item_sk
),
CustomerSegmentation AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status,
        ib.ib_income_band_sk,
        ib.ib_lower_bound,
        ib.ib_upper_bound
    FROM 
        customer AS c
    JOIN 
        customer_demographics AS cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    LEFT JOIN 
        household_demographics AS hd ON c.c_customer_sk = hd.hd_demo_sk
    LEFT JOIN 
        income_band AS ib ON hd.hd_income_band_sk = ib.ib_income_band_sk
),
TopItems AS (
    SELECT 
        sd.ws_item_sk,
        sd.total_quantity,
        RANK() OVER (ORDER BY sd.total_sales DESC) AS sales_rank
    FROM 
        SalesData sd
    WHERE 
        sd.total_quantity > 10
)
SELECT 
    cs.c_first_name,
    cs.c_last_name,
    cs.cd_gender,
    cs.cd_marital_status,
    ti.ws_item_sk,
    ti.total_quantity,
    ti.sales_rank,
    ib.ib_lower_bound,
    ib.ib_upper_bound
FROM 
    CustomerSegmentation cs
JOIN 
    TopItems ti ON cs.c_customer_sk = ti.ws_ship_customer_sk
JOIN 
    income_band ib ON cs.ib_income_band_sk = ib.ib_income_band_sk
WHERE 
    ti.sales_rank <= 5
ORDER BY 
    ti.sales_rank ASC, 
    cs.c_last_name;