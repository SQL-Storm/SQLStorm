WITH RankedSales AS (
    SELECT 
        ws.web_site_id,
        ws.ws_sold_date_sk,
        SUM(ws.ws_sales_price) AS total_sales,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws.ws_sales_price) DESC) AS sales_rank
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sold_date_sk BETWEEN (SELECT MAX(d_date_sk) FROM date_dim WHERE d_year = 2001) - 30 
        AND (SELECT MAX(d_date_sk) FROM date_dim WHERE d_year = 2001)
    GROUP BY 
        ws.web_site_id, 
        ws.ws_sold_date_sk
),
HighIncomeCustomers AS (
    SELECT 
        c.c_customer_id,
        h.hd_buy_potential,
        d.cd_marital_status,
        d.cd_gender
    FROM 
        customer c
    JOIN 
        household_demographics h ON c.c_customer_sk = h.hd_demo_sk
    JOIN 
        customer_demographics d ON c.c_current_cdemo_sk = d.cd_demo_sk
    WHERE 
        h.hd_income_band_sk = 1  
)
SELECT 
    r.sales_rank,
    r.web_site_id,
    r.total_sales,
    COUNT(DISTINCT hic.c_customer_id) AS customer_count,
    COALESCE(SUM(CASE WHEN hic.cd_marital_status = 'M' THEN 1 ELSE 0 END), 0) AS married_customers,
    COALESCE(SUM(CASE WHEN hic.cd_gender = 'F' THEN 1 ELSE 0 END), 0) AS female_customers
FROM 
    RankedSales r
LEFT JOIN 
    HighIncomeCustomers hic ON r.web_site_id = hic.c_customer_id  
GROUP BY 
    r.sales_rank, 
    r.web_site_id, 
    r.total_sales
HAVING 
    r.total_sales > (SELECT AVG(total_sales) FROM RankedSales)  
ORDER BY 
    r.total_sales DESC
LIMIT 10;