
WITH SalesData AS (
    SELECT 
        w.w_warehouse_id,
        w.w_city,
        SUM(ss_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ss_ticket_number) AS total_transactions,
        RANK() OVER (PARTITION BY w.w_city ORDER BY SUM(ss_ext_sales_price) DESC) AS sales_rank
    FROM 
        store_sales 
    JOIN 
        warehouse w ON store_sk = w.w_warehouse_sk
    WHERE 
        ss_sold_date_sk IN (SELECT d_date_sk FROM date_dim WHERE d_year = 2023 AND d_moy IN (5, 6))
    GROUP BY 
        w.w_warehouse_id, w.w_city
),
CustomerData AS (
    SELECT 
        c.c_customer_id,
        cd.cd_gender,
        SUM(ws_net_profit) AS total_net_profit
    FROM 
        customer c
    JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_marital_status = 'M' 
        AND cd.cd_gender IS NOT NULL
    GROUP BY 
        c.c_customer_id, cd.cd_gender
),
TopSales AS (
    SELECT 
        sd.w_warehouse_id,
        sd.w_city,
        sd.total_sales,
        sd.total_transactions,
        cd.total_net_profit,
        ROW_NUMBER() OVER (ORDER BY sd.total_sales DESC) AS rank
    FROM 
        SalesData sd
    LEFT JOIN 
        CustomerData cd ON sd.sales_rank = 1
    WHERE 
        sd.total_sales > (SELECT AVG(total_sales) FROM SalesData)
)
SELECT 
    ROW_NUMBER() OVER (ORDER BY COALESCE(ts.total_sales, 0) DESC) AS sales_rank,
    w.w_warehouse_id,
    w.w_city,
    COALESCE(ts.total_sales, 0) AS total_sales,
    COALESCE(ts.total_transactions, 0) AS total_transactions,
    COALESCE(ts.total_net_profit, 0) AS total_net_profit
FROM 
    warehouse w
LEFT JOIN 
    TopSales ts ON w.w_warehouse_id = ts.w_warehouse_id
WHERE 
    w.w_country = 'USA'
ORDER BY 
    total_sales DESC;
