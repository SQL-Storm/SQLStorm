WITH sales_summary AS (
    SELECT 
        ws.web_site_id,
        w.w_warehouse_name,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_net_paid_inc_tax) AS total_sales,
        AVG(ws.ws_net_profit) AS avg_net_profit,
        COUNT(DISTINCT ws.ws_order_number) AS order_count
    FROM 
        web_sales ws
    JOIN 
        web_site w ON ws.ws_web_site_sk = w.web_site_sk
    JOIN 
        warehouse wh ON ws.ws_warehouse_sk = wh.w_warehouse_sk
    WHERE 
        ws.ws_sold_date_sk BETWEEN 24120 AND 24150  
    GROUP BY 
        ws.web_site_id, w.w_warehouse_name
),
top_sales AS (
    SELECT 
        web_site_id,
        total_quantity,
        total_sales,
        avg_net_profit,
        order_count,
        RANK() OVER (ORDER BY total_sales DESC) AS sales_rank
    FROM 
        sales_summary
)
SELECT 
    ts.web_site_id,
    ts.total_quantity,
    ts.total_sales,
    ts.avg_net_profit,
    ts.order_count,
    cac.ca_city,
    cac.ca_state,
    cd.cd_gender,
    cd.cd_marital_status
FROM 
    top_sales ts
JOIN 
    customer c ON c.c_customer_sk = (SELECT ws.ws_bill_customer_sk FROM web_sales ws WHERE ws.ws_web_site_sk = ts.web_site_id LIMIT 1)
JOIN 
    customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
JOIN 
    customer_address cac ON c.c_current_addr_sk = cac.ca_address_sk
WHERE 
    ts.sales_rank <= 10  
ORDER BY 
    ts.total_sales DESC;