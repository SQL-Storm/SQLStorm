
WITH RankedSales AS (
    SELECT 
        ws.web_site_id,
        ws.ws_sold_date_sk,
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_sales_price) AS total_sales,
        RANK() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws.ws_sales_price) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        item i ON ws.ws_item_sk = i.i_item_sk
    JOIN 
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    WHERE 
        c.c_birth_year BETWEEN 1980 AND 1990 
        AND i.i_current_price BETWEEN 10.00 AND 100.00
    GROUP BY 
        ws.web_site_id, ws.ws_sold_date_sk, ws.ws_item_sk
),
TopSales AS (
    SELECT 
        web_site_id,
        ws_sold_date_sk,
        ws_item_sk,
        total_quantity,
        total_sales
    FROM 
        RankedSales
    WHERE 
        sales_rank <= 5
)
SELECT 
    ts.web_site_id,
    ts.ws_sold_date_sk,
    i.i_item_desc,
    ts.total_quantity,
    ts.total_sales,
    c.c_first_name,
    c.c_last_name,
    SUM(ws.ws_ext_tax) AS total_tax,
    SUM(ws.ws_net_profit) AS total_profit
FROM 
    TopSales ts
JOIN 
    web_sales ws ON ts.ws_item_sk = ws.ws_item_sk AND ts.ws_sold_date_sk = ws.ws_sold_date_sk
JOIN 
    item i ON ts.ws_item_sk = i.i_item_sk
JOIN 
    customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
JOIN 
    date_dim d ON ts.ws_sold_date_sk = d.d_date_sk
WHERE 
    d.d_year = 2023
GROUP BY 
    ts.web_site_id, ts.ws_sold_date_sk, i.i_item_desc, ts.total_quantity, ts.total_sales, c.c_first_name, c.c_last_name
ORDER BY 
    ts.total_sales DESC;
