
WITH ranked_sales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_order_number,
        SUM(ws.ws_net_profit) AS total_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_net_profit) DESC) AS rank
    FROM 
        web_sales ws
    INNER JOIN 
        time_dim td ON ws.ws_sold_date_sk = td.d_date_sk
    GROUP BY 
        ws.web_site_sk, ws.ws_order_number
),
web_customer_details AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        c.c_email_address,
        cd.cd_gender,
        cd.cd_marital_status,
        rank.total_net_profit
    FROM 
        customer c
    LEFT JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    LEFT JOIN 
        ranked_sales rank ON c.c_customer_sk = rank.ws_order_number
    WHERE 
        cd.cd_marital_status IN ('S', 'M') 
        AND rank.total_net_profit IS NOT NULL
        AND c.c_email_address IS NOT NULL
),
item_summary AS (
    SELECT 
        i.i_item_sk,
        i.i_item_id,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        AVG(ws.ws_sales_price) AS avg_sales_price
    FROM 
        item i
    JOIN 
        web_sales ws ON i.i_item_sk = ws.ws_item_sk
    GROUP BY 
        i.i_item_sk, i.i_item_id
    HAVING 
        AVG(ws.ws_sales_price) > (
            SELECT 
                AVG(ws2.ws_sales_price) * 1.1 
            FROM 
                web_sales ws2 
            WHERE 
                ws2.ws_sold_date_sk = (
                    SELECT MAX(td.d_date_sk) FROM time_dim td WHERE td.d_year = 2022)
        )
),
final_results AS (
    SELECT 
        wcd.c_first_name,
        wcd.c_last_name,
        SUM(iss.avg_sales_price) AS total_avg_sales_price,
        COUNT(DISTINCT iss.order_count) AS unique_items_purchased
    FROM 
        web_customer_details wcd
    LEFT JOIN 
        item_summary iss ON wcd.c_customer_sk = iss.i_item_sk
    GROUP BY 
        wcd.c_first_name, wcd.c_last_name
)
SELECT 
    COALESCE(f.c_first_name, 'Unknown') AS first_name,
    COALESCE(f.c_last_name, 'Unknown') AS last_name,
    COALESCE(f.total_avg_sales_price, 0) AS total_avg_sales_price,
    COALESCE(f.unique_items_purchased, 0) AS unique_items_purchased
FROM 
    final_results f
FULL OUTER JOIN 
    customer c ON f.first_name = c.c_first_name AND f.last_name = c.c_last_name
WHERE 
    f.total_avg_sales_price IS NOT NULL 
    AND f.unique_items_purchased < (SELECT COUNT(*) FROM item) * 0.1
ORDER BY 
    total_avg_sales_price DESC NULLS LAST;
