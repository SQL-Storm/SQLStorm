
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.web_site_id,
        SUM(ws.ws_net_profit) AS total_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_net_profit) DESC) AS rank
    FROM web_sales ws
    JOIN date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE dd.d_year = 2023
    GROUP BY ws.web_site_sk, ws.web_site_id
),
TopSites AS (
    SELECT 
        r.web_site_sk,
        r.web_site_id
    FROM RankedSales r
    WHERE r.rank <= 5
)
SELECT 
    ca.ca_city,
    ca.ca_state,
    COUNT(DISTINCT c.c_customer_sk) AS num_customers,
    SUM(ws.ws_quantity) AS total_quantity_sold,
    AVG(ws.ws_net_paid) AS avg_net_paid
FROM customer_address ca
LEFT JOIN customer c ON ca.ca_address_sk = c.c_current_addr_sk
LEFT JOIN web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
LEFT JOIN TopSites ts ON ws.ws_web_site_sk = ts.web_site_sk
WHERE ca.ca_state IN ('CA', 'NY')
GROUP BY ca.ca_city, ca.ca_state
HAVING SUM(ws.ws_net_profit) IS NOT NULL
ORDER BY num_customers DESC, total_quantity_sold DESC
FETCH FIRST 10 ROWS ONLY;
