
WITH SalesData AS (
    SELECT 
        ws.ws_item_sk,
        ws.ws_order_number,
        ws.ws_quantity,
        ws.ws_sales_price,
        ws.ws_ext_sales_price,
        c.c_gender,
        c.c_birth_year,
        d.d_year,
        w.w_warehouse_name,
        sm.sm_type
    FROM 
        web_sales AS ws
    JOIN 
        customer AS c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN 
        date_dim AS d ON ws.ws_sold_date_sk = d.d_date_sk
    JOIN 
        warehouse AS w ON ws.ws_warehouse_sk = w.w_warehouse_sk
    JOIN 
        ship_mode AS sm ON ws.ws_ship_mode_sk = sm.sm_ship_mode_sk
    WHERE 
        d.d_year BETWEEN 2020 AND 2023
),
AggregatedSales AS (
    SELECT 
        sd.c_gender,
        sd.c_birth_year,
        sd.d_year,
        SUM(sd.ws_quantity) AS total_quantity,
        SUM(sd.ws_ext_sales_price) AS total_sales,
        AVG(sd.ws_sales_price) AS average_price,
        COUNT(DISTINCT sd.ws_order_number) AS distinct_orders
    FROM 
        SalesData AS sd
    GROUP BY 
        sd.c_gender, sd.c_birth_year, sd.d_year
)
SELECT 
    ag.c_gender,
    ag.c_birth_year,
    ag.d_year,
    ag.total_quantity,
    ag.total_sales,
    ag.average_price,
    ag.distinct_orders,
    RANK() OVER (PARTITION BY ag.d_year ORDER BY ag.total_sales DESC) AS sales_rank
FROM 
    AggregatedSales AS ag
WHERE 
    ag.total_sales > 1000
ORDER BY 
    ag.d_year, sales_rank;
