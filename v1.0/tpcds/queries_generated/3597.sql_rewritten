WITH RankedSales AS (
    SELECT 
        ws_item_sk,
        ws_order_number,
        ws_ext_sales_price,
        RANK() OVER (PARTITION BY ws_item_sk ORDER BY ws_ext_sales_price DESC) AS sales_rank
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk BETWEEN (SELECT d_date_sk FROM date_dim WHERE d_date = cast('2002-10-01' as date) - INTERVAL '30 days')
        AND (SELECT d_date_sk FROM date_dim WHERE d_date = cast('2002-10-01' as date))
),
TopSales AS (
    SELECT 
        rs.ws_item_sk,
        rs.ws_order_number,
        rs.ws_ext_sales_price
    FROM 
        RankedSales rs
    WHERE 
        rs.sales_rank = 1
),
CustomerInfo AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
ItemDetails AS (
    SELECT 
        i.i_item_sk,
        i.i_item_desc,
        i.i_current_price
    FROM 
        item i
)
SELECT 
    ci.c_first_name,
    ci.c_last_name,
    ci.cd_gender,
    ci.cd_marital_status,
    id.i_item_desc,
    ts.ws_ext_sales_price,
    (ts.ws_ext_sales_price - id.i_current_price) AS profit_margin,
    (SELECT COUNT(DISTINCT sr.ticket_number) 
     FROM store_returns sr 
     WHERE sr.sr_item_sk = ts.ws_item_sk) AS returns_count
FROM 
    TopSales ts
JOIN 
    CustomerInfo ci ON ts.ws_order_number = (SELECT ws_order_number FROM web_sales WHERE ws_item_sk = ts.ws_item_sk LIMIT 1)
JOIN 
    ItemDetails id ON ts.ws_item_sk = id.i_item_sk
WHERE 
    profit_margin > 0
ORDER BY 
    profit_margin DESC
LIMIT 100
OFFSET 50;