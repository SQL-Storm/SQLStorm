
WITH RankedSales AS (
    SELECT
        w.w_warehouse_id,
        s.s_store_id,
        cs.cs_order_number,
        SUM(cs.cs_quantity) AS total_quantity,
        SUM(cs.cs_ext_sales_price) AS total_sales,
        ROW_NUMBER() OVER (PARTITION BY w.w_warehouse_id ORDER BY SUM(cs.cs_ext_sales_price) DESC) AS sales_rank
    FROM
        catalog_sales cs
    JOIN
        warehouse w ON cs.cs_warehouse_sk = w.w_warehouse_sk
    JOIN
        store s ON cs.cs_ship_addr_sk = s.s_addr_sk
    WHERE
        cs.cs_sold_date_sk BETWEEN 2451545 AND 2451546 
    GROUP BY
        w.w_warehouse_id, s.s_store_id, cs.cs_order_number
),
TopSales AS (
    SELECT
        w_warehouse_id AS warehouse_id,
        s_store_id AS store_id,
        total_quantity,
        total_sales
    FROM
        RankedSales
    WHERE
        sales_rank <= 10
)
SELECT
    ts.warehouse_id,
    ts.store_id,
    ts.total_quantity,
    ts.total_sales,
    c.c_first_name,
    c.c_last_name,
    cd.cd_gender,
    cd.cd_marital_status
FROM
    TopSales ts
JOIN
    web_sales ws ON ts.cs_order_number = ws.ws_order_number
JOIN
    customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
JOIN
    customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
ORDER BY
    ts.total_sales DESC;
