
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_item_sk,
        ws.ws_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.ws_net_profit DESC) AS profit_rank
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sold_date_sk BETWEEN 2450000 AND 2450500
),
TopProfitWebsites AS (
    SELECT 
        wr.web_site_id,
        wr.web_name,
        SUM(rs.ws_net_profit) AS total_profit
    FROM 
        web_site wr
    JOIN 
        RankedSales rs ON wr.web_site_sk = rs.web_site_sk
    WHERE 
        rs.profit_rank <= 10
    GROUP BY 
        wr.web_site_id, wr.web_name
),
CustomerDemographics AS (
    SELECT 
        cd.cd_demo_sk,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_purchase_estimate,
        COALESCE(ib.ib_lower_bound, 0) AS lower_income,
        COALESCE(ib.ib_upper_bound, 1000000) AS upper_income
    FROM 
        customer_demographics cd
    LEFT JOIN 
        household_demographics hd ON cd.cd_demo_sk = hd.hd_demo_sk
    LEFT JOIN 
        income_band ib ON hd.hd_income_band_sk = ib.ib_income_band_sk
)
SELECT 
    c.c_customer_id,
    c.c_first_name,
    c.c_last_name,
    cd.cd_gender,
    cd.cd_marital_status,
    cd.lower_income,
    cd.upper_income,
    tw.web_name,
    tw.total_profit
FROM 
    customer c
JOIN 
    CustomerDemographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
JOIN 
    TopProfitWebsites tw ON cd.cd_purchase_estimate BETWEEN tw.lower_income AND tw.upper_income
WHERE 
    c.c_birth_year BETWEEN 1980 AND 2000
    AND cd.cd_gender IS NOT NULL
UNION ALL
SELECT 
    'Aggregate' AS c_customer_id,
    NULL AS c_first_name,
    NULL AS c_last_name,
    NULL AS cd_gender,
    NULL AS cd_marital_status,
    NULL AS lower_income,
    NULL AS upper_income,
    tw.web_name,
    SUM(tw.total_profit) AS total_profit
FROM 
    TopProfitWebsites tw
GROUP BY 
    tw.web_name
ORDER BY 
    total_profit DESC;
