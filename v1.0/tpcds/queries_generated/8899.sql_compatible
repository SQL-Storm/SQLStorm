
WITH sales_data AS (
    SELECT 
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        SUM(ws.ws_ext_discount_amt) AS total_discount,
        MAX(d.d_year) AS max_year,
        AVG(c.c_birth_year) AS avg_birth_year
    FROM 
        web_sales ws
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    JOIN 
        customer c ON ws.ws_ship_customer_sk = c.c_customer_sk
    WHERE 
        d.d_year BETWEEN 2020 AND 2022
    GROUP BY 
        ws.ws_item_sk
),
customer_demographics AS (
    SELECT 
        cd.cd_gender,
        COUNT(DISTINCT c.c_customer_sk) AS customer_count,
        SUM(cd.cd_dep_count) AS total_dependents,
        AVG(cd.cd_purchase_estimate) AS avg_purchase_estimate
    FROM 
        customer_demographics cd
    JOIN 
        customer c ON cd.cd_demo_sk = c.c_current_cdemo_sk
    GROUP BY 
        cd.cd_gender
),
final_report AS (
    SELECT 
        sd.ws_item_sk,
        sd.total_quantity,
        sd.total_sales,
        sd.total_discount,
        cd.cd_gender,
        cd.customer_count,
        cd.total_dependents,
        cd.avg_purchase_estimate,
        sd.max_year,
        sd.avg_birth_year
    FROM 
        sales_data sd
    JOIN 
        customer_demographics cd ON cd.customer_count > 10
)
SELECT 
    fr.ws_item_sk,
    fr.total_quantity,
    fr.total_sales,
    fr.total_discount,
    fr.cd_gender,
    fr.customer_count,
    fr.total_dependents,
    fr.avg_purchase_estimate,
    fr.max_year,
    fr.avg_birth_year
FROM 
    final_report fr
ORDER BY 
    fr.total_sales DESC, 
    fr.total_quantity DESC
FETCH FIRST 100 ROWS ONLY;
