
WITH CustomerInfo AS (
    SELECT 
        c.c_customer_id, 
        CONCAT(c.c_salutation, ' ', c.c_first_name, ' ', c.c_last_name) AS full_name, 
        ca.ca_city, 
        ca.ca_state, 
        cd.cd_gender, 
        cd.cd_marital_status,
        LTRIM(RTRIM(cd.cd_education_status)) AS education_status 
    FROM 
        customer c
    JOIN 
        customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
SalesInfo AS (
    SELECT 
        ws.ws_order_number, 
        ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity, 
        SUM(ws.ws_net_paid) AS total_paid,
        COUNT(DISTINCT ws.ws_bill_customer_sk) AS unique_customers
    FROM 
        web_sales ws
    GROUP BY 
        ws.ws_order_number, ws_item_sk
),
AggregateSales AS (
    SELECT 
        si.full_name,
        si.ca_city,
        si.ca_state,
        si.cd_gender,
        si.cd_marital_status,
        si.education_status,
        SUM(sale.total_paid) AS total_spent,
        SUM(sale.total_quantity) AS total_purchased
    FROM 
        CustomerInfo si
    JOIN 
        SalesInfo sale ON si.c_customer_id = sale.ws_order_number
    GROUP BY 
        si.full_name, si.ca_city, si.ca_state, si.cd_gender, si.cd_marital_status, si.education_status
)
SELECT 
    full_name, 
    ca_city, 
    ca_state, 
    cd_gender, 
    cd_marital_status,
    education_status,
    total_spent,
    total_purchased,
    CASE 
        WHEN total_spent >= 1000 THEN 'High Value'
        WHEN total_spent >= 500 THEN 'Medium Value'
        ELSE 'Low Value'
    END AS customer_value
FROM 
    AggregateSales
WHERE 
    cd_gender = 'F' 
    AND ca_state = 'CA'
ORDER BY 
    total_spent DESC 
LIMIT 100;
