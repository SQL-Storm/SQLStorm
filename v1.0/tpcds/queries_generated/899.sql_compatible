
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws_order_number,
        SUM(ws.net_profit) AS total_net_profit,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.net_profit) DESC) AS rank
    FROM 
        web_sales ws
    INNER JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023 
        AND dd.d_moy IN (1, 2, 3) 
    GROUP BY 
        ws.web_site_sk, ws_order_number
),
CustomerReturnStats AS (
    SELECT 
        c.c_customer_sk,
        COUNT(DISTINCT wr.wr_order_number) AS total_web_returns,
        SUM(wr.wr_return_amt) AS total_web_return_amount
    FROM 
        web_returns wr
    LEFT JOIN 
        customer c ON wr.wr_returning_customer_sk = c.c_customer_sk
    GROUP BY 
        c.c_customer_sk
),
SalesAnalysis AS (
    SELECT 
        ca.ca_state,
        COUNT(DISTINCT cs.cs_order_number) AS total_catalog_sales,
        SUM(cs.cs_net_profit) AS total_profit,
        AVG(cs.cs_sales_price) AS avg_sales_price
    FROM 
        catalog_sales cs
    INNER JOIN 
        customer_address ca ON cs.cs_bill_addr_sk = ca.ca_address_sk
    WHERE 
        ca.ca_state IS NOT NULL
    GROUP BY 
        ca.ca_state
)
SELECT 
    r.rank,
    s.ca_state,
    s.total_catalog_sales,
    s.total_profit,
    s.avg_sales_price,
    c.total_web_returns,
    c.total_web_return_amount,
    COALESCE(r.total_net_profit, 0) AS total_net_profit
FROM 
    SalesAnalysis s
LEFT JOIN 
    CustomerReturnStats c ON c.c_customer_sk IN (
        SELECT 
            c.c_customer_sk 
        FROM 
            customer c 
        INNER JOIN RankedSales r ON c.c_current_addr_sk = r.web_site_sk
    )
LEFT JOIN 
    RankedSales r ON r.web_site_sk IN (
        SELECT 
            rs.web_site_sk 
        FROM 
            RankedSales rs 
        WHERE 
            rs.rank = 1
    )
WHERE 
    s.total_catalog_sales > 1000
ORDER BY 
    s.total_profit DESC;
