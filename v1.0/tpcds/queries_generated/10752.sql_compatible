
WITH sales_summary AS (
    SELECT 
        ws_web_site_sk,
        SUM(ws_sales_price) AS total_sales,
        COUNT(ws_order_number) AS order_count
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk >= 2458850  
    GROUP BY 
        ws_web_site_sk
)
SELECT 
    w.warehouse_id,
    w.warehouse_name,
    ss.total_sales,
    ss.order_count
FROM 
    warehouse w
JOIN 
    sales_summary ss ON w.warehouse_id = ss.ws_web_site_sk
ORDER BY 
    ss.total_sales DESC;
