
WITH customer_sales AS (
    SELECT 
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        COUNT(ws.ws_order_number) AS order_count,
        cd.cd_gender,
        cd.cd_marital_status,
        hd.hd_income_band_sk
    FROM 
        customer c
    JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        household_demographics hd ON c.c_current_hdemo_sk = hd.hd_demo_sk
    GROUP BY 
        c.c_customer_sk, c.c_first_name, c.c_last_name, cd.cd_gender, cd.cd_marital_status, hd.hd_income_band_sk
),
sales_summary AS (
    SELECT 
        cs.c_customer_sk,
        cs.c_first_name,
        cs.c_last_name,
        cs.total_sales,
        cs.order_count,
        CASE 
            WHEN cs.total_sales > 1000 THEN 'High Value'
            WHEN cs.total_sales BETWEEN 500 AND 1000 THEN 'Medium Value'
            ELSE 'Low Value'
        END AS value_category,
        cs.cd_gender,
        cs.cd_marital_status,
        ib.ib_income_band_sk
    FROM 
        customer_sales cs
    LEFT JOIN 
        income_band ib ON cs.hd_income_band_sk = ib.ib_income_band_sk
)
SELECT 
    s.c_first_name,
    s.c_last_name,
    s.order_count,
    s.total_sales,
    s.value_category,
    s.cd_gender,
    s.cd_marital_status,
    ib.ib_lower_bound,
    ib.ib_upper_bound
FROM 
    sales_summary s
JOIN 
    income_band ib ON s.cd_income_band_sk = ib.ib_income_band_sk
WHERE 
    s.order_count > 5 
    AND s.total_sales > 500 
ORDER BY 
    s.total_sales DESC
LIMIT 100;
