
SELECT c.c_customer_id, COUNT(DISTINCT ss.ticket_number) AS total_sales, SUM(ss.ext_sales_price) AS total_revenue
FROM customer c
JOIN store_sales ss ON c.c_customer_sk = ss.ss_customer_sk
JOIN date_dim d ON ss.sold_date_sk = d.d_date_sk
WHERE d.d_year = 2023 AND c.c_preferred_cust_flag = 'Y'
GROUP BY c.c_customer_id
ORDER BY total_revenue DESC
FETCH FIRST 10 ROWS ONLY;
