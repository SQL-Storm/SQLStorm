
WITH sales_data AS (
    SELECT 
        ws.web_site_id,
        ws.ws_order_number,
        ws.ws_sold_date_sk,
        SUM(ws.ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws.ws_ship_customer_sk) AS unique_customers,
        AVG(ws.ws_net_paid) AS avg_net_paid,
        COUNT(DISTINCT ws.ws_item_sk) AS items_sold
    FROM 
        web_sales ws
    INNER JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    INNER JOIN 
        promotion p ON ws.ws_promo_sk = p.p_promo_sk
    WHERE 
        dd.d_year = 2023
        AND p.p_discount_active = 'Y'
    GROUP BY 
        ws.web_site_id, ws.ws_order_number, ws.ws_sold_date_sk
),
customer_data AS (
    SELECT 
        c.c_customer_id,
        cd.cd_gender,
        cd.cd_income_band_sk,
        SUM(sd.total_profit) AS total_profit,
        COUNT(sd.ws_order_number) AS order_count
    FROM 
        sales_data sd
    INNER JOIN 
        customer c ON sd.unique_customers = c.c_customer_sk
    INNER JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    GROUP BY 
        c.c_customer_id, cd.cd_gender, cd.cd_income_band_sk
),
income_category AS (
    SELECT 
        ib.ib_income_band_sk,
        COUNT(*) AS customers_count,
        SUM(cd.total_profit) AS overall_profit
    FROM 
        customer_data cd
    LEFT JOIN 
        income_band ib ON cd.cd_income_band_sk = ib.ib_income_band_sk
    GROUP BY 
        ib.ib_income_band_sk
)
SELECT 
    ib.ib_income_band_sk,
    ib.ib_lower_bound,
    ib.ib_upper_bound,
    ic.customers_count,
    ic.overall_profit,
    (ic.overall_profit / NULLIF(ic.customers_count, 0)) AS avg_profit_per_customer
FROM 
    income_band ib
LEFT JOIN 
    income_category ic ON ib.ib_income_band_sk = ic.ib_income_band_sk
ORDER BY 
    ib.ib_income_band_sk;
