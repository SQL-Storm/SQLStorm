
WITH RankedSales AS (
    SELECT 
        ws.web_site_id,
        COUNT(DISTINCT ws_order_number) AS total_orders,
        SUM(ws_ext_sales_price) AS total_sales,
        DENSE_RANK() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws_ext_sales_price) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        web_site w ON ws.ws_web_site_sk = w.web_site_sk
    JOIN 
        date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE 
        d.d_year = 2022 
        AND d.d_moy IN (11, 12) 
    GROUP BY 
        ws.web_site_id
),
TopWebsites AS (
    SELECT 
        web_site_id, 
        total_orders, 
        total_sales
    FROM 
        RankedSales
    WHERE 
        sales_rank <= 5
)

SELECT 
    t.web_site_id,
    t.total_orders,
    t.total_sales,
    c.c_first_name,
    c.c_last_name,
    SUM(ws.ws_quantity) AS total_items_sold,
    AVG(ws.ws_net_profit) AS average_profit
FROM 
    TopWebsites t
JOIN 
    web_sales ws ON t.web_site_id = ws.ws_web_site_sk
JOIN 
    customer c ON ws.ws_ship_customer_sk = c.c_customer_sk
WHERE 
    c.c_birth_year BETWEEN 1980 AND 1990
GROUP BY 
    t.web_site_id, 
    t.total_orders, 
    t.total_sales, 
    c.c_first_name, 
    c.c_last_name
ORDER BY 
    t.total_sales DESC, 
    total_items_sold DESC;
