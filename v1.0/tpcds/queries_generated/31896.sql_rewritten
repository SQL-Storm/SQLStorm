WITH RECURSIVE SalesSummary AS (
    SELECT 
        w.warehouse_sk,
        w.warehouse_name,
        SUM(ws.ws_net_profit) AS total_net_profit,
        ROW_NUMBER() OVER (PARTITION BY w.warehouse_sk ORDER BY SUM(ws.ws_net_profit) DESC) AS rank
    FROM 
        warehouse w
    JOIN 
        web_sales ws ON w.warehouse_sk = ws.ws_warehouse_sk
    GROUP BY 
        w.warehouse_sk, w.warehouse_name
), 
CustomerReturns AS (
    SELECT 
        c.c_customer_id,
        COALESCE(SUM(sr_return_amt), 0) AS total_return_amount,
        COUNT(sr_ticket_number) AS total_returns,
        ROW_NUMBER() OVER (ORDER BY COALESCE(SUM(sr_return_amt), 0) DESC) AS return_rank
    FROM 
        customer c
    LEFT JOIN 
        store_returns sr ON c.c_customer_sk = sr.sr_customer_sk
    GROUP BY 
        c.c_customer_id
), 
MonthlySales AS (
    SELECT 
        d.d_year,
        d.d_month_seq AS month_seq,
        SUM(ws.ws_net_sales) AS total_sales,
        COUNT(ws.ws_order_number) AS total_orders
    FROM 
        date_dim d
    JOIN 
        web_sales ws ON d.d_date_sk = ws.ws_sold_date_sk
    GROUP BY 
        d.d_year, d.d_month_seq
),
TopCustomers AS (
    SELECT 
        c.c_customer_id,
        SUM(ws.ws_net_paid) AS total_spent,
        ROW_NUMBER() OVER (ORDER BY SUM(ws.ws_net_paid) DESC) AS customer_rank
    FROM 
        customer c
    JOIN 
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    GROUP BY 
        c.c_customer_id
)

SELECT 
    s.warehouse_name,
    ss.total_net_profit,
    cr.c_customer_id,
    cr.total_return_amount,
    cr.total_returns,
    ms.total_sales,
    ms.total_orders,
    tc.total_spent
FROM 
    SalesSummary ss
JOIN 
    CustomerReturns cr ON cr.return_rank = 1
JOIN 
    MonthlySales ms ON ms.month_seq = 1 AND ms.d_year = 2001 
JOIN 
    TopCustomers tc ON tc.customer_rank = 1
LEFT JOIN 
    warehouse s ON s.warehouse_sk = ss.warehouse_sk
WHERE 
    ss.total_net_profit > 10000 
ORDER BY 
    ss.total_net_profit DESC, cr.total_return_amount DESC;