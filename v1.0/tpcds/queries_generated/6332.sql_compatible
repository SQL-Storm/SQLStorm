
WITH sales_summary AS (
    SELECT
        ws_item_sk,
        SUM(ws_quantity) AS total_quantity,
        SUM(ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws_order_number) AS total_orders
    FROM
        web_sales
    WHERE
        ws_sold_date_sk BETWEEN 2458931 AND 2458991 
    GROUP BY
        ws_item_sk
),
customer_data AS (
    SELECT
        c.c_customer_sk,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_income_band_sk,
        ca.ca_state
    FROM
        customer c
    JOIN
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN
        customer_address ca ON c.c_current_addr_sk = ca.ca_address_sk
),
income_distribution AS (
    SELECT
        ib.ib_income_band_sk,
        COUNT(hd.hd_demo_sk) AS customer_count
    FROM
        household_demographics hd
    JOIN
        income_band ib ON hd.hd_income_band_sk = ib.ib_income_band_sk
    JOIN
        customer_data cd ON hd.hd_demo_sk = cd.c_customer_sk
    GROUP BY
        ib.ib_income_band_sk
)
SELECT
    i.i_item_id,
    i.i_item_desc,
    ss.total_quantity,
    ss.total_sales,
    cd.cd_gender,
    cd.cd_marital_status,
    id.ib_income_band_sk,
    id.customer_count
FROM
    sales_summary ss
JOIN
    item i ON ss.ws_item_sk = i.i_item_sk
JOIN
    customer_data cd ON cd.c_customer_sk IN (
        SELECT
            c_customer_sk
        FROM
            customer
        WHERE
            c_current_addr_sk IN (
                SELECT 
                    ca_address_sk 
                FROM 
                    customer_address 
                WHERE 
                    ca_state = 'CA'
            )
    )
JOIN
    income_distribution id ON cd.cd_income_band_sk = id.ib_income_band_sk
ORDER BY
    ss.total_sales DESC
LIMIT 10;
