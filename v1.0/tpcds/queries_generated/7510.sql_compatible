
WITH RankedSales AS (
    SELECT 
        ws.ws_item_sk,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_net_profit) AS total_profit,
        RANK() OVER (PARTITION BY ws.ws_item_sk ORDER BY SUM(ws.ws_net_profit) DESC) AS profit_rank
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023
    GROUP BY 
        ws.ws_item_sk
),
TopItems AS (
    SELECT 
        ri.ws_item_sk,
        i.i_item_desc,
        ri.total_quantity,
        ri.total_profit
    FROM 
        RankedSales ri
    JOIN 
        item i ON ri.ws_item_sk = i.i_item_sk
    WHERE 
        ri.profit_rank <= 10
)
SELECT 
    ti.i_item_desc,
    ti.total_quantity,
    ti.total_profit,
    c.c_first_name,
    c.c_last_name,
    cs.cs_sold_date_sk,
    cs.cs_order_number,
    cs.cs_net_profit
FROM 
    TopItems ti
JOIN 
    store_sales cs ON ti.ws_item_sk = cs.ss_item_sk
JOIN 
    customer c ON cs.ss_customer_sk = c.c_customer_sk
WHERE 
    cs.cs_sold_date_sk BETWEEN 20230101 AND 20231231
ORDER BY 
    ti.total_profit DESC, ti.total_quantity DESC
FETCH FIRST 100 ROWS ONLY;
