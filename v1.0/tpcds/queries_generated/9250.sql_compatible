
WITH SalesData AS (
    SELECT 
        ws.ws_sold_date_sk,
        dd.d_year,
        dd.d_month_seq,
        sd.s_store_name,
        SUM(ws.ws_sales_price) AS total_sales,
        SUM(ws.ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws.ws_order_number) AS order_count
    FROM 
        web_sales AS ws
    JOIN 
        date_dim AS dd ON ws.ws_sold_date_sk = dd.d_date_sk
    JOIN 
        store AS sd ON ws.ws_ship_addr_sk = sd.s_addr_sk
    WHERE 
        dd.d_year = 2023
    GROUP BY 
        ws.ws_sold_date_sk,
        dd.d_year,
        dd.d_month_seq,
        sd.s_store_name
),
RankedSales AS (
    SELECT 
        dd.d_year,
        dd.d_month_seq,
        sd.s_store_name,
        total_sales,
        total_profit,
        order_count,
        RANK() OVER (PARTITION BY dd.d_year, dd.d_month_seq ORDER BY total_sales DESC) AS sales_rank
    FROM 
        SalesData
)
SELECT 
    d_year,
    d_month_seq,
    s_store_name,
    total_sales,
    total_profit,
    order_count
FROM 
    RankedSales
WHERE 
    sales_rank <= 5
ORDER BY 
    d_year DESC, d_month_seq DESC, total_sales DESC;
