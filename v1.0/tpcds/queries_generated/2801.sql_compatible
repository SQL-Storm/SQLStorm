
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_order_number,
        ws.ws_sales_price,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.ws_sales_price DESC) AS rank
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sold_date_sk BETWEEN 
            (SELECT MAX(d_date_sk) FROM date_dim WHERE d_year = 2023) - 30 AND 
            (SELECT MAX(d_date_sk) FROM date_dim WHERE d_year = 2023)
),
TotalSales AS (
    SELECT 
        w.w_warehouse_name,
        SUM(r.ws_sales_price) AS total_sales,
        COUNT(DISTINCT r.ws_order_number) AS total_orders,
        AVG(r.ws_sales_price) AS avg_sale_price
    FROM 
        RankedSales r
    JOIN 
        warehouse w ON w.w_warehouse_sk = (
            SELECT ws.ws_warehouse_sk FROM web_sales ws WHERE ws.ws_order_number = r.ws_order_number LIMIT 1
        )
    WHERE 
        r.rank <= 10
    GROUP BY 
        w.w_warehouse_name
),
CustomerInfo AS (
    SELECT 
        c.c_customer_id,
        cd.cd_gender,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        SUM(ws.ws_net_profit) AS total_profit
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN 
        web_sales ws ON ws.ws_bill_customer_sk = c.c_customer_sk
    GROUP BY 
        c.c_customer_id, cd.cd_gender
)
SELECT 
    t.w_warehouse_name,
    MAX(t.total_sales) AS max_sales,
    MIN(t.total_sales) AS min_sales,
    (SELECT COUNT(DISTINCT c.c_customer_id) FROM CustomerInfo c WHERE c.order_count > 5) AS active_customers,
    (SELECT AVG(total_profit) FROM CustomerInfo) AS average_profit_per_customer
FROM 
    TotalSales t
GROUP BY 
    t.w_warehouse_name
HAVING 
    MAX(t.total_sales) > (
        SELECT AVG(total_sales) FROM TotalSales
    )
ORDER BY 
    max_sales DESC;
