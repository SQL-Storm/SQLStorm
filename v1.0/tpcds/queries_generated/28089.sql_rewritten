WITH RankedCustomers AS (
    SELECT 
        c.c_customer_id,
        CONCAT(c.c_first_name, ' ', c.c_last_name) AS full_name,
        cd.cd_gender,
        cd.cd_marital_status,
        ROW_NUMBER() OVER (PARTITION BY cd.cd_gender, cd.cd_marital_status ORDER BY c.c_birth_year DESC) AS rank
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        cd.cd_marital_status = 'M' 
        AND cd.cd_gender = 'F' 
),
SalesData AS (
    SELECT 
        ws.ws_order_number,
        ws.ws_sales_price,
        ws.ws_ship_date_sk,
        SUM(ws.ws_ext_sales_price) AS total_sales
    FROM 
        web_sales ws 
    JOIN 
        RankedCustomers rc ON ws.ws_bill_customer_sk = rc.c_customer_id
    GROUP BY 
        ws.ws_order_number, ws.ws_sales_price, ws.ws_ship_date_sk
),
FormattedSales AS (
    SELECT 
        order_number,
        total_sales,
        TO_CHAR(TO_DATE(ws_ship_date_sk, 'YYYYMMDD'), 'MM/DD/YYYY') AS formatted_ship_date
    FROM 
        SalesData
)
SELECT 
    f.order_number,
    f.total_sales,
    f.formatted_ship_date,
    rc.full_name
FROM 
    FormattedSales f
JOIN 
    RankedCustomers rc ON f.order_number = rc.c_customer_id
WHERE 
    f.total_sales > 100.00 
ORDER BY 
    f.total_sales DESC;