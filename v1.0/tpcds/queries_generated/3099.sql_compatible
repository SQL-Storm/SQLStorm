
WITH sales_data AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_sold_date_sk,
        SUM(ws.net_profit) AS total_profit,
        COUNT(DISTINCT ws.order_number) AS order_count
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023 AND 
        ws.net_profit > 0
    GROUP BY 
        ws.web_site_sk, 
        ws.ws_sold_date_sk
),
customer_data AS (
    SELECT 
        c.c_customer_sk,
        d.cd_gender,
        d.cd_marital_status,
        COALESCE(SUM(sd.total_profit), 0) AS profit_per_customer
    FROM 
        customer c
    JOIN 
        customer_demographics d ON c.c_current_cdemo_sk = d.cd_demo_sk
    LEFT JOIN 
        sales_data sd ON c.c_customer_sk = sd.web_site_sk
    GROUP BY 
        c.c_customer_sk, d.cd_gender, d.cd_marital_status
),
ranked_customers AS (
    SELECT 
        c.c_customer_sk,
        c.cd_gender,
        c.cd_marital_status,
        c.profit_per_customer,
        RANK() OVER (PARTITION BY c.cd_gender ORDER BY c.profit_per_customer DESC) AS profit_rank
    FROM 
        customer_data c
)
SELECT 
    cc.cc_name,
    COUNT(DISTINCT rc.c_customer_sk) AS high_value_customers
FROM 
    call_center cc
LEFT JOIN 
    ranked_customers rc ON rc.c_customer_sk = cc.cc_call_center_sk
WHERE 
    rc.profit_rank <= 10
GROUP BY 
    cc.cc_name
ORDER BY 
    high_value_customers DESC;
