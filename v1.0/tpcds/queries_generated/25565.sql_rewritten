WITH AddressConcat AS (
    SELECT
        ca_address_sk,
        CONCAT(ca_street_number, ' ', ca_street_name, ' ', ca_street_type, 
               COALESCE(CONCAT(' ', ca_suite_number), ''), ', ', 
               ca_city, ', ', ca_state, ', ', ca_zip) AS full_address
    FROM customer_address
),
GenderCounts AS (
    SELECT
        cd_gender,
        COUNT(*) AS count
    FROM customer_demographics
    GROUP BY cd_gender
),
ItemDescriptions AS (
    SELECT
        i_item_sk,
        REPLACE(UPPER(i_item_desc), ' ', '-') AS formatted_desc
    FROM item
    WHERE i_rec_start_date <= cast('2002-10-01' as date) 
    AND (i_rec_end_date IS NULL OR i_rec_end_date >= cast('2002-10-01' as date))
),
SalesSummary AS (
    SELECT
        ws.web_site_id,
        COUNT(*) AS total_sales,
        SUM(ws.ws_sales_price) AS total_revenue
    FROM web_sales ws
    JOIN web_site w ON ws.ws_web_site_sk = w.web_site_sk
    WHERE ws.ws_sold_date_sk BETWEEN 
         (SELECT MIN(d_date_sk) FROM date_dim WHERE d_year = 2001) AND 
         (SELECT MAX(d_date_sk) FROM date_dim WHERE d_year = 2001)
    GROUP BY ws.web_site_id
)
SELECT
    ac.full_address,
    gc.cd_gender,
    gc.count AS gender_count,
    id.formatted_desc,
    ss.total_sales,
    ss.total_revenue
FROM AddressConcat ac
JOIN GenderCounts gc ON gc.cd_gender = (SELECT cd_gender FROM customer WHERE c_current_addr_sk = ac.ca_address_sk LIMIT 1)
JOIN ItemDescriptions id ON id.i_item_sk = (SELECT ws_item_sk FROM web_sales WHERE ws_order_number = (SELECT MIN(ws_order_number) FROM web_sales) LIMIT 1)
JOIN SalesSummary ss ON ss.web_site_id = (SELECT web_site_id FROM web_site LIMIT 1)
ORDER BY ac.full_address, gc.cd_gender;