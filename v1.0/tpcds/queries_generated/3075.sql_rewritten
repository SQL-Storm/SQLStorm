WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_sales_price,
        ws.ws_quantity,
        CUME_DIST() OVER (PARTITION BY ws.web_site_sk ORDER BY ws.ws_sales_price DESC) AS price_rank,
        SUM(ws.ws_quantity) OVER (PARTITION BY ws.web_site_sk) AS total_quantity
    FROM 
        web_sales ws
    WHERE 
        ws.ws_sold_date_sk = (SELECT MAX(d_date_sk) FROM date_dim WHERE d_date = cast('2002-10-01' as date))
), 
CustomerInfo AS (
    SELECT 
        c.c_customer_sk,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_income_band_sk,
        ROW_NUMBER() OVER (PARTITION BY cd.cd_income_band_sk ORDER BY cd.cd_purchase_estimate DESC) AS rank_income
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
), 
Promotions AS (
    SELECT 
        p.p_promo_sk,
        p.p_discount_active,
        p.p_start_date_sk,
        p.p_end_date_sk
    FROM 
        promotion p
    WHERE 
        p.p_discount_active = 'Y'
        AND p.p_start_date_sk <= (SELECT MAX(d_date_sk) FROM date_dim WHERE d_date = cast('2002-10-01' as date))
        AND p.p_end_date_sk >= (SELECT MAX(d_date_sk) FROM date_dim WHERE d_date = cast('2002-10-01' as date))
)
SELECT 
    cu.c_customer_sk,
    cu.cd_gender,
    cu.cd_marital_status,
    SUM(ws.ws_net_profit) AS total_profit,
    COUNT(DISTINCT ws.ws_order_number) AS order_count,
    SUM(ws.ws_quantity) AS total_quantity,
    MAX(IFNULL(ps.p_promo_sk, 'NO PROMO')) AS used_promo
FROM 
    CustomerInfo cu
LEFT JOIN 
    web_sales ws ON cu.c_customer_sk = ws.ws_bill_customer_sk
LEFT JOIN 
    Promotions ps ON ws.ws_promo_sk = ps.p_promo_sk
WHERE 
    cu.rank_income = 1
GROUP BY 
    cu.c_customer_sk, cu.cd_gender, cu.cd_marital_status
HAVING 
    total_profit > 1000
ORDER BY 
    total_profit DESC