
WITH RankedSales AS (
    SELECT 
        w.w_warehouse_id,
        i.i_item_id,
        SUM(ws.ws_quantity) AS total_sales_quantity,
        SUM(ws.ws_sales_price) AS total_sales_revenue,
        RANK() OVER (PARTITION BY w.w_warehouse_id ORDER BY SUM(ws.ws_quantity) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        item i ON ws.ws_item_sk = i.i_item_sk
    JOIN 
        warehouse w ON ws.ws_warehouse_sk = w.w_warehouse_sk
    WHERE 
        ws.ws_sold_date_sk BETWEEN 2459580 AND 2459610  
        AND i.i_current_price > 10.00  
    GROUP BY 
        w.w_warehouse_id, i.i_item_id
),
TopSales AS (
    SELECT 
        w.w_warehouse_id AS warehouse_id,
        i.i_item_id,
        total_sales_quantity,
        total_sales_revenue
    FROM 
        RankedSales
    WHERE 
        sales_rank <= 5  
)
SELECT 
    ts.warehouse_id,
    ts.i_item_id,
    ts.total_sales_quantity,
    ts.total_sales_revenue,
    ca.ca_city,
    ca.ca_state,
    cd.cd_gender,
    cd.cd_marital_status
FROM 
    TopSales ts
JOIN 
    warehouse w ON ts.warehouse_id = w.w_warehouse_id
JOIN 
    customer c ON c.c_current_addr_sk IN (
        SELECT ca.ca_address_sk
        FROM customer_address ca
    )
JOIN 
    customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
WHERE 
    cd.cd_gender = 'F'  
ORDER BY 
    ts.total_sales_revenue DESC;
