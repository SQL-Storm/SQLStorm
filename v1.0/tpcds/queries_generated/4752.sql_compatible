
WITH RankedSales AS (
    SELECT 
        ws.web_site_sk,
        ws.ws_order_number,
        SUM(ws.ws_net_profit) AS total_net_profit,
        RANK() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_net_profit) DESC) AS rank_profit
    FROM 
        web_sales ws
    INNER JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023
    GROUP BY 
        ws.web_site_sk, ws.ws_order_number
),
TopWebsites AS (
    SELECT 
        web_site_sk, 
        total_net_profit 
    FROM 
        RankedSales 
    WHERE 
        rank_profit <= 5
),
CustomerReturns AS (
    SELECT 
        wr.refunded_customer_sk,
        SUM(wr.wr_return_amt) AS total_return_amt,
        COUNT(wr.wr_order_number) AS total_returns
    FROM 
        web_returns wr
    LEFT JOIN 
        web_sales ws ON wr.wr_order_number = ws.ws_order_number AND wr.wr_item_sk = ws.ws_item_sk
    GROUP BY 
        wr.refunded_customer_sk
),
ReturnStats AS (
    SELECT 
        cr.refunded_customer_sk,
        COALESCE(SUM(cr.total_return_amt), 0) AS total_returned,
        COALESCE(SUM(cr.total_returns), 0) AS total_return_count,
        CUST.c_gender,
        CUST.c_marital_status
    FROM 
        CustomerReturns cr
    LEFT JOIN 
        customer CUST ON cr.refunded_customer_sk = CUST.c_customer_sk
    GROUP BY 
        cr.refunded_customer_sk, CUST.c_gender, CUST.c_marital_status
)
SELECT 
    W.w_warehouse_name,
    T.total_net_profit,
    R.total_returned,
    R.total_return_count,
    (R.total_returned / NULLIF(T.total_net_profit, 0)) * 100 AS return_percentage
FROM 
    TopWebsites T
INNER JOIN 
    warehouse W ON T.web_site_sk = W.w_warehouse_sk
LEFT JOIN 
    ReturnStats R ON W.w_warehouse_sk = R.refunded_customer_sk
WHERE 
    W.w_gmt_offset IS NOT NULL
ORDER BY 
    return_percentage DESC;
