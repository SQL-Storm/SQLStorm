
WITH sales_summary AS (
    SELECT 
        ws_sold_date_sk,
        ws_ship_mode_sk,
        SUM(ws_net_profit) AS total_profit,
        COUNT(DISTINCT ws_order_number) AS order_count
    FROM 
        web_sales
    WHERE 
        ws_sold_date_sk BETWEEN (SELECT d_date_sk FROM date_dim WHERE d_date = '2023-01-01') 
                           AND (SELECT d_date_sk FROM date_dim WHERE d_date = '2023-12-31')
    GROUP BY 
        ws_sold_date_sk, ws_ship_mode_sk
),
customer_info AS (
    SELECT 
        c_customer_sk,
        cd_gender,
        cd_marital_status,
        cd_purchase_estimate
    FROM 
        customer
    INNER JOIN 
        customer_demographics ON c_current_cdemo_sk = cd_demo_sk
),
return_stats AS (
    SELECT 
        sr_item_sk,
        SUM(sr_return_quantity) AS total_returns,
        AVG(sr_return_amt) AS avg_return_amt
    FROM 
        store_returns
    GROUP BY 
        sr_item_sk
),
item_details AS (
    SELECT 
        i_item_sk,
        i_product_name,
        i_current_price,
        i_brand,
        COALESCE(ir.total_returns, 0) AS total_returns,
        COALESCE(ir.avg_return_amt, 0) AS avg_return_amt
    FROM 
        item
    LEFT JOIN 
        return_stats ir ON i_item_sk = ir.sr_item_sk
),
profit_rank AS (
    SELECT 
        s.ws_ship_mode_sk,
        ss.total_profit,
        ROW_NUMBER() OVER (PARTITION BY s.ws_ship_mode_sk ORDER BY ss.total_profit DESC) AS profit_rank
    FROM 
        sales_summary ss
    JOIN 
        ship_mode s ON ss.ws_ship_mode_sk = s.sm_ship_mode_sk
)
SELECT 
    ci.cd_gender,
    ci.cd_marital_status,
    SUM(pr.total_profit) AS total_profit,
    AVG(id.i_current_price) AS avg_price,
    SUM(id.total_returns) AS total_item_returns,
    COUNT(DISTINCT pr.ws_ship_mode_sk) AS distinct_shipping_methods
FROM 
    customer_info ci
JOIN 
    web_sales ws ON ci.c_customer_sk = ws.ws_bill_customer_sk
JOIN 
    item_details id ON ws.ws_item_sk = id.i_item_sk
JOIN 
    profit_rank pr ON ws.ws_ship_mode_sk = pr.ws_ship_mode_sk
WHERE 
    ci.cd_purchase_estimate IS NOT NULL 
GROUP BY 
    ci.cd_gender, ci.cd_marital_status
HAVING 
    SUM(id.total_returns) > 0
ORDER BY 
    total_profit DESC;
