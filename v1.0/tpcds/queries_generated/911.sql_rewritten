WITH RankedSales AS (
    SELECT 
        ws.ws_item_sk, 
        ws.ws_order_number, 
        ws.ws_quantity, 
        ws.ws_sales_price, 
        ws.ws_net_profit,
        RANK() OVER (PARTITION BY ws.ws_item_sk ORDER BY ws.ws_net_profit DESC) AS sales_rank
    FROM web_sales ws
    WHERE ws.ws_sold_date_sk >= 2456162 
),
ItemStats AS (
    SELECT 
        i.i_item_sk, 
        i.i_item_id, 
        SUM(ws.ws_quantity) AS total_quantity_sold,
        SUM(ws.ws_net_profit) AS total_net_profit
    FROM item i
    JOIN web_sales ws ON i.i_item_sk = ws.ws_item_sk
    GROUP BY i.i_item_sk, i.i_item_id
),
TopItems AS (
    SELECT 
        is.i_item_id, 
        is.total_quantity_sold, 
        is.total_net_profit, 
        ROW_NUMBER() OVER (ORDER BY is.total_net_profit DESC) AS rank
    FROM ItemStats is
    WHERE is.total_quantity_sold > 100
)
SELECT 
    ci.c_first_name || ' ' || ci.c_last_name AS customer_name,
    ci.c_email_address,
    ti.i_item_id,
    ti.total_quantity_sold,
    ti.total_net_profit,
    rs.sales_rank
FROM customer ci
LEFT JOIN RankedSales rs ON ci.c_customer_sk = rs.ws_bill_customer_sk
JOIN TopItems ti ON rs.ws_item_sk = ti.i_item_id
WHERE ti.rank <= 10
AND ci.c_birth_month BETWEEN 1 AND 6
AND (ci.c_birth_year IS NOT NULL AND ci.c_birth_year > 1980)
OR (ci.c_current_cdemo_sk IS NULL)
ORDER BY ti.total_net_profit DESC, customer_name;