
WITH sales_data AS (
    SELECT 
        ss.s_sold_date_sk,
        ss.ss_customer_sk,
        cs.cs_item_sk,
        SUM(ss.ss_quantity) AS total_quantity,
        SUM(ss.ss_net_profit) AS total_profit
    FROM 
        store_sales ss
    JOIN 
        catalog_sales cs ON ss.ss_item_sk = cs.cs_item_sk
    WHERE 
        ss.s_sold_date_sk BETWEEN 2459931 AND 2460592 
    GROUP BY 
        ss.s_sold_date_sk, ss.ss_customer_sk, cs.cs_item_sk
),
customer_demographics AS (
    SELECT 
        cd.cd_demo_sk,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_education_status
    FROM 
        customer_demographics cd
    WHERE 
        cd.cd_credit_rating = 'High' 
        AND cd.cd_dep_count > 2
),
rich_customers AS (
    SELECT DISTINCT
        c.c_customer_sk,
        c.c_first_name,
        c.c_last_name,
        cd.cd_gender,
        cd.cd_marital_status
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    WHERE 
        EXISTS (SELECT 1 FROM sales_data sd WHERE sd.ss_customer_sk = c.c_customer_sk)
)
SELECT 
    rc.c_first_name,
    rc.c_last_name,
    rc.cd_gender,
    rc.cd_marital_status,
    SUM(sd.total_quantity) AS total_quantity,
    SUM(sd.total_profit) AS total_profit
FROM 
    rich_customers rc
JOIN 
    sales_data sd ON rc.c_customer_sk = sd.ss_customer_sk
GROUP BY 
    rc.c_first_name, rc.c_last_name, rc.cd_gender, rc.cd_marital_status
ORDER BY 
    total_profit DESC
LIMIT 10;
