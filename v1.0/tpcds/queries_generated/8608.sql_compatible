
WITH SalesData AS (
    SELECT
        ws.web_site_id,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS total_orders,
        AVG(ws.ws_sales_price) AS average_sales_price,
        COUNT(DISTINCT ws.ws_bill_customer_sk) AS unique_customers
    FROM web_sales ws
    JOIN customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN date_dim d ON ws.ws_sold_date_sk = d.d_date_sk
    WHERE d.d_year = 2001
        AND cd.cd_gender = 'F'
        AND cd.cd_marital_status = 'M'
    GROUP BY ws.web_site_id
),
WarehouseData AS (
    SELECT
        w.w_warehouse_id,
        SUM(inv.inv_quantity_on_hand) AS total_inventory,
        COUNT(DISTINCT inv.inv_item_sk) AS unique_items
    FROM warehouse w
    JOIN inventory inv ON w.w_warehouse_id = inv.inv_warehouse_sk
    GROUP BY w.w_warehouse_id
),
CombinedData AS (
    SELECT
        sd.web_site_id,
        sd.total_sales,
        sd.total_orders,
        sd.average_sales_price,
        sd.unique_customers,
        wd.w_warehouse_id,
        wd.total_inventory,
        wd.unique_items
    FROM SalesData sd
    JOIN WarehouseData wd ON sd.web_site_id = 'Web01' 
)
SELECT
    web_site_id,
    total_sales,
    total_orders,
    average_sales_price,
    unique_customers,
    w_warehouse_id,
    total_inventory,
    unique_items,
    ROUND(total_sales / NULLIF(total_orders, 0), 2) AS sales_per_order,
    ROUND(total_inventory / NULLIF(unique_items, 0), 2) AS inventory_per_item
FROM CombinedData
ORDER BY total_sales DESC;
