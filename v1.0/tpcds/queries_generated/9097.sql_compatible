
WITH SalesData AS (
    SELECT 
        ws.ws_web_page_sk, 
        ws.ws_item_sk, 
        SUM(ws.ws_sales_price * ws.ws_quantity) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        DENSE_RANK() OVER (PARTITION BY ws.ws_web_page_sk ORDER BY SUM(ws.ws_sales_price * ws.ws_quantity) DESC) AS sales_rank
    FROM 
        web_sales AS ws
    JOIN 
        item AS i ON ws.ws_item_sk = i.i_item_sk
    JOIN 
        customer_address AS ca ON ws.ws_bill_addr_sk = ca.ca_address_sk
    JOIN 
        date_dim AS dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023
        AND ca.ca_state = 'CA'
        AND i.i_current_price > 50.00
    GROUP BY 
        ws.ws_web_page_sk, ws.ws_item_sk
), FilteredSales AS (
    SELECT 
        sd.ws_web_page_sk, 
        sd.ws_item_sk, 
        sd.total_sales,
        sd.order_count
    FROM 
        SalesData AS sd
    WHERE 
        sd.sales_rank <= 10
)

SELECT 
    wp.wp_url, 
    COUNT(DISTINCT fs.ws_item_sk) AS items_sold,
    SUM(fs.total_sales) AS total_revenue,
    (SELECT AVG(total_sales) FROM FilteredSales WHERE ws_web_page_sk = fs.ws_web_page_sk) AS average_sales_per_item
FROM 
    FilteredSales AS fs
JOIN 
    web_page AS wp ON fs.ws_web_page_sk = wp.wp_web_page_sk
GROUP BY 
    wp.wp_url
ORDER BY 
    total_revenue DESC;
