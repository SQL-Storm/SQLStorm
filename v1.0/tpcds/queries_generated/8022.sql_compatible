
WITH ranked_sales AS (
    SELECT
        ws.web_site_sk,
        ws.ws_bill_customer_sk,
        COUNT(ws.ws_order_number) AS total_orders,
        SUM(ws.ws_sales_price) AS total_sales,
        SUM(ws.ws_quantity) AS total_quantity,
        ROW_NUMBER() OVER (PARTITION BY ws.web_site_sk ORDER BY SUM(ws.ws_sales_price) DESC) AS rank
    FROM
        web_sales ws
    JOIN
        customer c ON ws.ws_bill_customer_sk = c.c_customer_sk
    JOIN
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
    JOIN
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    JOIN
        promotion p ON ws.ws_promo_sk = p.p_promo_sk
    WHERE
        cd.cd_gender = 'M'
        AND dd.d_year BETWEEN 2021 AND 2022
        AND p.p_discount_active = 'Y'
    GROUP BY
        ws.web_site_sk, ws.ws_bill_customer_sk
),
top_sales AS (
    SELECT 
        web_site_sk, 
        total_orders, 
        total_sales, 
        total_quantity 
    FROM 
        ranked_sales 
    WHERE 
        rank <= 10
)
SELECT 
    w.warehouse_id, 
    w.warehouse_name, 
    t.total_orders, 
    t.total_sales, 
    t.total_quantity
FROM 
    top_sales t
JOIN 
    warehouse w ON w.w_warehouse_sk = t.web_site_sk
ORDER BY 
    t.total_sales DESC;
