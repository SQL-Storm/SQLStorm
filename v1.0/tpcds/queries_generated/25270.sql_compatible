
WITH AddressDetails AS (
    SELECT 
        ca_address_id,
        ca_street_name,
        ca_city,
        ca_state,
        CONCAT(ca_street_number, ' ', ca_street_name, ' ', ca_street_type) AS full_address,
        LENGTH(ca_street_name) AS street_name_length
    FROM 
        customer_address
    WHERE 
        ca_state = 'CA'
), CustomerInfo AS (
    SELECT 
        c.c_customer_id,
        c.c_first_name,
        c.c_last_name,
        CAST(cd.cd_gender AS CHAR) AS gender,
        cd.cd_marital_status AS marital_status,
        cd.cd_education_status AS education_status
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
), AddressCustomer AS (
    SELECT 
        ac.ca_address_id,
        ac.full_address,
        ci.c_customer_id,
        ci.c_first_name,
        ci.c_last_name,
        ci.gender,
        ci.marital_status,
        ci.education_status,
        CONCAT(ci.c_first_name, ' ', ci.c_last_name) AS customer_full_name
    FROM 
        AddressDetails ac
    JOIN 
        CustomerInfo ci ON ac.ca_address_id = ci.c_customer_id
)
SELECT 
    ac.full_address,
    COUNT(*) AS customer_count,
    AVG(ac.street_name_length) AS avg_street_name_length,
    STRING_AGG(DISTINCT ac.customer_full_name ORDER BY ac.customer_full_name) AS customer_names
FROM 
    AddressCustomer ac
GROUP BY 
    ac.full_address, ac.street_name_length
HAVING 
    COUNT(*) > 5
ORDER BY 
    customer_count DESC;
