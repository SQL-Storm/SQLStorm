
WITH Ranked_Customers AS (
    SELECT 
        c.c_customer_sk,
        CONCAT(c.c_first_name, ' ', c.c_last_name) AS full_name,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_education_status,
        cd.cd_purchase_estimate,
        ROW_NUMBER() OVER (PARTITION BY cd.cd_gender ORDER BY cd.cd_purchase_estimate DESC) AS purchase_rank
    FROM 
        customer c
    JOIN 
        customer_demographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
),
Top_Customers AS (
    SELECT 
        c.c_customer_sk,
        full_name,
        cd_gender,
        cd_marital_status,
        cd_education_status,
        cd_purchase_estimate
    FROM 
        Ranked_Customers
    WHERE 
        purchase_rank <= 10
)
SELECT 
    tc.full_name,
    tc.cd_gender,
    tc.cd_marital_status,
    tc.cd_education_status,
    tc.cd_purchase_estimate,
    w.w_warehouse_name,
    w.w_city,
    w.w_state,
    CASE 
        WHEN tc.cd_purchase_estimate < 5000 THEN 'Low spender'
        WHEN tc.cd_purchase_estimate BETWEEN 5000 AND 15000 THEN 'Moderate spender'
        ELSE 'High spender'
    END AS spending_category
FROM 
    Top_Customers tc
JOIN 
    customer_address ca ON ca.ca_address_sk = (
        SELECT c.c_current_addr_sk 
        FROM customer c WHERE c.c_customer_sk = tc.c_customer_sk
    )
JOIN 
    warehouse w ON w.w_warehouse_sk = (
        SELECT MIN(w.w_warehouse_sk) 
        FROM warehouse w 
        WHERE w.w_gmt_offset = ca.ca_gmt_offset
    )
ORDER BY 
    tc.cd_gender, 
    tc.cd_purchase_estimate DESC;
