
WITH RankedSales AS (
    SELECT 
        ws.web_site_id,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        DENSE_RANK() OVER (PARTITION BY ws.web_site_id ORDER BY SUM(ws.ws_ext_sales_price) DESC) AS sales_rank
    FROM 
        web_sales ws
    JOIN 
        date_dim dd ON ws.ws_sold_date_sk = dd.d_date_sk
    WHERE 
        dd.d_year = 2023
    GROUP BY 
        ws.web_site_id
),
TopWebSites AS (
    SELECT 
        web_site_id,
        total_sales
    FROM 
        RankedSales
    WHERE 
        sales_rank <= 5
)
SELECT 
    ws.web_site_id,
    ws.web_name,
    ws.web_gmt_offset,
    ts.total_sales,
    COUNT(DISTINCT w.page_id) AS total_pages_visited,
    SUM(CASE WHEN w.wp_autogen_flag = 'Y' THEN 1 ELSE 0 END) AS auto_generated_pages
FROM 
    TopWebSites ts
JOIN 
    web_site ws ON ts.web_site_id = ws.web_site_id
JOIN 
    web_page w ON w.wp_web_page_sk = ws.web_site_sk
GROUP BY 
    ws.web_site_id, ws.web_name, ws.web_gmt_offset, ts.total_sales
ORDER BY 
    ts.total_sales DESC;
