
WITH CustomerSales AS (
    SELECT
        c.c_customer_id,
        SUM(ws.ws_ext_sales_price) AS total_sales,
        COUNT(DISTINCT ws.ws_order_number) AS order_count,
        COUNT(DISTINCT ws.ws_ship_date_sk) AS distinct_shipped_dates
    FROM
        customer c
    JOIN
        web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    WHERE
        c.c_birth_year BETWEEN 1980 AND 2000
    GROUP BY
        c.c_customer_id
),
TopCustomers AS (
    SELECT
        c.c_customer_id AS customer_id,
        cs.total_sales,
        cs.order_count,
        cs.distinct_shipped_dates,
        RANK() OVER (ORDER BY cs.total_sales DESC) AS sales_rank
    FROM
        CustomerSales cs
    JOIN
        customer c ON cs.c_customer_id = c.c_customer_id
)
SELECT
    tc.customer_id,
    tc.total_sales,
    tc.order_count,
    tc.distinct_shipped_dates,
    d.d_month_seq,
    d.d_year
FROM
    TopCustomers tc
JOIN
    date_dim d ON d.d_date_sk = (SELECT MAX(ws.ws_sold_date_sk) FROM web_sales ws WHERE ws.ws_bill_customer_sk = tc.customer_id)
WHERE
    tc.sales_rank <= 10;
