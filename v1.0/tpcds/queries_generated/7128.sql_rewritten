WITH customer_sales AS (
    SELECT
        c.c_customer_id,
        SUM(ws.ws_quantity) AS total_quantity,
        SUM(ws.ws_sales_price) AS total_sales,
        SUM(ws.ws_net_profit) AS total_profit
    FROM
        customer c
    JOIN web_sales ws ON c.c_customer_sk = ws.ws_bill_customer_sk
    WHERE
        EXISTS (
            SELECT 1
            FROM customer_demographics cd
            WHERE cd.cd_demo_sk = c.c_current_cdemo_sk
            AND cd.cd_gender = 'F'
            AND cd.cd_marital_status = 'M'
        )
    GROUP BY
        c.c_customer_id
),
recent_sales AS (
    SELECT
        cs.c_customer_id,
        cs.total_quantity,
        cs.total_sales,
        cs.total_profit
    FROM
        customer_sales cs
    JOIN date_dim d ON d.d_date_sk = (SELECT MAX(ws.ws_ship_date_sk) FROM web_sales ws WHERE ws.ws_bill_customer_sk = cs.c_customer_id)
    WHERE
        d.d_month_seq = (SELECT MAX(d2.d_month_seq) FROM date_dim d2 WHERE d2.d_year = EXTRACT(YEAR FROM cast('2002-10-01' as date)))
),
top_customers AS (
    SELECT
        r.c_customer_id,
        r.total_quantity,
        r.total_sales,
        rank() OVER (ORDER BY r.total_profit DESC) AS sales_rank
    FROM
        recent_sales r
)
SELECT
    tc.c_customer_id,
    tc.total_quantity,
    tc.total_sales,
    tc.sales_rank
FROM
    top_customers tc
WHERE
    tc.sales_rank <= 10
ORDER BY
    tc.sales_rank;