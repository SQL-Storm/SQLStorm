
WITH ranked_sales AS (
    SELECT 
        ws.bill_customer_sk,
        ws.ship_customer_sk,
        SUM(ws.net_profit) AS total_profit,
        COUNT(ws.order_number) AS total_orders,
        DENSE_RANK() OVER (PARTITION BY ws.bill_customer_sk ORDER BY SUM(ws.net_profit) DESC) AS rank
    FROM 
        web_sales ws
    GROUP BY 
        ws.bill_customer_sk, ws.ship_customer_sk
),
high_value_customers AS (
    SELECT 
        c.c_customer_id,
        c.c_first_name,
        c.c_last_name,
        r.total_profit,
        r.total_orders
    FROM 
        customer c
    JOIN 
        ranked_sales r ON c.c_customer_sk = r.bill_customer_sk
    WHERE 
        r.rank = 1
),
customer_addresses AS (
    SELECT 
        ca.*
    FROM 
        customer_address ca
    LEFT JOIN 
        customer c ON ca.ca_address_sk = c.c_current_addr_sk
    WHERE 
        c.c_customer_sk IS NOT NULL
),
totals AS (
    SELECT 
        ca.city,
        COUNT(DISTINCT c.c_customer_id) AS customer_count,
        SUM(hvc.total_profit) AS total_profit
    FROM 
        customer_addresses ca
    JOIN 
        high_value_customers hvc ON ca.ca_address_sk = hvc.bill_customer_sk
    GROUP BY 
        ca.city
)
SELECT 
    t.city,
    t.customer_count,
    t.total_profit,
    CASE 
        WHEN t.total_profit IS NULL THEN 'No Profit'
        WHEN t.total_profit >= 10000 THEN 'High Value City'
        ELSE 'Standard City'
    END AS city_category
FROM 
    totals t
WHERE 
    t.customer_count > 5
ORDER BY 
    t.total_profit DESC
FETCH FIRST 10 ROWS ONLY;
