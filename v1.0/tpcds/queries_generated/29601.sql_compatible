
WITH AddressDetails AS (
    SELECT 
        ca_addr.ca_address_id,
        ca_addr.ca_street_number || ' ' || ca_addr.ca_street_name || ' ' || ca_addr.ca_street_type || 
        COALESCE(' Suite ' || ca_addr.ca_suite_number, '') AS full_address,
        ca_addr.ca_city,
        ca_addr.ca_state,
        ca_addr.ca_zip
    FROM 
        customer_address ca_addr
),
CustomerDemographics AS (
    SELECT 
        cd.cd_demo_sk,
        cd.cd_gender,
        cd.cd_marital_status,
        cd.cd_education_status,
        cd.cd_credit_rating
    FROM 
        customer_demographics cd
),
SalesData AS (
    SELECT 
        ws.ws_order_number,
        ws.ws_item_sk,
        ws.ws_sales_price,
        ws.ws_net_profit,
        CASE 
            WHEN ws.ws_net_profit > 0 THEN 'Profitable'
            WHEN ws.ws_net_profit < 0 THEN 'Loss'
            ELSE 'Neutral'
        END AS profit_status
    FROM 
        web_sales ws
)
SELECT 
    c.c_customer_id,
    c.c_first_name || ' ' || c.c_last_name AS customer_name,
    ad.full_address,
    ad.ca_city,
    ad.ca_state,
    ad.ca_zip,
    cd.cd_gender,
    cd.cd_marital_status,
    cd.cd_education_status,
    sd.ws_order_number,
    sd.ws_item_sk,
    sd.ws_sales_price,
    sd.ws_net_profit,
    sd.profit_status
FROM 
    customer c
JOIN 
    AddressDetails ad ON c.c_current_addr_sk = ad.ca_address_id
JOIN 
    CustomerDemographics cd ON c.c_current_cdemo_sk = cd.cd_demo_sk
JOIN 
    SalesData sd ON c.c_customer_sk = sd.ws_bill_customer_sk
WHERE 
    ad.ca_state = 'CA' 
    AND cd.cd_gender = 'M' 
    AND sd.ws_net_profit > 0
ORDER BY 
    sd.ws_net_profit DESC
FETCH FIRST 100 ROWS ONLY;
