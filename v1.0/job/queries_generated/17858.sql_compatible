
SELECT 
    t.title, 
    a.name AS actor_name, 
    c.kind AS character_name 
FROM 
    aka_name a 
JOIN 
    cast_info ci ON a.person_id = ci.person_id 
JOIN 
    title t ON ci.movie_id = t.id 
JOIN 
    char_name c ON ci.role_id = c.id 
WHERE 
    t.production_year = 2023 
GROUP BY 
    t.title, a.name, c.kind 
ORDER BY 
    t.title;
