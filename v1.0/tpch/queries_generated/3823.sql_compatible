
WITH supplier_summary AS (
    SELECT 
        s.s_suppkey,
        s.s_name,
        SUM(ps.ps_availqty) AS total_available_qty,
        SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost
    FROM 
        supplier s
    JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    GROUP BY 
        s.s_suppkey, s.s_name
),
order_summary AS (
    SELECT 
        o.o_orderkey,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue,
        COUNT(l.l_orderkey) AS line_item_count,
        RANK() OVER (PARTITION BY EXTRACT(MONTH FROM o.o_orderdate) ORDER BY SUM(l.l_extendedprice * (1 - l.l_discount)) DESC) AS revenue_rank
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        o.o_orderdate >= DATE '1996-01-01' AND o.o_orderdate < DATE '1997-01-01'
    GROUP BY 
        o.o_orderkey
),
high_value_customers AS (
    SELECT 
        c.c_custkey,
        c.c_name,
        c.c_acctbal
    FROM 
        customer c
    WHERE 
        c.c_acctbal > (SELECT AVG(c2.c_acctbal) FROM customer c2)
)
SELECT 
    ns.n_name,
    COALESCE(ss.total_available_qty, 0) AS supplier_available_qty,
    COALESCE(os.total_revenue, 0) AS order_revenue,
    os.line_item_count,
    hvc.c_name AS high_value_customer
FROM 
    nation ns
LEFT JOIN 
    supplier_summary ss ON ns.n_nationkey = (SELECT s.s_nationkey FROM supplier s WHERE s.s_suppkey = ss.s_suppkey LIMIT 1)
LEFT JOIN 
    order_summary os ON ns.n_nationkey = (SELECT c.c_nationkey FROM customer c WHERE c.c_custkey = os.line_item_count LIMIT 1)
LEFT JOIN 
    high_value_customers hvc ON ns.n_nationkey = (SELECT c.c_nationkey FROM customer c WHERE c.c_custkey = hvc.c_custkey LIMIT 1)
WHERE 
    (ss.total_supply_cost IS NOT NULL OR os.total_revenue IS NOT NULL)
ORDER BY 
    ns.n_name, supplier_available_qty DESC, order_revenue DESC;
