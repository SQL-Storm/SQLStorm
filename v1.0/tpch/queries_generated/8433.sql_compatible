
WITH RankedOrders AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        o.o_totalprice,
        c.c_name,
        c.c_acctbal,
        RANK() OVER (PARTITION BY c.c_nationkey ORDER BY o.o_totalprice DESC) AS rank_order
    FROM 
        orders o
    JOIN 
        customer c ON o.o_custkey = c.c_custkey
    WHERE 
        o.o_orderdate >= DATE '1997-01-01'
        AND o.o_orderdate < DATE '1997-12-31'
),
TopOrders AS (
    SELECT 
        o_orderkey,
        o_orderdate,
        o_totalprice,
        c_name,
        c_acctbal
    FROM 
        RankedOrders
    WHERE 
        rank_order <= 5
),
SupplierParts AS (
    SELECT 
        ps.ps_partkey,
        ps.ps_suppkey,
        p.p_name,
        s.s_name,
        ps.ps_supplycost
    FROM 
        partsupp ps
    JOIN 
        supplier s ON ps.ps_suppkey = s.s_suppkey
    JOIN 
        part p ON ps.ps_partkey = p.p_partkey
),
LineItemDetails AS (
    SELECT 
        l.l_orderkey,
        l.l_linenumber,
        l.l_partkey,
        l.l_quantity,
        l.l_extendedprice,
        l.l_discount,
        l.l_tax,
        s.p_name,
        s.s_name AS supplier_name
    FROM 
        lineitem l
    JOIN 
        SupplierParts s ON l.l_partkey = s.ps_partkey
)
SELECT 
    TO.o_orderkey,
    TO.o_orderdate,
    TO.c_name AS customer_name,
    TO.o_totalprice,
    SUM(LD.l_extendedprice * (1 - LD.l_discount)) AS total_revenue,
    COUNT(DISTINCT LD.l_partkey) AS unique_parts,
    COUNT(DISTINCT LD.supplier_name) AS unique_suppliers
FROM 
    TopOrders TO
JOIN 
    LineItemDetails LD ON TO.o_orderkey = LD.l_orderkey
GROUP BY 
    TO.o_orderkey,
    TO.o_orderdate,
    TO.c_name,
    TO.o_totalprice
ORDER BY 
    total_revenue DESC
FETCH FIRST 10 ROWS ONLY;
