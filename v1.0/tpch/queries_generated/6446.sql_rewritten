WITH RankedOrders AS (
    SELECT 
        o.o_orderkey, 
        o.o_orderdate, 
        o.o_totalprice, 
        c.c_name, 
        ROW_NUMBER() OVER (PARTITION BY c.c_name ORDER BY o.o_orderdate DESC) AS rn
    FROM 
        orders o
    JOIN 
        customer c ON o.o_custkey = c.c_custkey
    WHERE 
        o.o_orderdate >= DATE '1997-01-01'
),
HighValueLineItems AS (
    SELECT 
        l.l_orderkey, 
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue
    FROM 
        lineitem l
    JOIN 
        RankedOrders ro ON l.l_orderkey = ro.o_orderkey
    GROUP BY 
        l.l_orderkey
    HAVING 
        SUM(l.l_extendedprice * (1 - l.l_discount)) > 100000
),
SupplierPartDetails AS (
    SELECT 
        p.p_partkey, 
        p.p_name, 
        s.s_name, 
        s.s_acctbal, 
        ps.ps_supplycost,
        ps.ps_availqty
    FROM 
        part p
    JOIN 
        partsupp ps ON p.p_partkey = ps.ps_partkey
    JOIN 
        supplier s ON ps.ps_suppkey = s.s_suppkey
)
SELECT 
    ro.o_orderkey, 
    ro.o_orderdate, 
    ro.o_totalprice, 
    c.c_name,
    h.total_revenue,
    sp.p_name,
    sp.s_name,
    sp.ps_supplycost,
    sp.ps_availqty
FROM 
    RankedOrders ro
LEFT JOIN 
    HighValueLineItems h ON ro.o_orderkey = h.l_orderkey
LEFT JOIN 
    SupplierPartDetails sp ON sp.p_partkey IN (
        SELECT ps.p_partkey 
        FROM partsupp ps 
        JOIN lineitem l ON ps.ps_partkey = l.l_partkey 
        WHERE l.l_orderkey = ro.o_orderkey
    )
WHERE 
    ro.rn = 1
ORDER BY 
    ro.o_orderdate DESC;