
WITH RankedOrders AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        o.o_totalprice,
        RANK() OVER (PARTITION BY o.o_orderstatus ORDER BY o.o_totalprice DESC) AS order_rank
    FROM 
        orders o
    WHERE 
        o.o_orderdate >= DATE '1997-01-01' AND 
        o.o_orderdate < DATE '1997-12-31'
),
SupplierParts AS (
    SELECT 
        ps.ps_partkey,
        ps.ps_suppkey,
        SUM(ps.ps_availqty * ps.ps_supplycost) AS total_supply_cost
    FROM 
        partsupp ps
    GROUP BY 
        ps.ps_partkey, ps.ps_suppkey
),
CustomerOrderStats AS (
    SELECT 
        c.c_nationkey, 
        COUNT(DISTINCT o.o_orderkey) AS total_orders,
        SUM(o.o_totalprice) AS total_revenue
    FROM 
        customer c
    LEFT JOIN 
        orders o ON c.c_custkey = o.o_custkey
    GROUP BY 
        c.c_nationkey
),
HighestRevenueNation AS (
    SELECT 
        n.n_name,
        n.n_nationkey,
        cos.total_revenue
    FROM 
        nation n
    JOIN 
        CustomerOrderStats cos ON n.n_nationkey = cos.c_nationkey
    WHERE 
        cos.total_revenue IS NOT NULL
    ORDER BY 
        cos.total_revenue DESC
    LIMIT 1
)
SELECT 
    rp.p_name,
    rp.total_supply_cost,
    hrn.n_name AS highest_revenue_nation,
    r_order.o_orderkey,
    r_order.o_orderdate,
    r_order.o_totalprice
FROM 
    RankedOrders r_order
JOIN 
    SupplierParts rp ON rp.ps_partkey IN (SELECT l.l_partkey FROM lineitem l WHERE l.l_orderkey = r_order.o_orderkey)
JOIN 
    HighestRevenueNation hrn ON hrn.n_nationkey = (SELECT s.s_nationkey FROM supplier s WHERE s.s_suppkey = rp.ps_suppkey LIMIT 1)
WHERE 
    r_order.order_rank <= 10
ORDER BY 
    r_order.o_totalprice DESC, rp.total_supply_cost ASC;
