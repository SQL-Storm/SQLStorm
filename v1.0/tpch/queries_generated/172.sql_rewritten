WITH RankedSuppliers AS (
    SELECT 
        s.s_suppkey, 
        s.s_name, 
        s.s_acctbal,
        RANK() OVER (PARTITION BY p.p_partkey ORDER BY s.s_acctbal DESC) AS supplier_rank
    FROM 
        supplier s
    JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    JOIN 
        part p ON ps.ps_partkey = p.p_partkey
),
TotalRevenue AS (
    SELECT 
        o.o_orderkey, 
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        l.l_shipdate >= DATE '1996-01-01' 
        AND l.l_shipdate < DATE '1997-01-01'
    GROUP BY 
        o.o_orderkey
),
HighValueOrders AS (
    SELECT 
        o.o_orderkey,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS order_value
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    GROUP BY 
        o.o_orderkey
    HAVING 
        SUM(l.l_extendedprice * (1 - l.l_discount)) > 1000
),
SupplierAnalysis AS (
    SELECT 
        s.s_suppkey,
        s.s_name,
        COALESCE(SUM(ps.ps_availqty), 0) AS total_available_qty,
        COUNT(DISTINCT ps.ps_partkey) AS part_count
    FROM 
        supplier s
    LEFT JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    GROUP BY 
        s.s_suppkey, 
        s.s_name
)
SELECT 
    r.r_name AS region_name,
    s.s_name AS supplier_name,
    s.total_available_qty,
    o.total_revenue,
    h.order_value,
    COALESCE(s.part_count, 0) AS supplier_part_count,
    CASE 
        WHEN supplier_rank = 1 THEN 'Top Supplier'
        ELSE 'Other Supplier' 
    END AS supplier_category
FROM 
    RankedSuppliers rs
LEFT JOIN 
    SupplierAnalysis s ON rs.s_suppkey = s.s_suppkey
LEFT JOIN 
    TotalRevenue o ON o.o_orderkey IN (
        SELECT o_orderkey 
        FROM HighValueOrders
    )
LEFT JOIN 
    nation n ON s.s_suppkey = n.n_nationkey
LEFT JOIN 
    region r ON n.n_regionkey = r.r_regionkey
WHERE 
    s.total_available_qty IS NOT NULL
ORDER BY 
    region_name, supplier_name;