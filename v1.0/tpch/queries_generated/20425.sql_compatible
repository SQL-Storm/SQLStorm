
WITH ranked_parts AS (
    SELECT 
        p.p_partkey,
        p.p_name,
        p.p_retailprice,
        ROW_NUMBER() OVER (PARTITION BY p.p_mfgr ORDER BY p.p_retailprice DESC) AS rank_price
    FROM 
        part p
    WHERE 
        p.p_retailprice IS NOT NULL
    AND 
        p.p_size IN (SELECT DISTINCT p_size FROM part WHERE p_size > 0)
),
supplier_avg_cost AS (
    SELECT 
        ps.ps_suppkey,
        AVG(ps.ps_supplycost) AS avg_cost
    FROM 
        partsupp ps
    GROUP BY 
        ps.ps_suppkey
    HAVING 
        COUNT(ps.ps_supplycost) > 2
),
customer_high_balance AS (
    SELECT 
        c.c_custkey,
        c.c_name,
        c.c_acctbal,
        CASE 
            WHEN c.c_acctbal IS NULL THEN 'Unknown'
            WHEN c.c_acctbal > 10000 THEN 'High'
            ELSE 'Standard' 
        END AS account_category
    FROM 
        customer c
    WHERE 
        c.c_acctbal IS NOT NULL
)
SELECT 
    r.*,
    COALESCE(sp.p_name, 'No Part Available') AS part_name,
    COALESCE(sp.p_retailprice, 0) AS retail_price,
    COALESCE(s.avg_cost, 0) AS supplier_avg_cost,
    c.account_category
FROM 
    ranked_parts sp
FULL OUTER JOIN supplier_avg_cost s ON s.ps_suppkey = (
        SELECT ps.ps_suppkey 
        FROM partsupp ps 
        WHERE ps.ps_partkey = sp.p_partkey 
        LIMIT 1
    )
LEFT JOIN customer_high_balance c ON c.c_custkey = (
        SELECT o.o_custkey 
        FROM orders o 
        WHERE o.o_orderkey = (
                SELECT l.l_orderkey 
                FROM lineitem l 
                WHERE l.l_partkey = sp.p_partkey 
                LIMIT 1
            ) 
        LIMIT 1
    )
JOIN region r ON r.r_regionkey = (
        SELECT n.n_regionkey 
        FROM nation n 
        WHERE n.n_nationkey = (
                SELECT s.s_nationkey 
                FROM supplier s 
                WHERE s.s_suppkey = s.ps_suppkey 
                LIMIT 1
            ) 
        LIMIT 1
    )
WHERE 
    sp.rank_price <= 5
AND 
    r.r_name LIKE '%East%'
ORDER BY 
    r.r_name, retail_price DESC
FETCH FIRST 100 ROWS ONLY;
