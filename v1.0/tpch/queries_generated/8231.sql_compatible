
WITH NationSuppliers AS (
    SELECT n.n_name, COUNT(DISTINCT s.s_suppkey) AS supplier_count
    FROM nation n
    JOIN supplier s ON n.n_nationkey = s.s_nationkey
    GROUP BY n.n_nationkey, n.n_name
),
PopularParts AS (
    SELECT ps.ps_partkey, SUM(ps.ps_availqty) AS total_avail_qty
    FROM partsupp ps
    JOIN part p ON ps.ps_partkey = p.p_partkey
    GROUP BY ps.ps_partkey
    HAVING SUM(ps.ps_availqty) > 100
),
OrderAnalysis AS (
    SELECT o.o_orderkey, COUNT(l.l_orderkey) AS lineitem_count, SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue
    FROM orders o
    JOIN lineitem l ON o.o_orderkey = l.l_orderkey
    GROUP BY o.o_orderkey
    HAVING SUM(l.l_extendedprice * (1 - l.l_discount)) > 10000
),
FinalReport AS (
    SELECT ns.n_name, pp.ps_partkey, pp.total_avail_qty, oa.lineitem_count, oa.total_revenue
    FROM NationSuppliers ns
    JOIN PopularParts pp ON ns.supplier_count > 5
    JOIN OrderAnalysis oa ON oa.lineitem_count > 5
)
SELECT n_name, ps_partkey, total_avail_qty, lineitem_count, total_revenue
FROM FinalReport
ORDER BY total_revenue DESC, total_avail_qty DESC
LIMIT 10;
