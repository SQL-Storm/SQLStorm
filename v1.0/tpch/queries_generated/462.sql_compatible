
WITH OrderSummary AS (
    SELECT 
        o.o_orderkey,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue,
        COUNT(DISTINCT l.l_partkey) AS unique_parts_count
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        o.o_orderdate >= DATE '1997-01-01' AND o.o_orderdate < DATE '1997-12-31'
    GROUP BY 
        o.o_orderkey
), SupplierStats AS (
    SELECT 
        s.s_suppkey,
        COUNT(DISTINCT ps.ps_partkey) AS part_count,
        SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost 
    FROM 
        supplier s
    LEFT JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    GROUP BY 
        s.s_suppkey
), RevenueRanking AS (
    SELECT 
        o.o_orderkey,
        os.total_revenue,
        DENSE_RANK() OVER (ORDER BY os.total_revenue DESC) AS revenue_rank
    FROM 
        OrderSummary os
)
SELECT 
    r.r_name AS region_name,
    n.n_name AS nation_name,
    s.s_name AS supplier_name,
    ss.part_count,
    ss.total_supply_cost,
    rr.total_revenue,
    rr.revenue_rank
FROM 
    SupplierStats ss
JOIN 
    supplier s ON ss.s_suppkey = s.s_suppkey
JOIN 
    nation n ON s.s_nationkey = n.n_nationkey
JOIN 
    region r ON n.n_regionkey = r.r_regionkey
LEFT JOIN 
    RevenueRanking rr ON rr.o_orderkey = (SELECT MIN(o_orderkey) 
                                            FROM RevenueRanking 
                                            WHERE revenue_rank <= 5)
WHERE 
    ss.total_supply_cost > (SELECT AVG(total_supply_cost) 
                             FROM SupplierStats)
ORDER BY 
    rr.total_revenue DESC;
