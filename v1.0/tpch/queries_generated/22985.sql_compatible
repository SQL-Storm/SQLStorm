
WITH RankedParts AS (
    SELECT 
        p.p_partkey, 
        p.p_name, 
        p.p_retailprice, 
        RANK() OVER (PARTITION BY p.p_size ORDER BY p.p_retailprice DESC) AS price_rank
    FROM 
        part p
    WHERE
        p.p_retailprice IS NOT NULL
),
SupplierStats AS (
    SELECT 
        s.s_suppkey,
        COUNT(DISTINCT ps.ps_partkey) AS part_count,
        SUM(ps.ps_supplycost) AS total_supply_cost,
        AVG(ps.ps_availqty) AS avg_avail_qty
    FROM 
        supplier s
    JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    GROUP BY 
        s.s_suppkey
),
TopSuppliers AS (
    SELECT 
        s.s_name, 
        ss.part_count,
        ss.total_supply_cost,
        ss.avg_avail_qty,
        RANK() OVER (ORDER BY ss.total_supply_cost DESC) AS supplier_rank
    FROM 
        SupplierStats ss
    JOIN 
        supplier s ON ss.s_suppkey = s.s_suppkey
    WHERE 
        ss.part_count > 10
        AND ss.avg_avail_qty IS NOT NULL
),
OrderSummary AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_value,
        COUNT(DISTINCT l.l_partkey) AS total_items
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        o.o_orderdate BETWEEN '1996-01-01' AND '1996-12-31'
    GROUP BY 
        o.o_orderkey, o.o_orderdate
)
SELECT 
    p.p_partkey,
    p.p_name,
    ps.s_name,
    os.total_value,
    os.total_items,
    CASE 
        WHEN os.total_value > 100000 THEN 'High Value'
        WHEN os.total_value BETWEEN 50000 AND 100000 THEN 'Medium Value'
        ELSE 'Low Value' 
    END AS order_value_category,
    COALESCE(ts.supplier_rank, 'N/A') AS supplier_rank
FROM 
    RankedParts p
LEFT JOIN 
    TopSuppliers ts ON p.price_rank = 1
LEFT JOIN 
    partsupp psu ON p.p_partkey = psu.ps_partkey
LEFT JOIN 
    supplier ps ON psu.ps_suppkey = ps.s_suppkey
JOIN 
    OrderSummary os ON os.total_items = psu.ps_partkey
WHERE 
    p.price_rank <= 5
    AND (p.p_name LIKE '%widget%' OR p.p_comment IS NULL)
ORDER BY 
    order_value_category DESC, p.p_name;
