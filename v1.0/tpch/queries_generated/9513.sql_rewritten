WITH ExtendedOrderInfo AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        o.o_totalprice,
        c.c_nationkey,
        c.c_mktsegment,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue,
        COUNT(DISTINCT l.l_partkey) AS unique_parts_sold,
        SUM(l.l_quantity) AS total_quantity_sold
    FROM 
        orders o
    JOIN 
        customer c ON o.o_custkey = c.c_custkey
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        o.o_orderdate >= DATE '1995-01-01' AND o.o_orderdate < DATE '1995-12-31'
    GROUP BY 
        o.o_orderkey, o.o_orderdate, o.o_totalprice, c.c_nationkey, c.c_mktsegment
),
PartSupplierInfo AS (
    SELECT 
        p.p_partkey,
        p.p_name,
        ps.ps_supplycost,
        s.s_name
    FROM 
        partsupp ps
    JOIN 
        part p ON ps.ps_partkey = p.p_partkey
    JOIN 
        supplier s ON ps.ps_suppkey = s.s_suppkey
    WHERE 
        ps.ps_availqty > 0
)
SELECT 
    e.o_orderkey,
    e.o_orderdate,
    e.total_revenue,
    e.unique_parts_sold,
    e.total_quantity_sold,
    p.p_name,
    p.ps_supplycost,
    p.s_name
FROM 
    ExtendedOrderInfo e
JOIN 
    PartSupplierInfo p ON e.o_orderkey = (SELECT MAX(o.o_orderkey) FROM orders o WHERE o.o_totalprice = e.o_totalprice)
ORDER BY 
    e.o_orderdate DESC, e.total_revenue DESC
LIMIT 50;