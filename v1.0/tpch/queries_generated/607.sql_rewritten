WITH SupplierCost AS (
    SELECT 
        s.s_suppkey,
        SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost
    FROM 
        supplier s
    JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    GROUP BY 
        s.s_suppkey
),
HighValueCustomers AS (
    SELECT 
        c.c_custkey,
        c.c_name,
        c.c_acctbal,
        ROW_NUMBER() OVER (ORDER BY c.c_acctbal DESC) AS rn
    FROM 
        customer c
    WHERE 
        c.c_acctbal > 10000
),
OrderDetails AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        COUNT(l.l_orderkey) AS lineitem_count,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_order_value,
        SUM(l.l_quantity) AS total_quantity
    FROM 
        orders o
    LEFT JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    GROUP BY 
        o.o_orderkey, o.o_orderdate
),
NationCounts AS (
    SELECT 
        n.n_name,
        COUNT(DISTINCT s.s_suppkey) AS supplier_count
    FROM 
        nation n
    LEFT JOIN 
        supplier s ON n.n_nationkey = s.s_nationkey
    GROUP BY 
        n.n_name
)
SELECT 
    n.n_name,
    nc.supplier_count,
    hc.c_name AS high_value_customer,
    hc.c_acctbal,
    od.o_orderkey,
    od.total_order_value,
    od.total_quantity,
    sc.total_supply_cost
FROM 
    NationCounts nc
LEFT JOIN 
    HighValueCustomers hc ON hc.rn <= 5
LEFT JOIN 
    OrderDetails od ON od.o_orderkey IN (SELECT o.o_orderkey FROM orders o WHERE o.o_orderdate BETWEEN '1997-01-01' AND '1997-12-31')
LEFT JOIN 
    SupplierCost sc ON sc.s_suppkey IN (SELECT ps.ps_suppkey FROM partsupp ps WHERE ps.ps_partkey IN (SELECT p.p_partkey FROM part p WHERE p.p_retailprice > 50.00))
LEFT JOIN 
    nation n ON n.n_nationkey = (SELECT s.n_nationkey FROM supplier s WHERE s.s_suppkey = sc.s_suppkey)
WHERE 
    hc.c_custkey IS NOT NULL OR sc.total_supply_cost IS NOT NULL
ORDER BY 
    nc.supplier_count DESC, od.total_order_value DESC;