WITH RankedSuppliers AS (
    SELECT 
        s.s_suppkey,
        s.s_name,
        s.s_acctbal,
        ROW_NUMBER() OVER (PARTITION BY s.n_nationkey ORDER BY s.s_acctbal DESC) as supplier_rank
    FROM 
        supplier s
    JOIN 
        nation n ON s.s_nationkey = n.n_nationkey
),
HighValueOrders AS (
    SELECT 
        o.o_orderkey,
        o.o_totalprice,
        o.o_orderdate,
        o.o_shippriority
    FROM 
        orders o
    WHERE 
        o.o_totalprice > (
            SELECT 
                AVG(l.l_extendedprice) 
            FROM 
                lineitem l 
            WHERE 
                l.l_shipdate >= DATE '1996-01-01'
        )
)
SELECT 
    p.p_name,
    p.p_retailprice,
    COUNT(DISTINCT o.o_orderkey) AS order_count,
    SUM(l.l_extendedprice) AS total_revenue,
    MAX(s.s_acctbal) AS highest_supplier_acctbal
FROM 
    part p
LEFT JOIN 
    lineitem l ON p.p_partkey = l.l_partkey
LEFT JOIN 
    HighValueOrders o ON l.l_orderkey = o.o_orderkey
LEFT JOIN 
    RankedSuppliers s ON l.l_suppkey = s.s_suppkey
WHERE 
    p.p_size > 10 
    AND (s.s_acctbal IS NULL OR s.s_acctbal > 500)
    AND o.o_orderdate >= DATE '1997-01-01'
GROUP BY 
    p.p_name, 
    p.p_retailprice
HAVING 
    total_revenue > 10000
ORDER BY 
    total_revenue DESC;