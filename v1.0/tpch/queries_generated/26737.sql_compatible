
SELECT 
    CONCAT('Part: ', p.p_name, ' | Supplier: ', s.s_name, 
           ' | Quantity: ', CAST(ps.ps_availqty AS VARCHAR(255)), 
           ' | Price: $', CAST(ps.ps_supplycost AS VARCHAR(255)), 
           ' | Order Date: ', TO_CHAR(o.o_orderdate, 'YYYY-MM-DD'), 
           ' | Customer: ', c.c_name) AS bench_mark_result
FROM 
    part p
JOIN 
    partsupp ps ON p.p_partkey = ps.ps_partkey
JOIN 
    supplier s ON ps.ps_suppkey = s.s_suppkey
JOIN 
    lineitem l ON p.p_partkey = l.l_partkey
JOIN 
    orders o ON l.l_orderkey = o.o_orderkey
JOIN 
    customer c ON o.o_custkey = c.c_custkey
WHERE 
    p.p_size BETWEEN 10 AND 20
    AND s.s_acctbal > 1000
    AND o.o_orderstatus = 'O'
    AND l.l_shipdate >= DATE '1997-01-01'
GROUP BY 
    p.p_name, s.s_name, ps.ps_availqty, ps.ps_supplycost, 
    o.o_orderdate, c.c_name
ORDER BY 
    c.c_name DESC
LIMIT 100;
