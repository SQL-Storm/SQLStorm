WITH RankedOrders AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        o.o_totalprice,
        o.o_custkey,
        ROW_NUMBER() OVER (PARTITION BY o.o_orderstatus ORDER BY o.o_totalprice DESC) AS OrderRank
    FROM 
        orders o
    WHERE 
        o.o_orderdate >= '1997-01-01'
    AND 
        o.o_totalprice > 1000
),
SupplierDetails AS (
    SELECT 
        s.s_suppkey,
        s.s_name,
        COALESCE(SUM(ps.ps_availqty), 0) AS TotalAvailableQty,
        COUNT(DISTINCT ps.ps_partkey) AS UniqueParts,
        STRING_AGG(DISTINCT p.p_name, ', ') AS PartNames
    FROM 
        supplier s
    LEFT JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    LEFT JOIN 
        part p ON ps.ps_partkey = p.p_partkey
    GROUP BY 
        s.s_suppkey, s.s_name
),
HighValueCustomers AS (
    SELECT 
        c.c_custkey,
        c.c_name,
        SUM(o.o_totalprice) AS TotalSpent
    FROM 
        customer c
    JOIN 
        orders o ON c.c_custkey = o.o_custkey
    WHERE 
        c.c_acctbal > 5000
    GROUP BY 
        c.c_custkey, c.c_name
    HAVING 
        SUM(o.o_totalprice) > 20000
),
FilteredLineItems AS (
    SELECT 
        l.l_orderkey,
        l.l_partkey,
        l.l_quantity,
        l.l_discount,
        l.l_extendedprice * (1 - l.l_discount) AS NetPrice
    FROM 
        lineitem l
    WHERE 
        l.l_discount BETWEEN 0.1 AND 0.5
    AND 
        l.l_returnflag = 'N'
),
MaxOrderDate AS (
    SELECT 
        MAX(o.o_orderdate) AS LatestOrderDate
    FROM 
        orders o
)
SELECT 
    r.r_name AS RegionName,
    COUNT(DISTINCT n.n_nationkey) AS NationCount,
    AVG(sd.TotalAvailableQty) AS AvgAvailableQty,
    SUM(f.NetPrice) AS TotalNetSales,
    COUNT(ho.c_custkey) AS HighValueCustCount
FROM 
    region r
JOIN 
    nation n ON r.r_regionkey = n.n_regionkey
LEFT JOIN 
    SupplierDetails sd ON EXISTS (
        SELECT 1 FROM partsupp ps 
        WHERE ps.ps_availqty > 50
        AND ps.ps_partkey IN (SELECT DISTINCT l.l_partkey FROM FilteredLineItems l WHERE l.l_orderkey IN (SELECT o.o_orderkey FROM RankedOrders o))
    )
LEFT JOIN 
    HighValueCustomers ho ON n.n_nationkey = ho.c_nationkey
LEFT JOIN 
    FilteredLineItems f ON f.l_orderkey IN (SELECT o.o_orderkey FROM RankedOrders o WHERE o.OrderRank <= 5)
WHERE 
    r.r_name IS NOT NULL
AND 
    (SELECT LatestOrderDate FROM MaxOrderDate) > '1997-06-01'
GROUP BY 
    r.r_name
HAVING 
    COUNT(DISTINCT ho.c_custkey) > 0
ORDER BY 
    RegionName;