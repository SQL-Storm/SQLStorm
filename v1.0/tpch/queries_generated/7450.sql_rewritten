WITH RankedOrders AS (
    SELECT 
        o.o_orderkey,
        o.o_orderdate,
        o.o_totalprice,
        o.o_orderpriority,
        c.c_name,
        c.c_nationkey,
        ROW_NUMBER() OVER (PARTITION BY o.o_orderpriority ORDER BY o.o_totalprice DESC) AS order_rank
    FROM 
        orders o
    JOIN 
        customer c ON o.o_custkey = c.c_custkey
    WHERE 
        o.o_orderstatus = 'O'
),
TopSuppliers AS (
    SELECT 
        ps.ps_partkey,
        ps.ps_suppkey,
        SUM(ps.ps_supplycost * ps.ps_availqty) AS total_supply_cost
    FROM 
        partsupp ps
    GROUP BY 
        ps.ps_partkey, ps.ps_suppkey
),
AggregatedLineItems AS (
    SELECT 
        l.l_orderkey,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_revenue,
        COUNT(l.l_orderkey) AS line_item_count
    FROM 
        lineitem l
    WHERE 
        l.l_shipdate >= '1997-01-01' AND l.l_shipdate < '1998-01-01'
    GROUP BY 
        l.l_orderkey
)
SELECT 
    ro.o_orderkey,
    ro.o_orderdate,
    ro.total_price,
    ro.o_orderpriority,
    ro.c_name,
    ts.total_supply_cost,
    ali.total_revenue,
    ali.line_item_count
FROM 
    RankedOrders ro
LEFT JOIN 
    TopSuppliers ts ON ts.ps_partkey IN (SELECT ps_partkey FROM partsupp)
LEFT JOIN 
    AggregatedLineItems ali ON ali.l_orderkey = ro.o_orderkey
WHERE 
    ro.order_rank <= 10
ORDER BY 
    ro.o_orderdate DESC, 
    ro.o_totalprice DESC;