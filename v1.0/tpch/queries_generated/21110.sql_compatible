
WITH RankedSuppliers AS (
    SELECT 
        s.s_suppkey,
        s.s_name,
        s.s_acctbal,
        RANK() OVER (PARTITION BY p.p_brand ORDER BY s.s_acctbal DESC) AS rank
    FROM 
        supplier s
    JOIN 
        partsupp ps ON s.s_suppkey = ps.ps_suppkey
    JOIN 
        part p ON ps.ps_partkey = p.p_partkey
    WHERE
        s.s_acctbal IS NOT NULL AND s.s_comment NOT LIKE '%obsolete%'
),
HighValueOrders AS (
    SELECT 
        o.o_orderkey,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS total_value
    FROM
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        o.o_orderstatus = 'O' AND 
        l.l_discount BETWEEN 0.05 AND 0.15
    GROUP BY 
        o.o_orderkey
    HAVING 
        SUM(l.l_extendedprice * (1 - l.l_discount)) > 10000
),
SupplierJoin AS (
    SELECT 
        r.n_nationkey,
        COUNT(DISTINCT s.s_suppkey) AS supplier_count,
        SUM(s.s_acctbal) AS total_acct_balance
    FROM 
        nation r
    LEFT JOIN 
        supplier s ON r.n_nationkey = s.s_nationkey
    GROUP BY 
        r.n_nationkey
),
OrderCounts AS (
    SELECT 
        c.c_custkey,
        COUNT(o.o_orderkey) AS order_count
    FROM 
        customer c
    LEFT JOIN 
        orders o ON c.c_custkey = o.o_custkey
    GROUP BY 
        c.c_custkey
    HAVING 
        COUNT(o.o_orderkey) > 5
)

SELECT 
    r.r_name,
    COALESCE(su.supplier_count, 0) AS supplier_count,
    COALESCE(su.total_acct_balance, 0) AS total_acct_balance,
    hc.total_value AS high_value_order,
    COALESCE(oc.order_count, 0) AS customer_order_count
FROM 
    region r
LEFT JOIN 
    SupplierJoin su ON su.n_nationkey = r.r_regionkey
LEFT JOIN 
    HighValueOrders hc ON 
        hc.o_orderkey IN (SELECT o.o_orderkey FROM orders o) 
LEFT JOIN 
    OrderCounts oc ON 
        oc.c_custkey IN (SELECT c.c_custkey FROM customer c)
WHERE 
    r.r_name NOT LIKE '%Nation%' AND 
    (su.total_acct_balance > 50000 OR r.r_comment IS NULL)
ORDER BY 
    r.r_name ASC, supplier_count DESC, total_acct_balance DESC;
