
WITH RankedParts AS (
    SELECT 
        p.p_partkey,
        p.p_name,
        p.p_retailprice,
        ROW_NUMBER() OVER (PARTITION BY p.p_brand ORDER BY p.p_retailprice DESC) AS rn
    FROM 
        part p
    WHERE 
        p.p_retailprice IS NOT NULL
),
NationSummary AS (
    SELECT 
        n.n_nationkey,
        n.n_name,
        COUNT(DISTINCT s.s_suppkey) AS supplier_count,
        SUM(s.s_acctbal) AS total_acctbal
    FROM 
        nation n
    LEFT JOIN 
        supplier s ON n.n_nationkey = s.s_nationkey
    GROUP BY 
        n.n_nationkey, n.n_name
),
LargeOrders AS (
    SELECT 
        o.o_orderkey,
        o.o_totalprice,
        o.o_orderdate,
        o.o_orderstatus,
        o.o_shippriority,
        SUM(l.l_quantity * (1 - l.l_discount)) AS net_value
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    WHERE 
        o.o_orderstatus = 'F' AND 
        l.l_shipdate BETWEEN '1995-01-01' AND '1995-12-31'
    GROUP BY 
        o.o_orderkey, o.o_totalprice, o.o_orderdate, o.o_orderstatus, o.o_shippriority
    HAVING 
        SUM(l.l_quantity * (1 - l.l_discount)) > 1000
),
NullHandling AS (
    SELECT 
        ns.n_nationkey,
        ns.n_name,
        COALESCE(ns.supplier_count, 0) AS supplier_count,
        COALESCE(ns.total_acctbal, 0) AS total_acctbal,
        COUNT(DISTINCT lo.o_orderkey) AS order_count,
        AVG(lo.net_value) AS avg_net_value
    FROM 
        NationSummary ns
    LEFT JOIN 
        LargeOrders lo ON ns.n_nationkey = (SELECT c.c_nationkey FROM customer c WHERE c.c_custkey = (SELECT MIN(c2.c_custkey) FROM customer c2 WHERE c2.c_acctbal IS NOT NULL))
    GROUP BY 
        ns.n_nationkey, ns.n_name
)
SELECT 
    rp.p_partkey,
    rp.p_name,
    rp.p_retailprice,
    nh.n_name,
    nh.supplier_count,
    nh.total_acctbal,
    nh.order_count,
    nh.avg_net_value
FROM 
    RankedParts rp
JOIN 
    NullHandling nh ON rp.rn = 1 AND EXISTS (
        SELECT 1 
        FROM partsupp ps 
        WHERE ps.ps_partkey = rp.p_partkey AND ps.ps_availqty = (SELECT MAX(ps2.ps_availqty) FROM partsupp ps2 WHERE ps2.ps_partkey = rp.p_partkey)
    )
ORDER BY 
    rp.p_retailprice DESC, nh.total_acctbal DESC
LIMIT 100 OFFSET 0;
