
WITH total_sales AS (
    SELECT 
        l_partkey,
        SUM(l_extendedprice * (1 - l_discount)) AS revenue
    FROM 
        lineitem
    WHERE 
        l_shipdate >= DATE '1995-01-01' AND l_shipdate < DATE '1996-01-01'
    GROUP BY 
        l_partkey
),
ranked_sales AS (
    SELECT 
        ps.partkey,
        ps.suppkey,
        ts.revenue,
        RANK() OVER (ORDER BY ts.revenue DESC) AS revenue_rank
    FROM 
        partsupp ps
    JOIN 
        total_sales ts ON ps.ps_partkey = ts.l_partkey
)
SELECT 
    n.n_name,
    r.r_name,
    SUM(rs.revenue) AS total_revenue,
    COUNT(DISTINCT rs.suppkey) AS unique_suppliers
FROM 
    ranked_sales rs
JOIN 
    supplier s ON rs.suppkey = s.s_suppkey
JOIN 
    nation n ON s.s_nationkey = n.n_nationkey
JOIN 
    region r ON n.n_regionkey = r.r_regionkey
WHERE 
    rs.revenue_rank <= 10
GROUP BY 
    n.n_name, r.r_name
ORDER BY 
    total_revenue DESC;
