
WITH RankedSuppliers AS (
    SELECT 
        s.s_suppkey,
        s.s_name,
        s.s_acctbal,
        ROW_NUMBER() OVER (PARTITION BY n.n_name ORDER BY s.s_acctbal DESC) AS supplier_rank
    FROM 
        supplier s
    JOIN 
        nation n ON s.s_nationkey = n.n_nationkey
),
HighValueOrders AS (
    SELECT 
        o.o_orderkey,
        SUM(l.l_extendedprice * (1 - l.l_discount)) AS net_value
    FROM 
        orders o
    JOIN 
        lineitem l ON o.o_orderkey = l.l_orderkey
    GROUP BY 
        o.o_orderkey
    HAVING 
        SUM(l.l_extendedprice * (1 - l.l_discount)) > 1000
),
TopRegions AS (
    SELECT 
        r.r_name,
        AVG(s.s_acctbal) AS avg_acctbal
    FROM 
        region r
    JOIN 
        nation n ON r.r_regionkey = n.n_regionkey
    JOIN 
        supplier s ON n.n_nationkey = s.s_nationkey
    GROUP BY 
        r.r_name
    HAVING 
        AVG(s.s_acctbal) > 5000
)
SELECT 
    r.r_name,
    COUNT(DISTINCT o.o_orderkey) AS HighValueOrderCount,
    SUM(COALESCE(l.l_discount, 0) / NULLIF(l.l_extendedprice, 0)) AS DiscountToPriceRatio,
    COUNT(DISTINCT ps.ps_suppkey) AS SupplierCount,
    MAX(s.s_acctbal) AS HighestSupplierBalance
FROM 
    region r
LEFT JOIN 
    nation n ON r.r_regionkey = n.n_regionkey
LEFT JOIN 
    supplier s ON n.n_nationkey = s.s_nationkey
LEFT JOIN 
    partsupp ps ON ps.ps_suppkey = s.s_suppkey
LEFT JOIN 
    HighValueOrders o ON o.o_orderkey IN (SELECT l.l_orderkey FROM lineitem l WHERE l.l_partkey IN (SELECT p.p_partkey FROM part p WHERE p.p_size > 20))
LEFT JOIN 
    lineitem l ON o.o_orderkey = l.l_orderkey
WHERE 
    s.s_acctbal IS NOT NULL
GROUP BY 
    r.r_name
HAVING 
    COUNT(DISTINCT o.o_orderkey) > (SELECT AVG(HighValueOrderCount) FROM (SELECT r.r_name, COUNT(DISTINCT o.o_orderkey) AS HighValueOrderCount FROM region r LEFT JOIN HighValueOrders o ON r.r_regionkey = o.o_orderkey GROUP BY r.r_name) AS subquery);
